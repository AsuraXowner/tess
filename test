local AutoComplate = true
local selectedSuggestionIndex = 1
local suggestions = {}
local suggestionButtons = {}

local function updateSuggestionHighlights()
	for i, btn in ipairs(suggestionButtons) do
		if i == selectedSuggestionIndex then
			btn.BackgroundColor3 = Color3.fromRGB(0, 157, 255)
			btn.BackgroundTransparency = 0.3
		else
			btn.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			btn.BackgroundTransparency = 1
		end
	end
end

local function updateSuggestions()
	local updateInProgress = false
	if updateInProgress then return end
	updateInProgress = true

	local input = navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox0.Text:lower()

	if navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox0.Visible then
		input = navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox0.Text:lower()
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox1.Visible then
		input = navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox1.Text:lower()
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox2.Visible then
		input = navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox2.Text:lower()
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox3.Visible then
		input = navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox3.Text:lower()
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox4.Visible then
		input = navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox4.Text:lower()
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox5.Visible then
		input = navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox5.Text:lower()
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox6.Visible then
		input = navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox6.Text:lower()
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox7.Visible then
		input = navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox7.Text:lower()
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox8.Visible then
		input = navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox8.Text:lower()
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox9.Visible then
		input = navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox9.Text:lower()
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox10.Visible then
		input = navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox10.Text:lower()
	end

	if input:match("local%s$") or input:match("\n$") then
		coroutine.wrap(function()
			tween_service:Create(AsuraX_Instances.SuggesstionFrame, quick, {BackgroundTransparency = 1}):Play()
			tween_service:Create(AsuraX_Instances.SuggesstionsTextHighlight, quick, {TextTransparency = 1}):Play()
			tween_service:Create(AsuraX_Instances.Stroke11, quick, {Transparency = 1}):Play()
			AsuraX_Instances.SuggesstionsText.Visible = false
			AsuraX_Instances.SuggesstionsImages.Visible = false
			task.wait(1)
			AsuraX_Instances.SuggesstionFrame.Visible = false
		end)()
		selectedSuggestionIndex = 1
		suggestions = {}
		suggestionButtons = {}
		updateInProgress = false
		return
	end

	if input == "" then
		coroutine.wrap(function()
			tween_service:Create(AsuraX_Instances.SuggesstionFrame, quick, {BackgroundTransparency = 1}):Play()
			tween_service:Create(AsuraX_Instances.SuggesstionsTextHighlight, quick, {TextTransparency = 1}):Play()
			tween_service:Create(AsuraX_Instances.Stroke11, quick, {Transparency = 1}):Play()
			AsuraX_Instances.SuggesstionsText.Visible = false
			AsuraX_Instances.SuggesstionsImages.Visible = false
			task.wait(1)
			AsuraX_Instances.SuggesstionFrame.Visible = false
		end)()
		selectedSuggestionIndex = 1
		suggestions = {}
		suggestionButtons = {}
		updateInProgress = false
		return
	end

	for _, child in pairs(AsuraX_Instances.SuggesstionsText:GetChildren()) do
		if child:IsA("TextButton") or child:IsA("ImageLabel") then
			child:Destroy()
		end
	end
	for _, child in pairs(AsuraX_Instances.SuggesstionsImages:GetChildren()) do
		if child:IsA("TextButton") or child:IsA("ImageLabel") then
			child:Destroy()
		end
	end
	suggestions = {}
	suggestionButtons = {}

	for func, desc in pairs(functionInfo) do
		if func:sub(1, #input) == input then
			local suggestionText = func .. " " .. desc
			table.insert(suggestions, {text = suggestionText, func = func})
		end
	end

	if #suggestions > 0 then
		if AutoComplate then
			coroutine.wrap(function()
				AsuraX_Instances.SuggesstionFrame.Visible = true
				AsuraX_Instances.SuggesstionsText.Visible = true
				AsuraX_Instances.SuggesstionsImages.Visible = true
				tween_service:Create(AsuraX_Instances.SuggesstionFrame, quick, {BackgroundTransparency = 0}):Play()
				tween_service:Create(AsuraX_Instances.SuggesstionsTextHighlight, quick, {TextTransparency = 0}):Play()
				tween_service:Create(AsuraX_Instances.Stroke11, quick, {Transparency = 0.5}):Play()
			end)()
		end

		if selectedSuggestionIndex > #suggestions then
			selectedSuggestionIndex = 1
		end

		for i, suggestion in ipairs(suggestions) do
			local suggestionText = suggestion.text
			local func = suggestion.func
			local imageId = imageAssetIds[func] or ""
			local textColor = functionColors[func] or Color3.fromRGB(255, 255, 255)

			local textLabel = Instance.new("TextButton")
			textLabel.Name = func .."Text"
			textLabel.Text = suggestionText
			textLabel.Size = UDim2.new(1, 0, 0, 12)
			textLabel.Position = UDim2.new(0, 0, 0, (i - 1) * 35)
			textLabel.TextColor3 = textColor
			textLabel.BackgroundTransparency = 1
			textLabel.TextXAlignment = Enum.TextXAlignment.Left
			textLabel.TextYAlignment = Enum.TextYAlignment.Top
			textLabel.Parent = AsuraX_Instances.SuggesstionsText

			table.insert(suggestionButtons, textLabel)

			if imageId ~= "" then
				local imageLabel = Instance.new("ImageLabel")
				imageLabel.Name = func .. "Image"
				imageLabel.Image = imageId
				imageLabel.Size = UDim2.new(0, 15, 0, 15)
				imageLabel.Position = UDim2.new(1, -35, 0, (i - 1) * 35)
				imageLabel.BackgroundTransparency = 1
				imageLabel.Parent = AsuraX_Instances.SuggesstionsImages
			end

			textLabel.MouseButton1Down:Connect(function()
				if navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox0.Visible then
					navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox0.Text = func
				elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox1.Visible then
					navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox1.Text = func
				elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox2.Visible then
					navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox2.Text = func
				elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox3.Visible then
					navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox3.Text = func
				elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox4.Visible then
					navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox4.Text = func
				elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox5.Visible then
					navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox5.Text = func
				elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox6.Visible then
					navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox6.Text = func
				elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox7.Visible then
					navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox7.Text = func
				elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox8.Visible then
					navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox8.Text = func
				elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox9.Visible then
					navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox9.Text = func
				elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox10.Visible then
					navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox10.Text = func
				end
				coroutine.wrap(function()
					tween_service:Create(AsuraX_Instances.SuggesstionFrame, quick, {BackgroundTransparency = 1}):Play()
					tween_service:Create(AsuraX_Instances.SuggesstionsTextHighlight, quick, {TextTransparency = 1}):Play()
					tween_service:Create(AsuraX_Instances.Stroke11, quick, {Transparency = 1}):Play()
					AsuraX_Instances.SuggesstionsText.Visible = false
					AsuraX_Instances.SuggesstionsImages.Visible = false
					task.wait(1)
					AsuraX_Instances.SuggesstionFrame.Visible = false
				end)()
			end)

			textLabel.MouseEnter:Connect(function()
				selectedSuggestionIndex = i
				updateSuggestionHighlights()
			end)
		end

		updateSuggestionHighlights()
	else
		coroutine.wrap(function()
			tween_service:Create(AsuraX_Instances.SuggesstionFrame, quick, {BackgroundTransparency = 1}):Play()
			tween_service:Create(AsuraX_Instances.SuggesstionsTextHighlight, quick, {TextTransparency = 1}):Play()
			tween_service:Create(AsuraX_Instances.Stroke11, quick, {Transparency = 1}):Play()
			AsuraX_Instances.SuggesstionsText.Visible = false
			AsuraX_Instances.SuggesstionsImages.Visible = false
			task.wait(1)
			AsuraX_Instances.SuggesstionFrame.Visible = false
		end)()
		selectedSuggestionIndex = 1
		suggestions = {}
		suggestionButtons = {}
	end

	updateInProgress = false
end

navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox0:GetPropertyChangedSignal("Text"):Connect(function()
	updateSuggestions()
end)
navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox1:GetPropertyChangedSignal("Text"):Connect(function()
	updateSuggestions()
end)
navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox2:GetPropertyChangedSignal("Text"):Connect(function()
	updateSuggestions()
end)
navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox3:GetPropertyChangedSignal("Text"):Connect(function()
	updateSuggestions()
end)
navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox4:GetPropertyChangedSignal("Text"):Connect(function()
	updateSuggestions()
end)
navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox5:GetPropertyChangedSignal("Text"):Connect(function()
	updateSuggestions()
end)
navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox6:GetPropertyChangedSignal("Text"):Connect(function()
	updateSuggestions()
end)
navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox7:GetPropertyChangedSignal("Text"):Connect(function()
	updateSuggestions()
end)
navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox8:GetPropertyChangedSignal("Text"):Connect(function()
	updateSuggestions()
end)
navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox9:GetPropertyChangedSignal("Text"):Connect(function()
	updateSuggestions()
end)
navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox10:GetPropertyChangedSignal("Text"):Connect(function()
	updateSuggestions()
end)

-- Keyboard navigation
navi3.Parent.scroll.build.InputBegan:Connect(function(input, gameProcessed)
	if gameProcessed then return end

	if #suggestions > 0 then
		if input.KeyCode == Enum.KeyCode.Down then
			selectedSuggestionIndex = math.min(selectedSuggestionIndex + 1, #suggestions)
			updateSuggestionHighlights()
		elseif input.KeyCode == Enum.KeyCode.Up then
			selectedSuggestionIndex = math.max(selectedSuggestionIndex - 1, 1)
			updateSuggestionHighlights()
		elseif input.KeyCode == Enum.KeyCode.Return then
			local selected = suggestions[selectedSuggestionIndex]
			if selected then
				if navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox0.Visible then
					navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox0.Text = selected.func
				elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox1.Visible then
					navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox1.Text = selected.func
				elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox2.Visible then
					navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox2.Text = selected.func
				elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox3.Visible then
					navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox3.Text = selected.func
				elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox4.Visible then
					navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox4.Text = selected.func
				elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox5.Visible then
					navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox5.Text = selected.func
				elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox6.Visible then
					navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox6.Text = selected.func
				elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox7.Visible then
					navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox7.Text = selected.func
				elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox8.Visible then
					navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox8.Text = selected.func
				elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox9.Visible then
					navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox9.Text = selected.func
				elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox10.Visible then
					navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox10.Text = selected.func
				end
				coroutine.wrap(function()
					tween_service:Create(AsuraX_Instances.SuggesstionFrame, quick, {BackgroundTransparency = 1}):Play()
					tween_service:Create(AsuraX_Instances.SuggesstionsTextHighlight, quick, {TextTransparency = 1}):Play()
					tween_service:Create(AsuraX_Instances.Stroke11, quick, {Transparency = 1}):Play()
					AsuraX_Instances.SuggesstionsText.Visible = false
					AsuraX_Instances.SuggesstionsImages.Visible = false
					task.wait(1)
					AsuraX_Instances.SuggesstionFrame.Visible = false
				end)()
			end
		end
	end
end)

local function countLines(text)
	local _, lineCount = text:gsub('\n', '\n')
	return lineCount + 1
end

local function updateLineCount()
	local text = navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox0.Text

	if navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox0.Visible then
		text = navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox0.Text
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox1.Visible then
		text = navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox1.Text
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox2.Visible then
		text = navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox2.Text
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox3.Visible then
		text = navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox3.Text
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox4.Visible then
		text = navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox4.Text
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox5.Visible then
		text = navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox5.Text
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox6.Visible then
		text = navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox6.Text
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox7.Visible then
		text = navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox7.Text
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox8.Visible then
		text = navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox8.Text
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox9.Visible then
		text = navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox9.Text
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox10.Visible then
		text = navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox10.Text
	end
	local lineCount = tonumber(countLines(text))
	if lineCount ~= previousLineCount then
		previousLineCount = lineCount

		local lineNumbers = ""
		if lineCount > 200 then
			lineCount = 200
		end
		for i = 1, lineCount do
			lineNumbers = lineNumbers .. i .. "\n"
		end

		AsuraX_Instances.LinesText.Text = lineNumbers
	end
end

navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox0:GetPropertyChangedSignal("Text"):Connect(updateLineCount)
navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox1:GetPropertyChangedSignal("Text"):Connect(updateLineCount)
navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox2:GetPropertyChangedSignal("Text"):Connect(updateLineCount)
navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox3:GetPropertyChangedSignal("Text"):Connect(updateLineCount)
navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox4:GetPropertyChangedSignal("Text"):Connect(updateLineCount)
navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox5:GetPropertyChangedSignal("Text"):Connect(updateLineCount)
navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox6:GetPropertyChangedSignal("Text"):Connect(updateLineCount)
navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox7:GetPropertyChangedSignal("Text"):Connect(updateLineCount)
navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox8:GetPropertyChangedSignal("Text"):Connect(updateLineCount)
navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox9:GetPropertyChangedSignal("Text"):Connect(updateLineCount)
navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox10:GetPropertyChangedSignal("Text"):Connect(updateLineCount)

function Insane()
	task.wait(1)
	coroutine.wrap(function()
		Configs.Functions.ActiveFunctions += 1
		if Configs.Tabs.Tab2Active and navi3:FindFirstChild("Editor2") then
			navi3:FindFirstChild("Editor2").MouseButton1Down:Connect(function()
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox0.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox1.Visible = true
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox2.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox3.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox4.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox5.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox6.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox7.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox8.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox9.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox10.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox0.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox1.Visible = true
				navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox2.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox3.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox4.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox5.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox6.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox7.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox8.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox9.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox10.Visible = false
				Configs.Tabs.TabsOpened.Tab1Active=false
				Configs.Tabs.TabsOpened.Tab2Active=true
				Configs.Tabs.TabsOpened.Tab3Active=false
				Configs.Tabs.TabsOpened.Tab4Active=false
				Configs.Tabs.TabsOpened.Tab5Active=false
				Configs.Tabs.TabsOpened.Tab6Active=false
				Configs.Tabs.TabsOpened.Tab7Active=false
				Configs.Tabs.TabsOpened.Tab8Active=false
				Configs.Tabs.TabsOpened.Tab9Active=false
				Configs.Tabs.TabsOpened.Tab10Active=false
				Configs.Tabs.TabsOpened.Tab11Active=false
			end)
		end
		if Configs.Tabs.Tab3Active and navi3:FindFirstChild("Editor3") then
			navi3:FindFirstChild("Editor3").MouseButton1Down:Connect(function()
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox0.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox1.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox2.Visible = true
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox3.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox4.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox5.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox6.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox7.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox8.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox9.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox10.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox0.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox1.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox2.Visible = true
				navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox3.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox4.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox5.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox6.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox7.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox8.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox9.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox10.Visible = false
				Configs.Tabs.TabsOpened.Tab1Active=false
				Configs.Tabs.TabsOpened.Tab2Active=false
				Configs.Tabs.TabsOpened.Tab3Active=true
				Configs.Tabs.TabsOpened.Tab4Active=false
				Configs.Tabs.TabsOpened.Tab5Active=false
				Configs.Tabs.TabsOpened.Tab6Active=false
				Configs.Tabs.TabsOpened.Tab7Active=false
				Configs.Tabs.TabsOpened.Tab8Active=false
				Configs.Tabs.TabsOpened.Tab9Active=false
				Configs.Tabs.TabsOpened.Tab10Active=false
				Configs.Tabs.TabsOpened.Tab11Active=false
			end)
		end
		if Configs.Tabs.Tab4Active and navi3:FindFirstChild("Editor4") then
			navi3:FindFirstChild("Editor4").MouseButton1Down:Connect(function()
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox0.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox1.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox2.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox3.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox4.Visible = true
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox5.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox6.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox7.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox8.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox9.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox10.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox0.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox1.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox2.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox3.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox4.Visible = true
				navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox5.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox6.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox7.Visible = false
