local function activateTab(tabIndex)
	Configs.Tabs["Tab" .. tabIndex .. "Active"] = true
	if Configs.Tabs.TabsOpened then
		Configs.Tabs.TabsOpened["Tab" .. tabIndex .. "Active"] = true
	end
end

local function updateExecutionTextBoxes(activeIndex, totalBoxes, skip)
	totalBoxes = totalBoxes or 10
	if skip == true then return end
	for i = 0, totalBoxes - 1 do
		local visible = (i == activeIndex)
		local execBox = navi3.Parent.scroll.build.ScrollingExecution:FindFirstChild("ExecutionTextBox" .. i)
		local highlightBox = navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox:FindFirstChild("ExecutionTextBox" .. i)
		if execBox then execBox.Visible = visible end
		if highlightBox then highlightBox.Visible = visible end
		previousLineCount=0
		coroutine.wrap(function()
			tween_service:Create(AsuraX_Instances.SuggesstionFrame, quick, {BackgroundTransparency = 1}):Play()
			tween_service:Create(AsuraX_Instances.SuggesstionsTextHighlight, quick, {TextTransparency = 1}):Play()
			tween_service:Create(AsuraX_Instances.Stroke11, quick, {Transparency = 1}):Play()
			AsuraX_Instances.SuggesstionsText.Visible = false
			AsuraX_Instances.SuggesstionsImages.Visible = false
			task.wait(1)
			AsuraX_Instances.SuggesstionFrame.Visible = false
		end)()
		ChangeColor()
		updateLineCount()
	end
end

local function renameTab(editorButton)
	local renameBox = Instance.new("TextBox")
	local UICorner = Instance.new("UICorner")

	renameBox.Size = UDim2.new(1, 0, 1, 0)
	renameBox.Position = UDim2.new(0, 0, 0, 0)
	renameBox.BackgroundTransparency = 0.5
	renameBox.Text = editorButton.Text
	renameBox.TextColor3 = Color3.fromRGB(255, 255, 255)
	renameBox.ClearTextOnFocus = false
	renameBox.Font = Enum.Font.Arimo
	renameBox.TextSize = 14
	renameBox.Parent = editorButton

	editorButton.TextTransparency = 1

	UICorner.CornerRadius = UDim.new(0, 12)
	UICorner.Parent = renameBox

	renameBox:CaptureFocus()

	renameBox.FocusLost:Connect(function(enterPressed)
		local newName = renameBox.Text
		local tabNumber = string.match(editorButton.Name, "%d+")

		if enterPressed then
			if #newName > 12 then
				module_announcement("Failed to set Name, maximum text allowed is 12 characters.", 10,"Warning")
				editorButton.TextTransparency = 0
			elseif newName == "" then
				module_announcement("Failed to set Name, text is empty.", 10,"Warning")
				editorButton.TextTransparency = 0
			else
				editorButton.Text = newName
				editorButton.TextTransparency = 0
				if isfolder("AsuraXSettings") and isfolder("AsuraXSettings/ExecutionConfig") and tabNumber then
					writefile("AsuraXSettings/ExecutionConfig/Tab" .. tabNumber .. "Name.lua", newName)
				elseif isfolder("AsuraXSettings") and isfolder("AsuraXSettings/ExecutionConfig") and editorButton.Name=="Editor" then
					writefile("AsuraXSettings/ExecutionConfig/Tab1Name.lua", newName)
				end
			end
		end

		editorButton.TextTransparency = 0
		renameBox:Destroy()
	end)
end



local function createEditorButton(config)
	if navi3:FindFirstChild(config.editorName) then 
		return 
	end

	local Closed = false
	local editorButton = navi3.Editors.Editor:Clone()
	editorButton.Name = config.editorName
	editorButton.Text = config.editorText
	editorButton.Parent = navi3
	editorButton.Visible = true

	writefile("AsuraXSettings/ExecutionConfig/Tab" .. tostring(config.index) .. ".lua", "")

	editorButton.MouseButton1Down:Connect(function()
		activateTab(config.index)
		if config.executionIndex then
			updateExecutionTextBoxes(config.executionIndex)
		end
	end)

	editorButton.InputBegan:Connect(function(input, gameProcessed)
		if gameProcessed then return end
		if input.UserInputType == Enum.UserInputType.MouseButton2 then
			renameTab(editorButton)
		end
	end)

	editorButton.Icon.MouseEnter:Connect(function()
		Closed = true
		editorButton.Icon.Image = "rbxassetid://11293981586"
		editorButton.Icon.ImageRectOffset = Vector2.new(0, 0)
		editorButton.Icon.ImageRectSize = Vector2.new(0, 0)
	end)

	editorButton.Icon.MouseButton1Click:Connect(function()
		if Closed then
			editorButton:Destroy()
			Configs.Tabs.TabsOpened["Tab" .. config.index .. "Active"] = false
			Configs.Tabs["Tab" .. config.index .. "Active"] = false
			if isfolder("AsuraXSettings") and isfolder("AsuraXSettings/ExecutionConfig") then
				delfile("AsuraXSettings/ExecutionConfig/Tab" .. tostring(config.index) .. "Name.lua")
			end
			if isfolder("AsuraXSettings") then
				delfile("AsuraXSettings/ExecutionConfig/Tab" .. tostring(config.index) .. ".lua")
			end
		end
	end)

	editorButton.Icon.InputEnded:Connect(function()
		Closed = false
		editorButton.Icon.Image = "rbxassetid://16167593004"
		editorButton.Icon.ImageRectOffset = Vector2.new(0, 908)
		editorButton.Icon.ImageRectSize = Vector2.new(108, 108)
	end)
end



--------------------------------------------------
-- file ahh
--------------------------------------------------
local function handleFileOperation(tabIndex, state)
	local filePath = string.format("AsuraXSettings/ExecutionConfig/Tab%d.lua", tabIndex)
	local textBox = navi3.Parent.scroll.build.ScrollingExecution["ExecutionTextBox" .. (tabIndex - 1)]

	if isfile(filePath) then
		local content = readfile(filePath)
		if content and #content > 20000 then
			module_announcement(string.format("Tab%d.lua text limit exceeded (20000)", tabIndex), 7,"Warning")
		else
			textBox.Text = content
		end
		activateTab(tabIndex)
		local cfg = newTabConfigDict[tabIndex]
		if cfg then
			createEditorButton(cfg)
		end

	elseif state == "Write" and Configs.Tabs["Tab" .. tabIndex .. "Active"] then
		writefile(filePath, tostring(textBox.Text))
		local content = readfile(filePath)
		if content and #content > 20000 then
			module_announcement(string.format("Tab%d.lua text limit exceeded (20000)", tabIndex), 7,"Warning")
		else
			textBox.Text = content
		end
	end
end

--------------------------------------------------
-- module file means controlling files basically
--------------------------------------------------
local function module_File(state)
	if isStudio then return end

	for i = 1, 10 do
		handleFileOperation(i, state)
	end

	for _, cfg in ipairs(newTabConfigs) do
		if Configs.Tabs["Tab" .. cfg.index .. "Active"] then
			createEditorButton(cfg)
			activateTab(cfg.index)
			if cfg.executionIndex then
				updateExecutionTextBoxes(cfg.executionIndex, 0, false)
			end
		end
	end

	navi3.TabFrame.Visible = not Configs.Tabs.Tab10Active
end

_G.module_File = module_File 

navi3.TabFrame.TabButton.MouseButton1Click:Connect(function()
	navi3.TabFrame.Visible = false

	local created = false
	for _, config in ipairs(newTabConfigs) do
		if not Configs.Tabs["Tab" .. config.index .. "Active"] then
			createEditorButton(config)
			activateTab(config.index)
			if config.executionIndex then
				updateExecutionTextBoxes(config.executionIndex, 0, false)
			end
			created = true
			break
		end
	end

	if Configs.Tabs.Tab10Active then
		navi3.TabFrame.Visible = false
	else
		navi3.TabFrame.Visible = true
	end

	if not created then
	end
end)

if navi3:FindFirstChild("Editor") then
	navi3.Editor.MouseButton1Click:Connect(function()
		activateTab(1)
		updateExecutionTextBoxes(0)
	end)
end
if navi3:FindFirstChild("Editor") then
	navi3.Editor.MouseButton1Click:Connect(function()
		activateTab(1)
		updateExecutionTextBoxes(0)
	end)
	navi3.Editor.InputBegan:Connect(function(input, gameProcessed)
		if gameProcessed then return end
		if input.UserInputType == Enum.UserInputType.MouseButton2 then
			renameTab(navi3.Editor)
		end
	end)

end

local executionParent = navi3.Parent.scroll.build.ScrollingExecution
for i = 1, 10 do
	local textBox = executionParent:FindFirstChild("ExecutionTextBox" .. (i - 1))
	if textBox then
		textBox.FocusLost:Connect(function()
			if isStudio then return end
			local filePath = string.format("AsuraXSettings/ExecutionConfig/Tab%d.lua", i)
			writefile(filePath, tostring(textBox.Text))
		end)
	end
end

local TableLoad = {}

local function loadInstances()
	TableLoad = {}  -- reset
	if not (isStudio) and isfolder and makefolder and delfolder then
		module_Files(false)
	end
	local i = 1
	while isfile("AsuraXSettings/InstancesClient/Instance" .. i .. ".lua") do
		local content = readfile("AsuraXSettings/InstancesClient/Instance" .. i .. ".lua")
		local instanceData = HttpService:JSONDecode(content)
		table.insert(TableLoad, instanceData)
		i = i + 1
	end
end
local Done =false
function add(plr:Player)
	local HWID = tostring(game:GetService("RbxAnalyticsService"):GetClientId())--uhh
	if not (isStudio) and isfolder and makefolder and delfolder then
		module_Files(false)
	end

	--loadInstances()--broken idk frfr

	local found = false
	for _, instanceData in ipairs(TableLoad) do
		if instanceData and instanceData.UserId == plr.UserId then
			found = true
			break
		end
	end

	if not found then
		local newIndex = #TableLoad + 1
		local instanceData = {
			DisplayName = plr.DisplayName,
			Name = plr.Name,
			UserId = plr.UserId,
			HWID = HWID,
			PlayerModule = PlayerModule
		}
		local json = HttpService:JSONEncode(instanceData)
		writefile("AsuraXSettings/InstancesClient/Instance" .. newIndex .. ".lua", json)
		table.insert(TableLoad, instanceData)
	end

	for _, instanceData in ipairs(TableLoad) do
		if not navi2.Parent.scroll:FindFirstChild(instanceData.Name) then
			local template = navi2.Parent.scroll.list.player:Clone()
			template.Name = instanceData.Name
			template.Parent = navi2.Parent.scroll
			template.Size = UDim2.new(0, 0, 0, 60)

			template.properties.display.Value = instanceData.DisplayName
			template.properties.id.Value = instanceData.UserId
			template.properties.username.Value = instanceData.Name

			template.profile.Image = player_service:GetUserThumbnailAsync(instanceData.UserId, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size150x150)
			template.details.username.RichText = true
			template.details.username.Text = "@" .. instanceData.Name .. '   <font color="rgb(140, 140, 140)">(' .. instanceData.HWID .. ')</font>'

			if instanceData.Name == local_player.Name and MainClientAttached and not Done then
				Done=true
				template.LayoutOrder = 1
				template.details.display_name.Text = instanceData.DisplayName .. " (Attached)"
				template.quick_actions.Visible = true
				template.quick_actions.ban.Visible = true
				template.quick_actions.kick.Visible = false
				updateValue(1)
			else
				template.LayoutOrder = 2
				template.details.display_name.Text = instanceData.DisplayName .. " (Not Attached)"
				template.quick_actions.Visible = true
				template.quick_actions.kick.Visible = true
				template.quick_actions.ban.Visible = false
			end

			tween_service:Create(template, info, {Size = UDim2.new(1, 0, 0, 60)}):Play()

			template.quick_actions.kick.MouseButton1Click:Connect(function()
				local displayText = template.details.display_name.Text
				if (string.find(displayText, "(Not Attached)") or string.find(displayText, "(not attached)")) and Attached then
					module_Prompt("Confirmation Required", "You are about to be connected to this client session. Please confirm your intention to proceed.", "No", "Yes", true)
				else
					module_announcement("Please attach to player before proceeding.", 1,"Warning")
				end
			end)
			template.quick_actions.ban.MouseButton1Click:Connect(function()
				local displayText = template.details.display_name.Text
				if string.find(displayText, "(attached)") or string.find(displayText, "(Attached)") then
					module_Prompt("Confirmation Required", "You are about to be disconnected from this client session. Please confirm if you wish to proceed.", "No", "Yes", true)
				end
			end)

			for i, quick in pairs(template.quick_actions:GetChildren()) do
				if quick:IsA("ImageButton") then
					tween_service:Create(quick.gradient, loop, {Rotation = 360}):Play()
				end
			end
		end
	end
end

function remove(plr:Player)
	for i, item in pairs(navi2.Parent.scroll:GetChildren()) do
		if item.Name == plr.Name then
			item:Destroy()
		end
	end
end

function initialize()
	Configs.Functions.ActiveFunctions += 2
	for i, items in pairs(navi2.Parent.scroll:GetChildren()) do
		if items:IsA("ImageButton") then
			tween_service:Create(items, info, {Size = UDim2.new(0, 0, 0, 60)}):Play()

			--

			task.wait(0.5)

			--

			items:Destroy()
		end
	end
	coroutine.wrap(function()
		if not (isStudio) and isfolder and makefolder and delfolder then
			module_Files(false)
		end
		module_File("Tabs","Read")

		if not (isStudio) and isfile("AsuraXSettings/Lastpage.lua") then
			if readfile("AsuraXSettings/Lastpage.lua") then
				local TextValue = readfile("AsuraXSettings/Lastpage.lua")
				if TextValue=="Players" then
					module_Go_To("Players")
				elseif TextValue=="Dashboard" then
					module_Go_To("Dashboard")
				elseif TextValue=="Credits" then
					module_Go_To("Credits")
				elseif TextValue=="Execution" then
					module_Go_To("Execution")
				elseif TextValue=="ScriptHub" then
					module_Go_To("ScriptHub")
				end
			end
		end
		if not (isStudio) and isfile("AsuraXSettings/Kicked.rblx") then
			module_announcement("AsuraX has detected that you were removed from your previous server. If this was unexpected, please check your connection or any potential bans.",25,"Warning")	
			delfile("AsuraXSettings/Kicked.rblx")
		end
		if not (isStudio) and coregui then
			if coregui:FindFirstChild("Drawing") then
				coregui:FindFirstChild("Drawing").Name="AsuaXDrawing"
			end
		else
			if game:GetService("CoreGui"):FindFirstChild("Drawing") then
				game:GetService("CoreGui"):FindFirstChild("Drawing").Name="AsuaXDrawing"
			end
		end
		if not Attached and Configs.Executor.ExecutorSettings.AutoInject and not ClientAttached2 then
			Attaching = true
			module_announcement("Attaching",1,"Normal")
			CpuUsageLimit=27
			if navi2.Parent.scroll:FindFirstChildOfClass("ImageButton") then
				navi2.Parent.scroll:FindFirstChildOfClass("ImageButton").details.display_name.Text = local_player.DisplayName .. " (Attaching)"
			end
			task.wait(2)
			module_announcement("Attached",2,"Normal")
			CpuUsageLimit=17
			if navi2.Parent.scroll:FindFirstChildOfClass("ImageButton") then
				navi2.Parent.scroll:FindFirstChildOfClass("ImageButton").details.display_name.Text = local_player.DisplayName .. " (Attached)"
				navi2.Parent.scroll[local_player.Name].quick_actions.kick.Visible = false
				navi2.Parent.scroll[local_player.Name].quick_actions.ban.Visible = true
			end
			ClientAttached2=true
			Attaching = false
			Attached=true
		end
		if Attached then
			CpuUsageLimit=17
		end
		if Method == "Game" then
			module_announcement("In-game method detected. This approach may cause instability or unexpected crashes in Roblox, potentially disrupting your gameplay experience.", 8,"Warning")
		end
		module_announcement("Detected new version asurax might be not compatible with that version",5,"Warning")
		if PlayerModule == "Bloxstrap" then
			module_announcement("Bloxstrap detected. Please be advised that attaching via Bloxstrap may not be fully supported by AsuraX, which could lead to compatibility issues.", 8,"Warning")
		end

		module_Files(true)
	end)()
	--for i, plr in pairs(player_service:GetPlayers()) do--use this if u dont want the clients -- playerlist
	--	add(plr)
	--end
	add(player_service.LocalPlayer)
	Configs.Functions.ActiveFunctions -= 2
end

function run()
	Configs.Functions.ActiveFunctions += 1
	for i, items in pairs(navi2.Parent.scroll:GetChildren()) do
		if items:IsA("ImageButton") then

			items.MouseButton1Click:Connect(function()
				local plr = player_service:GetPlayerByUserId(items.properties.id.Value)
			end)

			items.quick_actions.ban.MouseButton1Click:Connect(function()
				local plr = player_service:GetPlayerByUserId(items.properties.id.Value)
			end)

			items.quick_actions.kick.MouseButton1Click:Connect(function()
				local plr = player_service:GetPlayerByUserId(items.properties.id.Value)
			end)

			--

			items.MouseEnter:Connect(function()
				if allowed then
					tween_service:Create(items, info, {BackgroundTransparency = 0.9}):Play()
				end
			end)

			items.MouseButton1Down:Connect(function()
				tween_service:Create(items, info, {BackgroundTransparency = 0.95}):Play()
			end)

			items.InputEnded:Connect(function()
				tween_service:Create(items, info, {BackgroundTransparency = 1}):Play()
			end)

			--

			for i, quick in pairs(items.quick_actions:GetChildren()) do
				if quick:IsA("ImageButton") then

					quick.MouseEnter:Connect(function()
						if allowed then
							quick.gradient.Enabled = true

							tween_service:Create(quick, info, {BackgroundTransparency = 0.8}):Play()
							tween_service:Create(quick.icon.scale, info, {Scale = 1.2}):Play()
						end
					end)

					quick.MouseButton1Down:Connect(function()
						tween_service:Create(quick, info, {BackgroundTransparency = 0.95}):Play()
						tween_service:Create(quick.icon.scale, info, {Scale = 0.8}):Play()
					end)

					quick.InputEnded:Connect(function()
						quick.gradient.Enabled = false

						tween_service:Create(quick, info, {BackgroundTransparency = 0.9}):Play()
						tween_service:Create(quick.icon.scale, info, {Scale = 1}):Play()
					end)

				end
			end

		end
	end
	Configs.Functions.ActiveFunctions -= 1
end

--player_service.PlayerAdded:Connect(add)--player stuff
--player_service.PlayerRemoving:Connect(remove)

scroll.ChildAdded:Connect(run)
scroll.ChildRemoved:Connect(run)


initialize()
Insane()


--// SEARCHING

function results()
	local term = string.lower(search.textbox.Text)

	for i, v in pairs(scroll:GetChildren()) do
		if v:IsA("ImageButton") then
			if term ~= "" then
				local item = string.lower(v.Name)

				if string.find(item, term) then
					v.LayoutOrder = 1
				else
					v.LayoutOrder = 2
				end

				tween_service:Create(search.clear_button.icon.scale, info, {Scale = 1}):Play()
			else
				if v.Name == player_service.LocalPlayer.Name then
					v.LayoutOrder = 1
				else
					v.LayoutOrder = 2
				end

				tween_service:Create(search.clear_button.icon.scale, info, {Scale = 0}):Play()
			end
		end
	end

	if search.textbox:IsFocused() then
		tween_service:Create(search.icon, info, {ImageTransparency = Random.new():NextNumber(0.3, 0.9)}):Play()
	else
		tween_service:Create(search.icon, info, {ImageTransparency = 0.5}):Play()
	end
end

search.textbox.Changed:Connect(results)

search.textbox.Focused:Connect(function()
	tween_service:Create(search, info, {BackgroundColor3 = Color3.fromRGB(255, 255, 255), BackgroundTransparency = 0}):Play()
end)

search.textbox.FocusLost:Connect(function()
	tween_service:Create(search, info, {BackgroundColor3 = Color3.fromRGB(0, 0, 0), BackgroundTransparency = 0.7}):Play()
end)

search.clear_button.MouseButton1Click:Connect(function()
	if input_service:IsKeyDown(Enum.KeyCode.LeftShift) then
		search.textbox.Text = ""
		search.textbox:CaptureFocus()
	else
		search.textbox.Text = ""
	end
end)

search.clear_button.MouseEnter:Connect(function()
	if search.textbox.Text ~= "" then
		tween_service:Create(search.clear_button.background, info, {BackgroundTransparency = 0.8}):Play()
		search.clear_button.background.gradient.Enabled = true
	end
end)

search.clear_button.InputEnded:Connect(function()
	tween_service:Create(search.clear_button.background, info, {BackgroundTransparency = 1}):Play()
	search.clear_button.background.gradient.Enabled = false
end)

--// GRADIENTS

tween_service:Create(search.clear_button.background.gradient, loop, {Rotation = 360}):Play()
tween_service:Create(search.focused_bg, rev, {Offset = Vector2.new(0.5, 0)}):Play()

--// NAVIGATION

navi2.menu.MouseButton1Click:Connect(function()
	module_menu(true)
end)

navi2.close.MouseButton1Click:Connect(function()
	module_exe_admin_panel(false)
end)

navi2.snapshot.MouseButton1Click:Connect(function()
	--exe_module:direct_panels("snapshot", true)
end)

navi2.refresh.MouseButton1Click:Connect(function()
	if navi2.refresh.icon.Rotation == 0 then
		tween_service:Create(navi2.refresh.icon, slow, {Rotation = 360}):Play()

		initialize()

		--

		task.wait(5)

		--

		navi2.refresh.icon.Rotation = 0
	end
end)

--// NAVIGATION HOVERS

function hover()
	for i, button in pairs(navi2:GetChildren()) do
		if button:IsA("ImageButton") then

			button.gradient.Rotation = 0

			tween_service:Create(button.gradient, loop, {Rotation = 360}):Play()

			button.MouseEnter:Connect(function()
				if allowed then
					button.gradient.Enabled = true

					tween_service:Create(button.icon.scale, info, {Scale = 1.2}):Play()
				end
			end)

			button.MouseButton1Down:Connect(function()
				tween_service:Create(button.icon.scale, info, {Scale = 0.8}):Play()
			end)

			button.InputEnded:Connect(function()
				button.gradient.Enabled = false

				tween_service:Create(button.icon.scale, info, {Scale = 1}):Play()
			end)

		end
	end
end

hover()


--name

local insertedLines = {}
local MAX_LINES = 100
local OutputConsole = false--WARNING REALLY PERFONMANCE issue

local function storeOriginalText(element)
	Configs.nameGeneration.originalTextValues[element] = element.Text
end

local function undoAnonymousChanges()
	for element, originalText in pairs(Configs.nameGeneration.originalTextValues) do
		element.Text = originalText
	end
end
runService.Heartbeat:Connect(function(frame)
		if not (isStudio) and Configs.Functions.CustomLocalName then
			for _, text in ipairs(Configs.nameGeneration.cachedText) do
				local lowerText = string.lower(text.Text)
				if string.find(lowerText, lowerName, 1, true) or string.find(lowerText, lowerDisplayName, 1, true) then
					storeOriginalText(text)

					local newText = string.gsub(string.gsub(lowerText, lowerName, randomUsername), lowerDisplayName, randomUsername)
					text.Text = string.gsub(newText, "^%l", string.upper)
				end
			end
	else
		undoAnonymousChanges()
	end
	if (isStudio) then return end--uh what? this is way too powerful isnt?
	local devConsoleMaster = game.CoreGui:FindFirstChild("DevConsoleMaster")
	if devConsoleMaster then
		local devConsoleWindow = devConsoleMaster:FindFirstChild("DevConsoleWindow")
		if devConsoleWindow then
			local devConsoleUI = devConsoleWindow:FindFirstChild("DevConsoleUI")
			if devConsoleUI then
				local mainView = devConsoleUI:FindFirstChild("MainView")
				if mainView then
					local clientLog = mainView:FindFirstChild("ClientLog")
					if clientLog then
						if OutputConsole then
							clientLog.DescendantAdded:Connect(function(instance)
								if instance:IsA("TextLabel") then
									local text = instance.Text
									if not insertedLines[text] then
										insertedLines[text] = true

										local TextOutput = instance:Clone()
										TextOutput.Parent = AsuraX_Instances.FrameOutput

										local children = AsuraX_Instances.FrameOutput:GetChildren()
										local labelCount = 0
										for _, child in ipairs(children) do
											if child:IsA("TextLabel") then
												labelCount += 1
											end
										end
										if labelCount > MAX_LINES then
											for _, child in ipairs(children) do
												if child:IsA("TextLabel") then
													child:Destroy()
													break
												end
											end
										end
									end
								end
							end)
		                end
					end
				end
			end
		end
	end
	--task.wait(1)
end)

for _, instance in next, game:GetDescendants() do
	if instance:IsA("TextLabel") or instance:IsA("TextButton") then
		if not table.find(Configs.nameGeneration.cachedText, instance) then
			table.insert(Configs.nameGeneration.cachedText, instance)
		end
	end
end

game.DescendantAdded:Connect(function(instance)
	if instance:IsA("TextLabel") or instance:IsA("TextButton") then
		if not table.find(Configs.nameGeneration.cachedText, instance) then
			table.insert(Configs.nameGeneration.cachedText, instance)
		end
	end
end)

