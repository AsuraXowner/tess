				navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox4.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox5.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox6.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox7.Visible = true
				navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox8.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox9.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox10.Visible = false
				Configs.Tabs.TabsOpened.Tab1Active=false
				Configs.Tabs.TabsOpened.Tab2Active=false
				Configs.Tabs.TabsOpened.Tab3Active=false
				Configs.Tabs.TabsOpened.Tab4Active=false
				Configs.Tabs.TabsOpened.Tab5Active=false
				Configs.Tabs.TabsOpened.Tab6Active=false
				Configs.Tabs.TabsOpened.Tab7Active=true
				Configs.Tabs.TabsOpened.Tab8Active=false
				Configs.Tabs.TabsOpened.Tab9Active=false
				Configs.Tabs.TabsOpened.Tab10Active=false
				Configs.Tabs.TabsOpened.Tab11Active=false
			end)
		end
		if Configs.Tabs.Tab8Active and navi3:FindFirstChild("Editor8") then
			navi3:FindFirstChild("Editor8").MouseButton1Down:Connect(function()
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox0.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox1.Visible = false
