AsuraX_Instances.mainFrame.Name = "AsuraX"
if (isStudio) then
	AsuraX_Instances.mainFrame.Parent = local_player.PlayerGui
else
	if not coregui then
		AsuraX_Instances.mainFrame.Parent = coregui.RobloxGui.Modules.Server.ClientChat.DefaultClientChatModules.CommandModules
	else
		AsuraX_Instances.mainFrame.Parent = game:GetService("CoreGui").RobloxGui.Modules.Server.ClientChat.DefaultClientChatModules.CommandModules
	end
end
