	{ keyword = "adustytrip", asset = "rbxassetid://17492516124" },
}



function CreateScriptHub(title, desc, Author, verified, Reviews, HasKey, universal, loadstring, likeCount, dislikeCount, owner, patched)
	Configs.Functions.ActiveFunctions += 1
	local template = navi4.scroll.list.Script:Clone()
	template.Name = "Script: " .. title
	template.Parent = navi4.scroll

	template.Author_name.Text = "uploaded by " .. (owner and owner.username or "unknown")
	template.Script_description.Text = desc
	template.Script_name.Text = title

	template.Tags.KeySystem.Visible = HasKey
	template.Tags.Review.Visible = Reviews
	template.Tags.Universal.Visible = universal
	template.Tags.Verified.Visible = owner and owner.verified or false

	template.LoadstringValue.Value = loadstring

	local lowerTitle = title:lower()
	for _, mapping in ipairs(imageMappings) do
		if string.find(lowerTitle, mapping.keyword) then
			template.ScriptPicture.Image = mapping.asset
			break
		else
			template.ScriptPicture.Image = "rbxassetid://16308272122"
			break
		end
	end

	if patched then
		template.Tags.KeySystem.Title.Text = "Patched"
		template.Tags.KeySystem.Visible = patched
	end

	local likes = likeCount or 0
	local dislikes = dislikeCount or 0

	if likes ~= dislikes then
		template.Tags.Review.Title.Text = (likes > dislikes) and "Positive Reviews" or "Negative Reviews"
		template.Tags.Review.BackgroundColor3 = (likes > dislikes) and Color3.fromRGB(0, 139, 102) or Color3.fromRGB(180, 0, 0)
		template.Tags.Review.stroke.Color = (likes > dislikes) and Color3.fromRGB(78, 126, 75) or Color3.fromRGB(140, 0, 0)
	elseif likes > 0 then
		template.Tags.Review.Title.Text = "Mixed Reviews"
		template.Tags.Review.BackgroundColor3 = Color3.fromRGB(198, 132, 0)
		template.Tags.Review.stroke.Color = Color3.fromRGB(148, 112, 0)
	else
		template.Tags.Review.Visible = false
	end

	template.Visible = true

	template.PasteInExecutor.Title.MouseButton1Click:Connect(function()
		if template.LoadstringValue.Value and type(template.LoadstringValue.Value) == "string" then
			if navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox0.Visible then
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox0.Text = template.LoadstringValue.Value
			elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox1.Visible then
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox1.Text = template.LoadstringValue.Value
			elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox2.Visible then
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox2.Text = template.LoadstringValue.Value
			elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox3.Visible then
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox3.Text = template.LoadstringValue.Value
			elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox4.Visible then
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox4.Text = template.LoadstringValue.Value
			elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox5.Visible then
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox5.Text = template.LoadstringValue.Value
			elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox6.Visible then
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox6.Text = template.LoadstringValue.Value
			elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox7.Visible then
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox7.Text = template.LoadstringValue.Value
			elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox8.Visible then
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox8.Text = template.LoadstringValue.Value
			elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox9.Visible then
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox9.Text = template.LoadstringValue.Value
			elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox10.Visible then
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox10.Text = template.LoadstringValue.Value
			end
		end
	end)
	template.RunScript.Title.MouseButton1Click:Connect(function()
		if (isStudio) then return end
		setclipboard(loadstring)
		module_announcement("Copied to Clipboard", 10,"Normal")
	end)


	--hover setup
	template.RunScript.Title.MouseEnter:Connect(function()
		tween_service:Create(template.RunScript, info, {BackgroundTransparency = 0}):Play()
		tween_service:Create(template.RunScript.Title, info, {TextTransparency = 0}):Play()
	end)
	template.RunScript.Title.MouseLeave:Connect(function()
		tween_service:Create(template.RunScript, info, {BackgroundTransparency = 0.2}):Play()
		tween_service:Create(template.RunScript.Title, info, {TextTransparency = 0.4}):Play()
	end)
	template.PasteInExecutor.Title.MouseEnter:Connect(function()
		tween_service:Create(template.PasteInExecutor, info, {BackgroundTransparency = 0}):Play()
		tween_service:Create(template.PasteInExecutor.Title, info, {TextTransparency = 0}):Play()
	end)
	template.PasteInExecutor.Title.MouseLeave:Connect(function()
		tween_service:Create(template.PasteInExecutor, info, {BackgroundTransparency = 0.2}):Play()
		tween_service:Create(template.PasteInExecutor.Title, info, {TextTransparency = 0.4}):Play()
	end)
	Configs.Functions.ActiveFunctions -= 1
	return template
end

function SearchScripts(query, paid, page, max)
	ClearSearchResults()

	local response = RawSearch(query, page or 1, max or 10, paid or false)
	assert(response and response.result, "No results found.")

	local result = response.result

	for _, script in pairs(result.scripts or {}) do
		local slugData = FetchSlug(script)

		local title = script.title or "Unknown Title"
		local desc = (slugData.script and slugData.script.features) or "No description available"
		local owner = (slugData.script and slugData.script.owner) or {}
		local author = owner.username or "Uploaded by unknown"
		local likeCount = (slugData.script and slugData.script.likeCount) or 5
		local dislikeCount = (slugData.script and slugData.script.dislikeCount) or 5
		local hasKey = (slugData.script and slugData.script.key) or false
		local universal = (slugData.script and slugData.script.isUniversal) or false
		local verified = (slugData.script and slugData.script.verified)
		local patched = (slugData.script and slugData.script.isPatched)
		local scriptLoadstring = (slugData.script and slugData.script.script) or ""

		if IsScriptDuplicate(title) then
		else
			local frame = CreateScriptHub(title, desc, author, verified, likeCount > 5, hasKey, universal, scriptLoadstring, likeCount, dislikeCount, owner, patched)
			table.insert(createdFrames, frame)
		end
	end

	return createdFrames
end

local function updateSearch()
	if AsuraX_Instances.TextBox6.Text == "" then
		ClearSearchResults()
	else
		ClearSearchResults()
		if (isStudio) then module_announcement("Studio mode is active cannot search",5,"Warning") return end
		SearchScripts(AsuraX_Instances.TextBox6.Text, false, 1, 20)
	end
end


AsuraX_Instances.TextBox6.FocusLost:Connect(function()
	tween_service:Create(search, info, {BackgroundColor3 = Color3.fromRGB(0, 0, 0), BackgroundTransparency = 0.7}):Play()
	updateSearch()
end)

AsuraX_Instances.TextBox6.Focused:Connect(function()
	tween_service:Create(search, info, {BackgroundColor3 = Color3.fromRGB(255, 255, 255), BackgroundTransparency = 0}):Play()
end)

if AsuraX_Instances.TextBox6:IsFocused() then
	tween_service:Create(navi4.search.icon, info, {ImageTransparency = Random.new():NextNumber(0.3, 0.9)}):Play()
else
	tween_service:Create(navi4.search.icon, info, {ImageTransparency = 0.5}):Play()
end

