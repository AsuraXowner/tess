-- Module Scripts

function module_Files(load)
	if not (isStudio) then
		if not load then
			if not isfolder("AsuraXSettings") then
				makefolder("AsuraXSettings")
			end

			if not isfolder("AsuraXSettings/SettingsConfig") then
				makefolder("AsuraXSettings/SettingsConfig")
			end
			if not isfolder("AsuraXSettings/ExecutionConfig") then
				makefolder("AsuraXSettings/ExecutionConfig")
			end

			if not isfolder("AsuraXSettings/InstancesClient") then
				makefolder("AsuraXSettings/InstancesClient")
			end
		else
			if isfile("AsuraXSettings/SettingsConfig/HideLocalName.lua") then
				local Value = readfile("AsuraXSettings/SettingsConfig/HideLocalName.lua")

				if Value == "true" then
					Configs.Functions.CustomLocalName = true
				end
			end
			if isfile("AsuraXSettings/ExecutionConfig/Tab1Name.lua") then
				local Value = readfile("AsuraXSettings/ExecutionConfig/Tab1Name.lua")

				if Configs.Tabs.Tab1Active and navi3:FindFirstChild("Editor") then
					navi3:FindFirstChild("Editor").Text = Value
				end
			end
			if isfile("AsuraXSettings/ExecutionConfig/Tab2Name.lua") then
				local Value = readfile("AsuraXSettings/ExecutionConfig/Tab2Name.lua")

				if Configs.Tabs.Tab2Active and navi3:FindFirstChild("Editor2") then
					navi3:FindFirstChild("Editor2").Text = Value
				end
			end
			if isfile("AsuraXSettings/ExecutionConfig/Tab3Name.lua") then
				local Value = readfile("AsuraXSettings/ExecutionConfig/Tab3Name.lua")

				if Configs.Tabs.Tab3Active and navi3:FindFirstChild("Editor3") then
					navi3:FindFirstChild("Editor3").Text = Value
				end
			end
			if isfile("AsuraXSettings/ExecutionConfig/Tab4Name.lua") then
				local Value = readfile("AsuraXSettings/ExecutionConfig/Tab4Name.lua")

				if Configs.Tabs.Tab4Active and navi3:FindFirstChild("Editor4") then
					navi3:FindFirstChild("Editor4").Text = Value
				end
			end
			if isfile("AsuraXSettings/ExecutionConfig/Tab5Name.lua") then
				local Value = readfile("AsuraXSettings/ExecutionConfig/Tab5Name.lua")

				if Configs.Tabs.Tab5Active and navi3:FindFirstChild("Editor5") then
					navi3:FindFirstChild("Editor5").Text = Value
				end
			end
			if isfile("AsuraXSettings/ExecutionConfig/Tab6Name.lua") then
				local Value = readfile("AsuraXSettings/ExecutionConfig/Tab6Name.lua")

				if Configs.Tabs.Tab6Active and navi3:FindFirstChild("Editor6") then
					navi3:FindFirstChild("Editor6").Text = Value
				end
			end
			if isfile("AsuraXSettings/ExecutionConfig/Tab7Name.lua") then
				local Value = readfile("AsuraXSettings/ExecutionConfig/Tab7Name.lua")

				if Configs.Tabs.Tab7Active and navi3:FindFirstChild("Editor7") then
					navi3:FindFirstChild("Editor7").Text = Value
				end
			end
			if isfile("AsuraXSettings/ExecutionConfig/Tab8Name.lua") then
				local Value = readfile("AsuraXSettings/ExecutionConfig/Tab8Name.lua")

				if Configs.Tabs.Tab8Active and navi3:FindFirstChild("Editor8") then
					navi3:FindFirstChild("Editor8").Text = Value
				end
			end
			if isfile("AsuraXSettings/ExecutionConfig/Tab9Name.lua") then
				local Value = readfile("AsuraXSettings/ExecutionConfig/Tab9Name.lua")

				if Configs.Tabs.Tab9Active and navi3:FindFirstChild("Editor9") then
					navi3:FindFirstChild("Editor9").Text = Value
				end
			end
			if isfile("AsuraXSettings/ExecutionConfig/Tab10Name.lua") then
				local Value = readfile("AsuraXSettings/ExecutionConfig/Tab10Name.lua")

				if Configs.Tabs.Tab10Active and navi3:FindFirstChild("Editor10") then
					navi3:FindFirstChild("Editor10").Text = Value
				end
			end
		end
	end
end


function module_announcement(Text,Duration,state)
	coroutine.wrap(function()
		local notification = Instance.new("ImageButton")
		local padding = Instance.new("UIPadding")
		local corner = Instance.new("UICorner")
		local gradient = Instance.new("UIGradient")
		local description = Instance.new("TextLabel")
		local icon = Instance.new("ImageButton")
		local close = Instance.new("ImageButton")
		local timeline = Instance.new("ImageLabel")
		local bar = Instance.new("ImageLabel")
		local scale = Instance.new("UIScale")
		local scale1 = Instance.new("UIScale")
		local Stroke = Instance.new("UIStroke")
		
		local ColorBackground = Color3.fromRGB(79, 79, 79)
		local ColorImageBackground = Color3.fromRGB(80, 80, 80)
		local ColorStrokeGradient = Color3.fromRGB(255, 255, 255)
		
		if state=="Warning" then
			ColorBackground = Color3.fromRGB(112, 55, 9)
			ColorImageBackground = Color3.fromRGB(197, 108, 0)
			ColorStrokeGradient = Color3.fromRGB(112, 55, 9)
		elseif state=="Error" then
			ColorBackground = Color3.fromRGB(112, 58, 66)
			ColorImageBackground = Color3.fromRGB(197, 127, 128)
			ColorStrokeGradient = Color3.fromRGB(255, 47, 50)
		elseif state=="Normal" then
			ColorBackground = Color3.fromRGB(79, 79, 79)
			ColorImageBackground = Color3.fromRGB(80, 80, 80)
			ColorStrokeGradient = Color3.fromRGB(255, 255, 255)
		else
			ColorBackground = Color3.fromRGB(79, 79, 79)
			ColorImageBackground = Color3.fromRGB(80, 80, 80)
			ColorStrokeGradient = Color3.fromRGB(255, 255, 255)
		end


		notification.Name = "notification"
		notification.Parent = AsuraX_Instances.notificationList
		notification.AnchorPoint = Vector2.new(0.5, 0)
		notification.AutomaticSize = Enum.AutomaticSize.X
		notification.BackgroundColor3 = ColorBackground
		notification.BackgroundTransparency = 1
		notification.BorderColor3 = Color3.fromRGB(0, 0, 0)
		notification.BorderSizePixel = 0
		notification.ClipsDescendants = true
		notification.Position = UDim2.new(0.5, 0, 0, 30)
		notification.Selectable = false
		notification.Size = UDim2.new(0, 0, 0, 40)
		notification.ZIndex = 10
		notification.AutoButtonColor = false
		--notification.Visible = false
		notification.Image = "rbxassetid://16294030997"
		notification.ImageTransparency = 1
		notification.ImageColor3 = ColorImageBackground
		notification.ScaleType = Enum.ScaleType.Crop
		notification.SliceCenter = Rect.new(512, 512, 512, 512)
		notification.SliceScale = 4

		padding.Name = "padding"
		padding.Parent = notification
		padding.PaddingBottom = UDim.new(0, 5)
		padding.PaddingLeft = UDim.new(0, 20)
		padding.PaddingRight = UDim.new(0, 20)
		padding.PaddingTop = UDim.new(0, 5)

		corner.CornerRadius = UDim.new(0, 100)
		corner.Name = "corner"
		corner.Parent = notification

		description.Name = "description"
		description.AutomaticSize = Enum.AutomaticSize.X
		description.Parent = notification
		description.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
		description.BackgroundTransparency = 1
		description.BorderColor3 = Color3.fromRGB(0, 0, 0)
		description.BorderSizePixel = 0
		description.Position = UDim2.new(0, 26, 0, 0)
		description.Size = UDim2.new(0, 0, 1, 0)
		description.ZIndex = 10
		description.Font = Enum.Font.Arimo
		description.Text = "Notification"
		description.TextColor3 = Color3.fromRGB(255, 255, 255)
		description.TextSize = 15
		description.TextTransparency = 1

		icon.Name = "icon"
		icon.Parent = notification
		icon.AnchorPoint = Vector2.new(0.5, 0.5)
		icon.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		icon.BackgroundTransparency = 1
		icon.BorderColor3 = Color3.fromRGB(0, 0, 0)
		icon.BorderSizePixel = 0
		icon.Position = UDim2.new(0, 8, 0.5, 0)
		icon.Size = UDim2.new(0, 16, 0, 16)
		icon.ZIndex = 10
		icon.Image = "rbxassetid://11295275950"
		icon.ImageTransparency = 1
		icon.ScaleType = Enum.ScaleType.Fit

		scale.Name = "scale"
		scale.Parent = icon

		Stroke.Name = "stroke"
		Stroke.Color = Color3.fromRGB(255, 255, 255)
		Stroke.Parent = notification
		Stroke.Transparency = 1

		gradient.Rotation = 80
		gradient.Color = ColorSequence.new{ColorSequenceKeypoint.new(0, ColorStrokeGradient), ColorSequenceKeypoint.new(1, Color3.fromRGB(255, 255, 255))}
		gradient.Transparency = NumberSequence.new{NumberSequenceKeypoint.new(0, 0.69), NumberSequenceKeypoint.new(1, 0.87)}
		gradient.Name = "gradient"
		gradient.Parent = Stroke

		close.Name = "close"
		close.Parent = notification
		close.AnchorPoint = Vector2.new(0.5, 0.5)
		close.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		close.BackgroundTransparency = 1
		close.BorderColor3 = Color3.fromRGB(0, 0, 0)
		close.BorderSizePixel = 0
		close.Position = UDim2.new(0, 8, 0.5, 0)
		close.Size = UDim2.new(0, 16, 0, 16)
		close.ZIndex = 10
		close.Interactable=false
		close.Image = "rbxassetid://11293981586"
		close.ImageTransparency = 1
		close.ScaleType = Enum.ScaleType.Fit

		scale1.Name = "scale"
		scale1.Parent = close

		timeline.Name = "timeline"
		timeline.Parent = notification
		timeline.AnchorPoint = Vector2.new(0.5, 1)
		timeline.BackgroundColor3 = Color3.fromRGB(100, 106, 105)
		timeline.BackgroundTransparency = 1
		timeline.BorderColor3 = Color3.fromRGB(0, 0, 0)
		timeline.BorderSizePixel = 0
		timeline.ClipsDescendants = true
		timeline.Position = UDim2.new(0.5, 0, 2, 0)
		timeline.Size = UDim2.new(0.100000001, 50, 0, 2)
		timeline.Image = "rbxassetid://16294678871"
		timeline.ImageTransparency = 1
		timeline.ScaleType = Enum.ScaleType.Slice
		timeline.SliceCenter = Rect.new(206, 206, 206, 206)

		bar.Name = "bar"
		bar.Parent = timeline
		bar.AnchorPoint = Vector2.new(0, 0.5)
		bar.BackgroundColor3 = Color3.fromRGB(100, 106, 105)
		bar.BackgroundTransparency = 1
		bar.BorderColor3 = Color3.fromRGB(0, 0, 0)
		bar.BorderSizePixel = 0
		bar.ClipsDescendants = true
		bar.Position = UDim2.new(0, 0, 0.5, 0)
		bar.Size = UDim2.new(0, 0, 1, 0)
		bar.Image = "rbxassetid://16294678871"
		bar.ImageTransparency = 1
		bar.ScaleType = Enum.ScaleType.Slice
		bar.SliceCenter = Rect.new(206, 206, 206, 206)

		icon.MouseEnter:Connect(function()
			if icon.ImageTransparency == 1 then return end
			--InDebounce=true
			tween_service:Create(close, quick, {ImageTransparency = 0}):Play()
			close.Interactable=true
			icon.Interactable=false
			tween_service:Create(icon, quick, {ImageTransparency = 1}):Play()
			task.wait(0.7)
			--InDebounce=false
		end)

		notification.InputEnded:Connect(function()
			if close.ImageTransparency == 1 then return end
			--InDebounce=true
			tween_service:Create(close, quick, {ImageTransparency = 1}):Play()
			close.Interactable=false
			icon.Interactable=true
			tween_service:Create(icon, quick, {ImageTransparency = 0}):Play()
			task.wait(0.7)
			--InDebounce=false
		end)

		close.MouseButton1Click:Connect(function()
			tween_service:Create(notification, quick, {BackgroundTransparency = 1}):Play()
			tween_service:Create(notification, quick, {ImageTransparency = 1}):Play()
			tween_service:Create(description, quick, {TextTransparency = 1}):Play()
			tween_service:Create(close, quick, {ImageTransparency = 1}):Play()
			tween_service:Create(icon, quick, {ImageTransparency = 1}):Play()
			tween_service:Create(Stroke, quick, {Transparency = 1}):Play()
			mainFrame:FindFirstChildOfClass("Sound"):Destroy()
			task.wait(0.5)
			notification:Destroy()
		end)
		tween_service:Create(gradient, loop, {Rotation = 360}):Play()

		local Sound = Instance.new("Sound")
		Sound.Parent = mainFrame
		Sound.SoundId="rbxassetid://3398620867"
		Sound:Play()
		description.Text=Text
		task.wait(0.1)
		tween_service:Create(notification, quick, {BackgroundTransparency = 0.05}):Play()
		tween_service:Create(notification, quick, {ImageTransparency = 0}):Play()
		tween_service:Create(close, quick, {ImageTransparency = 1}):Play()
		tween_service:Create(icon, quick, {ImageTransparency = 0}):Play()
		tween_service:Create(Stroke, quick, {Transparency = 0}):Play()
		tween_service:Create(description, quick, {TextTransparency = 0.2}):Play()
		task.wait(Duration)
		tween_service:Create(notification, quick, {BackgroundTransparency = 1}):Play()
		tween_service:Create(notification, quick, {ImageTransparency = 1}):Play()
		tween_service:Create(description, quick, {TextTransparency = 1}):Play()
		tween_service:Create(close, quick, {ImageTransparency = 1}):Play()
		tween_service:Create(icon, quick, {ImageTransparency = 1}):Play()
		tween_service:Create(Stroke, quick, {Transparency = 1}):Play()
		if mainFrame:FindFirstChildOfClass("Sound") then
			mainFrame:FindFirstChildOfClass("Sound"):Destroy()
		end
		task.wait(0.5)
		notification:Destroy()
	end)()
end

function module_exe_admin_panel(state)
	if state then
		if not frame.Visible then
			StorageOpened = true

			frame.Visible = true
			frame.BackgroundTransparency = 1
			frame.ImageTransparency = 1

			frame.scale.Scale = 1.2

			frame.dashboard_frame.GroupTransparency = 1
			frame.main_frame.GroupTransparency = 1
			frame.window_controls.GroupTransparency = 1

			tween_service:Create(button, quick, {ImageTransparency = 1}):Play()
			tween_service:Create(button.page.front.open.Icon, quick, {TextTransparency = 1}):Play()
			tween_service:Create(button.page.options.dashboard.icon, quick, {ImageTransparency = 1}):Play()
			tween_service:Create(button.page.options.dashboard.icon, quick, {ImageTransparency = 1}):Play()
			tween_service:Create(button.page.options.dashboard.icon, quick, {ImageTransparency = 1}):Play()
			mainFrame.accessibility_button.Interactable = false

			--load stuff
			if Method=="Game" or Method=="Menu" then
				navi.Parent.scroll.contributors.item.value.Text = Method
			end
			if Method=="Player" or Method=="Bloxstrap" then
				navi.Parent.scroll.system.model.value.Text = Method
			end
			tween_service:Create(frame, info, {BackgroundTransparency = 0.1, ImageTransparency = 0.8}):Play()

			tween_service:Create(frame.dashboard_frame, info, {GroupTransparency = 0}):Play()
			tween_service:Create(frame.main_frame, info, {GroupTransparency = 0}):Play()
			tween_service:Create(frame.credits_frame, info, {GroupTransparency = 0}):Play()

			tween_service:Create(frame.window_controls, info, {GroupTransparency = 0}):Play()

			tween_service:Create(blur, info, {Size = 40}):Play()

			if Storagefullscreen then
				tween_service:Create(frame.scale, info, {Scale = 1}):Play()
			else
				tween_service:Create(frame.shadow, info, {ImageTransparency = 0}):Play()
				tween_service:Create(frame.scale, info, {Scale = Storagescale_on_window}):Play()
			end
		end
	else
		StorageOpened = false

		tween_service:Create(frame, info, {BackgroundTransparency = 1, ImageTransparency = 1}):Play()

		tween_service:Create(frame.dashboard_frame, info, {GroupTransparency = 1}):Play()
		tween_service:Create(frame.main_frame, info, {GroupTransparency = 1}):Play()
		tween_service:Create(frame.credits_frame, info, {GroupTransparency = 1}):Play()

		tween_service:Create(frame.shadow, info, {ImageTransparency = 1}):Play()

		tween_service:Create(frame.window_controls, info, {GroupTransparency = 1}):Play()

		tween_service:Create(blur, info, {Size = 0}):Play()

		tween_service:Create(frame.scale, info, {Scale = 1.2}):Play()

		tween_service:Create(button, quick, {ImageTransparency = 0.8}):Play()
		tween_service:Create(button.page.front.open.Icon, quick, {TextTransparency = 0}):Play()
		tween_service:Create(button.page.options.dashboard.icon, quick, {ImageTransparency = 0}):Play()
		tween_service:Create(button.page.options.dashboard.icon, quick, {ImageTransparency = 0}):Play()
		tween_service:Create(button.page.options.dashboard.icon, quick, {ImageTransparency = 0}):Play()
		mainFrame.accessibility_button.Interactable = true

		--

		task.wait(0.5)

		--

		frame.Visible = false
	end
end

function module_menu(state)
	if state and not menu.Visible then
		menu.Visible = true
		menu.menu.Position = UDim2.new(0, -60, 0.5, 0)
		menu.menu.scale.Scale = 0.5

		tween_service:Create(menu, info, {BackgroundTransparency = 0.5}):Play()
		tween_service:Create(menu.menu, info, {AnchorPoint = Vector2.new(0.5, 0.5), Position = UDim2.new(0, 60, 0.5, 0)}):Play()
		tween_service:Create(menu.menu.scale, info, {Scale = 1}):Play()

	else
		tween_service:Create(menu, info, {BackgroundTransparency = 1}):Play()
		tween_service:Create(menu.menu, info, {AnchorPoint = Vector2.new(0, 0.5), Position = UDim2.new(0, -60, 0.5, 0)}):Play()
		tween_service:Create(menu.menu.scale, info, {Scale = 0.5}):Play()

		--

		task.wait(0.5)

		--

		menu.Visible = false
	end
end

function module_Go_To(page:pages)
	Configs.Functions.ActiveFunctions += 1
	if page == "Players" then
		main_frame.ClipsDescendants = true

		main_frame.credits_frame.Visible = false
		main_frame.Execution_frame.Visible = false
		main_frame.dashboard_frame.Visible = false
		main_frame.main_frame.Visible = true

		--

		tween_service:Create(main_frame.credits_frame, slow, {Position = UDim2.new(0.5, 0, 1.2, 0), GroupTransparency = 1}):Play()
		tween_service:Create(main_frame.Execution_frame, slow, {Position = UDim2.new(0.5, 0, 1, 0), GroupTransparency = 1}):Play()
		tween_service:Create(main_frame.dashboard_frame, slow, {Position = UDim2.new(0.5, 0, 1.2, 0), GroupTransparency = 1}):Play()
		tween_service:Create(main_frame.main_frame, slow, {Position = UDim2.new(0.5, 0, 1, 0), GroupTransparency = 0}):Play()

		--


		main_frame.ClipsDescendants = false

		--save last page
		if not (isStudio) then
			writefile("AsuraXSettings/Lastpage.lua", "Players")
		end
	elseif page == "Dashboard" then

		main_frame.ClipsDescendants = true

		main_frame.credits_frame.Visible = false
		main_frame.Execution_frame.Visible = false
		main_frame.dashboard_frame.Visible = true
		main_frame.main_frame.Visible = false

		--

		tween_service:Create(main_frame.credits_frame, slow, {Position = UDim2.new(0.5, 0, 1.2, 0), GroupTransparency = 1}):Play()
		tween_service:Create(main_frame.Execution_frame, slow, {Position = UDim2.new(0.5, 0, 1, 0), GroupTransparency = 1}):Play()
		tween_service:Create(main_frame.dashboard_frame, slow, {Position = UDim2.new(0.5, 0, 1, 0), GroupTransparency = 0}):Play()
		tween_service:Create(main_frame.main_frame, slow, {Position = UDim2.new(0.5, 0, 1.2, 0), GroupTransparency = 1}):Play()

		--

		main_frame.ClipsDescendants = false

		--save last page
		if not (isStudio) then
			writefile("AsuraXSettings/Lastpage.lua", "Dashboard")
		end
	elseif page == "Credits" then
		main_frame.ClipsDescendants = true

		main_frame.credits_frame.Visible = true
		main_frame.Execution_frame.Visible = false
		main_frame.dashboard_frame.Visible = false
		main_frame.main_frame.Visible = false

		--// ACCESSIBILITY BUTTON RECENT

		options.recent.icon.Image = "rbxassetid://11419720347"
		options.recent.icon.corner.CornerRadius = UDim.new(0, 0)

		options.recent.func:SetAttribute("profile", false)
		options.recent.func:SetAttribute("dashboard", false)
		options.recent.func:SetAttribute("credits", true)
		options.recent.func:SetAttribute("execution", false)

		--

		tween_service:Create(main_frame.credits_frame, slow, {Position = UDim2.new(0.5, 0, 1, 0), GroupTransparency = 0}):Play()
		tween_service:Create(main_frame.Execution_frame, slow, {Position = UDim2.new(0.5, 0, 1, 0), GroupTransparency = 1}):Play()
		tween_service:Create(main_frame.dashboard_frame, slow, {Position = UDim2.new(0.5, 0, 1.2, 0), GroupTransparency = 1}):Play()
		tween_service:Create(main_frame.main_frame, slow, {Position = UDim2.new(0.5, 0, 1.2, 0), GroupTransparency = 1}):Play()

		--


		main_frame.ClipsDescendants = false

		--save last page
		if not (isStudio) then
			writefile("AsuraXSettings/Lastpage.lua", "Credits")
		end
	elseif page == "Execution" then
		main_frame.ClipsDescendants = true

		main_frame.credits_frame.Visible = false
		main_frame.Execution_frame.Visible = true
		main_frame.dashboard_frame.Visible = false
		main_frame.main_frame.Visible = false

		--// ACCESSIBILITY BUTTON RECENT

		options.recent.func:SetAttribute("profile", false)
		options.recent.func:SetAttribute("dashboard", false)
		options.recent.func:SetAttribute("credits", false)
		options.recent.func:SetAttribute("execution", true)

		--

		tween_service:Create(main_frame.credits_frame, slow, {Position = UDim2.new(0.5, 0, 1, 0), GroupTransparency = 1}):Play()
		tween_service:Create(main_frame.Execution_frame, slow, {Position = UDim2.new(0.5, 0, 1, 0), GroupTransparency = 0}):Play()
		tween_service:Create(main_frame.dashboard_frame, slow, {Position = UDim2.new(0.5, 0, 1.2, 0), GroupTransparency = 1}):Play()
		tween_service:Create(main_frame.main_frame, slow, {Position = UDim2.new(0.5, 0, 1.2, 0), GroupTransparency = 1}):Play()

		--

		main_frame.ClipsDescendants = false

		--save last page
		if not (isStudio) then
			writefile("AsuraXSettings/Lastpage.lua", "Execution")
		end
	elseif page == "ScriptHub" then
		main_frame.ClipsDescendants = true

		main_frame.ScriptHub.Visible = true

		--// ACCESSIBILITY BUTTON RECENT

		tween_service:Create(navi4.scale, info, {Scale = 1}):Play()

		--

		main_frame.ClipsDescendants = false

		--save last page
		if not (isStudio) then
			writefile("AsuraXSettings/Lastpage.lua", "ScriptHub")
		end
	end
	Configs.Functions.ActiveFunctions -= 1
end

function accessibility(state)
	if StorageOpened and Storagefullscreen then return end
	Configs.Functions.ActiveFunctions += 1
	if state then
		page.page:JumpTo(page.options)

		--

		tween_service:Create(button, info, {Size = UDim2.fromOffset(270, 40)}):Play()

		tween_service:Create(button, info, {ImageTransparency = 0}):Play()
		tween_service:Create(button.scale, info, {Scale = 1.2}):Play()

		task.spawn(function()
			tween_service:Create(page.options.players.icon, inst, {ImageTransparency = 1}):Play()
			tween_service:Create(page.options.dashboard.icon, inst, {ImageTransparency = 1}):Play()
			tween_service:Create(page.options.recent.icon, inst, {ImageTransparency = 1}):Play()

			tween_service:Create(page.options.players.icon.scale, inst, {Scale = 0}):Play()
			tween_service:Create(page.options.dashboard.icon.scale, inst, {Scale = 0}):Play()
			tween_service:Create(page.options.recent.icon.scale, inst, {Scale = 0}):Play()

			--
			task.wait()
			--

			tween_service:Create(page.options.players.icon, quick, {ImageTransparency = 0}):Play()
			tween_service:Create(page.options.players.icon.scale, quick, {Scale = 1}):Play()

			--
			task.wait(0.1)
			--

			tween_service:Create(page.options.dashboard.icon, quick, {ImageTransparency = 0}):Play()
			tween_service:Create(page.options.dashboard.icon.scale, quick, {Scale = 1}):Play()

			--
			task.wait(0.1)
			--

			tween_service:Create(page.options.recent.icon, quick, {ImageTransparency = 0}):Play()
			tween_service:Create(page.options.recent.icon.scale, quick, {Scale = 1}):Play()

		end)
	else
		page.page:JumpTo(page.front)

		--

		tween_service:Create(button, info, {Size = UDim2.fromOffset(190, 40)}):Play()
	end
	Configs.Functions.ActiveFunctions -= 1
end

function module_Prompt(PromptName,PromptDesc,cancel,confirm,open)
	if open then
		coroutine.wrap(function()
			local Sound = Instance.new("Sound")
			Sound.Parent = mainFrame.frame
			Sound.SoundId="rbxassetid://18886652611"
			Sound:Play()
			task.wait(3)
			mainFrame.frame:FindFirstChildOfClass("Sound"):Destroy()
		end)()
		tween_service:Create(main_frame.confirmation_prompt, info, {ImageTransparency = 0.5}):Play()
		main_frame.confirmation_prompt.prompt.description.Text = PromptDesc
		main_frame.confirmation_prompt.prompt.header.Text = PromptName
		main_frame.confirmation_prompt.prompt.confirm.Text = confirm.." (Y)"
		main_frame.confirmation_prompt.prompt.cancel.Text = cancel.." (U)"

		main_frame.confirmation_prompt.Visible = true


		--// ACCESSIBILITY BUTTON RECENT

		tween_service:Create(main_frame.confirmation_prompt.prompt.scale, info, {Scale = 1}):Play()
	elseif not open then
		tween_service:Create(main_frame.confirmation_prompt.prompt.scale, info, {Scale = 0}):Play()
		task.wait(1)
		tween_service:Create(main_frame.confirmation_prompt, info, {ImageTransparency = 1}):Play()
		main_frame.confirmation_prompt.prompt.description.Text = ""
		main_frame.confirmation_prompt.prompt.header.Text = ""
		main_frame.confirmation_prompt.prompt.confirm.Text = ""
		main_frame.confirmation_prompt.prompt.cancel.Text = ""
		main_frame.confirmation_prompt.Visible = false
	end
end

local function updateValue(delta)
	local currentVal = tonumber(AsuraX_Instances.value1.Text) or 0
	AsuraX_Instances.value1.Text = tostring(currentVal + delta)
end

-- Function to attach to a specific instance UI epic
local function attachInstance(instanceUI)
	if not instanceUI:FindFirstChild("details") then return end
	instanceUI.details.display_name.Text = local_player.DisplayName .. " (Attaching)"
	task.wait(3)
	instanceUI.details.display_name.Text = local_player.DisplayName .. " (Attached)"
	instanceUI.quick_actions.kick.Visible = false
	instanceUI.quick_actions.ban.Visible = true
	--updateValue(1)
	ClientAttached2 = true
	if instanceUI.Name== local_player.Name then
		MainClientAttached=true
	end
end

-- Function to detach from a instance UI
local function detachInstance(instanceUI)
	if not instanceUI:FindFirstChild("details") then return end
	instanceUI.details.display_name.Text = local_player.DisplayName .. " (Deattaching)"
	task.wait(3)
	instanceUI.details.display_name.Text = local_player.DisplayName .. " (Not Attached)"
	instanceUI.quick_actions.kick.Visible = true
	instanceUI.quick_actions.ban.Visible = false
	updateValue(-1)
	ClientAttached2 = false
	if instanceUI.Name== local_player.Name then
		MainClientAttached=false
	end
end

local function module_execute(parent,text)
	Configs.Functions.ActiveFunctions += 1
	if Attached and ClientAttached2 then
		if text=="" then
			module_announcement("Text Empty",5,"Warning")
		elseif text==('loadstring(game:HttpGet("https://github.com/juhoa-fr/universal/raw/main/sc"))()') and Configs.Settings.FakeScriptDetector==true then
			module_announcement("This script contains malware",5,"Warning")
		elseif text==("saveinstance()") then
			module_announcement("Saveinstance Not supported",5,"Warning")
			--RunSaveInstance()
		else
			--warn("loadstring success")
			--local script = HttpService:GetAsync(url)
			--if Configs.Settings.LoadstringRQ == true then
			local success, result = pcall(loadstring(text)())
			module_announcement("Executed",5,"Normal")
			if not success then
				module_announcement("Error: "..result,10,"Warning")
			end
			--module_announcement("Executed",5)
		end
	elseif not Attached then
		module_announcement("Not Attached",5,"Warning")
	elseif not ClientAttached2 then
		module_announcement("No Clients selected.",5,"Warning")
	else
		module_announcement("Loadstring Empty",5,"Normal")
	end
	Configs.Functions.ActiveFunctions -= 1
end

