				Configs.Tabs.TabsOpened.Tab8Active=false
				Configs.Tabs.TabsOpened.Tab9Active=true
				Configs.Tabs.TabsOpened.Tab10Active=false
				Configs.Tabs.TabsOpened.Tab11Active=false
			end)
		end

		if Configs.Tabs.Tab10Active and navi3:FindFirstChild("Editor10") then
			navi3:FindFirstChild("Editor10").MouseButton1Down:Connect(function()
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox0.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox1.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox2.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox3.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox4.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox5.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox6.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox7.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox8.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox9.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox10.Visible = true
				navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox0.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox1.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox2.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox3.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox4.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox5.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox6.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox7.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox8.Visible = false
				navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox9.Visible = false
