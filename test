				Configs.Tabs.TabsOpened.Tab8Active=false
				Configs.Tabs.TabsOpened.Tab9Active=false
				Configs.Tabs.TabsOpened.Tab10Active=true
				Configs.Tabs.TabsOpened.Tab11Active=false
			end)
		end
		Configs.Functions.ActiveFunctions -= 1
		if Configs.BanwaveGames[game.PlaceId] then
			module_announcement("This game is flagged as part of the current Roblox Hunt event banwave. Your account may be at risk of restrictions. Please proceed with caution.", 10,"Warning")
		end
	end)()
end
--highlight stuff
function ChangeColor()
	Configs.Functions.ActiveFunctions += 1
	local text = navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox0.Text

	if navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox0.Visible then
		text = navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox0.Text
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox1.Visible then
		text = navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox1.Text
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox2.Visible then
		text = navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox2.Text
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox3.Visible then
		text = navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox3.Text
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox4.Visible then
		text = navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox4.Text
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox5.Visible then
		text = navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox5.Text
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox6.Visible then
		text = navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox6.Text
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox7.Visible then
		text = navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox7.Text
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox8.Visible then
		text = navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox8.Text
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox9.Visible then
		text = navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox9.Text
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox10.Visible then
		text = navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox10.Text
	end

	if #text > 20000 then
		if navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox0.Visible then
			module_announcement("Highlight failed on Tab1 text limit has been passed 20000",5,"Warning")
			navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox0.Text = ""
		elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox1.Visible then
			module_announcement("Highlight failed on Tab2 text limit has been passed 20000",5,"Warning")
			navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox1.Text = ""
		elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox2.Visible then
			module_announcement("Highlight failed on Tab3 text limit has been passed 20000",5,"Warning")
			navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox2.Text = ""
		elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox3.Visible then
			module_announcement("Highlight failed on Tab4 text limit has been passed 20000",5,"Warning")
			navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox3.Text = ""
		elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox4.Visible then
			module_announcement("Highlight failed on Tab5 text limit has been passed 20000",5,"Warning")
			navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox4.Text = ""
		elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox5.Visible then
			module_announcement("Highlight failed on Tab6 text limit has been passed 20000",5,"Warning")
			navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox5.Text = ""
		elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox6.Visible then
			module_announcement("Highlight failed on Tab7 text limit has been passed 20000",5,"Warning")
			navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox6.Text = ""
		elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox7.Visible then
			module_announcement("Highlight failed on Tab8 text limit has been passed 20000",5,"Warning")
			navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox7.Text = ""
		elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox8.Visible then
			module_announcement("Highlight failed on Tab9 text limit has been passed 20000",5,"Warning")
			navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox8.Text = ""
		elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox9.Visible then
			module_announcement("Highlight failed on Tab10 text limit has been passed 20000",5,"Warning")
			navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox9.Text = ""
		elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox10.Visible then
			module_announcement("Highlight failed on Tab11 text limit has been passed 20000",5,"Warning")
			navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox10.Text = ""
		end
		return
	end

	local formattedText = text

	formattedText = formattedText:gsub('".-"', function(quoted)
		return '<font color="#f1fa8c">' .. quoted .. '</font>'
	end)

	formattedText = formattedText:gsub('([(){}%[%]+%%])', function(symbol)
		return '<font color="#ff79c6">' .. symbol .. '</font>'
	end)

	formattedText = formattedText:gsub('%-%-.*', function(comment)
		return '<font color="#6272a4">' .. comment .. '</font>'
	end)

	formattedText = formattedText:gsub('(%f[%a]%a+%f[%A])', function(word)
		if language.keyword[word] then
			return '<font color="#ff79c6">' .. word .. '</font>'
		end
		return word
	end)

	formattedText = formattedText:gsub('(%f[%a]%a+%f[%A])', function(word)
		for library, funcs in pairs(language.libraries) do
			if funcs[word] then
				return '<font color="#bd93f9">' .. word .. '</font>'
			end
		end
		return word
	end)

	formattedText = formattedText:gsub('(%f[%a]%a+%f[%A])', function(word)
		if language.events[word] then
			return '<font color="#ffb86c">' .. word .. '</font>'
		end
		return word
	end)

	formattedText = formattedText:gsub('(%f[%a]%a+%f[%A])', function(word)
		if language.builtin[word] then
			local color = (language.builtin[word] == "function") and "#8be9fd" or "#f1fa8c"
			return '<font color="' .. color .. '">' .. word .. '</font>'
		end
		return word
	end)

	formattedText = formattedText:gsub('([%w_]+)%s*%b()', function(funcCall)
		local funcName, args = funcCall:match('^([%w_]+)%s*%b()$')
		if funcName and language.builtin[funcName] then
			local functionColor = (language.builtin[funcName] == "function") and "#8be9fd" or "#f1fa8c"
			local argsFormatted = args:gsub('"([^"]*)"', function(arg) return '<font color="#50fa7b">&quot;' .. arg .. '&quot;</font>' end)
			return '<font color="' .. functionColor .. '">' .. funcName .. '</font><font color="#ff79c6">(</font>' .. argsFormatted .. '<font color="#ff79c6">)</font>'
		end
		Configs.Functions.ActiveFunctions -= 1
		return funcCall
	end)

	if #formattedText > 20000 then
		if navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox0.Visible then
			module_announcement("Highlight failed on Tab1 text limit has been passed 20000",5,"Warning")
			navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox0.Text = ""
		elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox1.Visible then
			module_announcement("Highlight failed on Tab2 text limit has been passed 20000",5,"Warning")
			navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox1.Text = ""
		elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox2.Visible then
			module_announcement("Highlight failed on Tab3 text limit has been passed 20000",5,"Warning")
			navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox2.Text = ""
		elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox3.Visible then
			module_announcement("Highlight failed on Tab4 text limit has been passed 20000",5,"Warning")
			navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox3.Text = ""
		elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox4.Visible then
			module_announcement("Highlight failed on Tab5 text limit has been passed 20000",5,"Warning")
			navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox4.Text = ""
		elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox5.Visible then
			module_announcement("Highlight failed on Tab6 text limit has been passed 20000",5,"Warning")
			navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox5.Text = ""
		elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox6.Visible then
			module_announcement("Highlight failed on Tab7 text limit has been passed 20000",5,"Warning")
			navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox6.Text = ""
		elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox7.Visible then
			module_announcement("Highlight failed on Tab8 text limit has been passed 20000",5,"Warning")
			navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox7.Text = ""
		elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox8.Visible then
			module_announcement("Highlight failed on Tab9 text limit has been passed 20000",5,"Warning")
			navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox8.Text = ""
		elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox9.Visible then
