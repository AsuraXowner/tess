navi3.Parent.Buttons.AttachButton.MouseButton1Click:Connect(function()
	if Attaching and not ClientAttached2 then
		module_announcement("Already Attaching",20,"Normal")
	end
	if not ClientAttached2 and Attached then
		module_announcement("Client not Attached",20,"Normal")
	end
	if Attached and ClientAttached2 then
		module_announcement("Already Attached",20,"Normal")
	elseif not Attached and not ClientAttached2 then
		Attaching = true
		module_announcement("Attaching",1,"Normal")
		if navi2.Parent.scroll:FindFirstChildOfClass("ImageButton") then
			navi2.Parent.scroll:FindFirstChildOfClass("ImageButton").details.display_name.Text = local_player.DisplayName .. " (Attaching)"
		end
		task.wait(2)
		module_announcement("Attached",2,"Normal")
		if navi2.Parent.scroll:FindFirstChildOfClass("ImageButton") then
			navi2.Parent.scroll:FindFirstChildOfClass("ImageButton").details.display_name.Text = local_player.DisplayName .. " (Attached)"
			navi2.Parent.scroll[local_player.Name].quick_actions.kick.Visible = false
			navi2.Parent.scroll[local_player.Name].quick_actions.ban.Visible = true
		end
		Attaching = false
		ClientAttached2=true
		Attached=true
	end
end)


