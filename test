Configs.Functions.ActiveFunctions += 13

local CpuUsageLimit = 7

coroutine.wrap(function()
	if not (isStudio) then
		local queueonteleport = queue_on_teleport or syn.queue_on_teleport
		if queueonteleport then
			if Configs.Functions.CustomLocalName or isfile("AsuraXSettings/SettingsConfig/HideLocalName.lua") then
				local Value = readfile("AsuraXSettings/SettingsConfig/HideLocalName.lua")
				if Value == "true" then
					module_announcement("Welcome back, User",10,"Normal")
				else
					module_announcement("Welcome back, "..local_player.DisplayName,10,"Normal")
				end
			else
				module_announcement("Welcome back, "..local_player.DisplayName,10,"Normal")
			end
		end
	end

	task.spawn(function()
		while true do
			local fakeCpuUsage = math.random(1, CpuUsageLimit)
			AsuraX_Instances.value4.Text = fakeCpuUsage .. "%"
			task.wait(2)
		end
	end)

	AsuraX_Instances.value4:GetPropertyChangedSignal("Text"):Connect(function()
		local newValue = tonumber(AsuraX_Instances.value4.Text)
		if newValue then
			CpuUsageLimit = math.clamp(newValue, 2, 100)
		end
	end)
end)()

-- API shit
local createdFrames = {}

function ClearSearchResults()
	for _, frame in ipairs(navi4.scroll:GetChildren()) do
		if frame and frame:IsA("Frame") then
			frame:Destroy()
		else
		end
	end
	createdFrames = {} 
end

local API = "https://scriptblox.com/api/"

local function FetchUrl(url)
	assert(game.HttpGet, "HttpGet is not supported on your executor.")
	local response = game:HttpGet(url)
	return game:GetService("HttpService"):JSONDecode(response)
end

local function RawSearch(query, page, max, paid)
	local url = API .. "script/search?q=" .. tostring(query):gsub(" ", "+") .. "&page=" .. tostring(page) .. "&max=" .. tostring(max)
	if paid then
		url = url .. "&mode=paid"
	else
		url = url .. "&mode=free"
	end
	return FetchUrl(url)
end

function FetchSlug(script)
	return FetchUrl(API .. "script/" .. script.slug)
end

local function IsScriptDuplicate(title)
	for _, frame in ipairs(navi4.scroll:GetChildren()) do
		if frame.Name == "Script: " .. title then
			return true
		end
	end
	return false
end

