		end
		return
	end
	if navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox0.Visible then
		navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox0.Text = formattedText
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox1.Visible then
		navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox1.Text = formattedText
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox2.Visible then
		navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox2.Text = formattedText
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox3.Visible then
		navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox3.Text = formattedText
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox4.Visible then
		navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox4.Text = formattedText
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox5.Visible then
		navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox5.Text = formattedText
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox6.Visible then
		navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox6.Text = formattedText
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox7.Visible then
		navi3.Parent.scroll.build.ScrollingExecution.HighlightExecutionTextBox.ExecutionTextBox7.Text = formattedText
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox8.Visible then
