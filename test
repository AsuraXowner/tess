			end)
		end
		if Configs.Tabs.Tab5Active and navi3:FindFirstChild("Editor5") then
			navi3:FindFirstChild("Editor5").MouseButton1Down:Connect(function()
				navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox0.Visible = false
