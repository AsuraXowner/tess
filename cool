local AutoComplate = true
local selectedSuggestionIndex = 1
local suggestions = {}
local suggestionButtons = {}

local function updateSuggestionHighlights()
	for i, btn in ipairs(suggestionButtons) do
		if i == selectedSuggestionIndex then
			btn.BackgroundColor3 = Color3.fromRGB(0, 157, 255)
			btn.BackgroundTransparency = 0.3
		else
			btn.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			btn.BackgroundTransparency = 1
		end
	end
end

local function updateSuggestions()--i dont really wanna work on this again gave me seizure
	local updateInProgress = false
	if updateInProgress then return end
	updateInProgress = true

	local input = navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox0.Text:lower()

	if navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox0.Visible then
		input = navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox0.Text:lower()
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox1.Visible then
		input = navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox1.Text:lower()
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox2.Visible then
		input = navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox2.Text:lower()
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox3.Visible then
		input = navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox3.Text:lower()
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox4.Visible then
		input = navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox4.Text:lower()
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox5.Visible then
		input = navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox5.Text:lower()
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox6.Visible then
		input = navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox6.Text:lower()
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox7.Visible then
		input = navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox7.Text:lower()
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox8.Visible then
		input = navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox8.Text:lower()
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox9.Visible then
		input = navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox9.Text:lower()
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox10.Visible then
		input = navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox10.Text:lower()
	end

	if input:match("local%s$") or input:match("\n$") then
		coroutine.wrap(function()
			tween_service:Create(Essence_Instances.SuggesstionFrame, quick, {BackgroundTransparency = 1}):Play()
			tween_service:Create(Essence_Instances.SuggesstionsTextHighlight, quick, {TextTransparency = 1}):Play()
			tween_service:Create(Essence_Instances.Stroke11, quick, {Transparency = 1}):Play()
			Essence_Instances.SuggesstionsText.Visible = false
			Essence_Instances.SuggesstionsImages.Visible = false
			task.wait(1)
			Essence_Instances.SuggesstionFrame.Visible = false
		end)()
		selectedSuggestionIndex = 1
		suggestions = {}
		suggestionButtons = {}
		updateInProgress = false
		return
	end

	if input == "" then
		coroutine.wrap(function()
			tween_service:Create(Essence_Instances.SuggesstionFrame, quick, {BackgroundTransparency = 1}):Play()
			tween_service:Create(Essence_Instances.SuggesstionsTextHighlight, quick, {TextTransparency = 1}):Play()
			tween_service:Create(Essence_Instances.Stroke11, quick, {Transparency = 1}):Play()
			Essence_Instances.SuggesstionsText.Visible = false
			Essence_Instances.SuggesstionsImages.Visible = false
			task.wait(1)
			Essence_Instances.SuggesstionFrame.Visible = false
		end)()
		selectedSuggestionIndex = 1
		suggestions = {}
		suggestionButtons = {}
		updateInProgress = false
		return
	end

	for _, child in pairs(Essence_Instances.SuggesstionsText:GetChildren()) do
		if child:IsA("TextButton") or child:IsA("ImageLabel") then
			child:Destroy()
		end
	end
	for _, child in pairs(Essence_Instances.SuggesstionsImages:GetChildren()) do
		if child:IsA("TextButton") or child:IsA("ImageLabel") then
			child:Destroy()
		end
	end
	suggestions = {}
	suggestionButtons = {}

	for func, desc in pairs(functionInfo) do
		if func:sub(1, #input) == input then
			local suggestionText = func .. " " .. desc
			table.insert(suggestions, {text = suggestionText, func = func})
		end
	end

	if #suggestions > 0 then
		if AutoComplate then
			coroutine.wrap(function()
				Essence_Instances.SuggesstionFrame.Visible = true
				Essence_Instances.SuggesstionsText.Visible = true
				Essence_Instances.SuggesstionsImages.Visible = true
				tween_service:Create(Essence_Instances.SuggesstionFrame, quick, {BackgroundTransparency = 0}):Play()
				tween_service:Create(Essence_Instances.SuggesstionsTextHighlight, quick, {TextTransparency = 0}):Play()
				tween_service:Create(Essence_Instances.Stroke11, quick, {Transparency = 0.5}):Play()
			end)()
		end

		if selectedSuggestionIndex > #suggestions then
			selectedSuggestionIndex = 1
		end

		for i, suggestion in ipairs(suggestions) do
			local suggestionText = suggestion.text
			local func = suggestion.func
			local imageId = imageAssetIds[func] or ""
			local textColor = functionColors[func] or Color3.fromRGB(255, 255, 255)

			local textLabel = Instance.new("TextButton")
			textLabel.Name = func .."Text"
			textLabel.Text = suggestionText
			textLabel.Size = UDim2.new(1, 0, 0, 12)
			textLabel.Position = UDim2.new(0, 0, 0, (i - 1) * 35)
			textLabel.TextColor3 = textColor
			textLabel.BackgroundTransparency = 1
			textLabel.TextXAlignment = Enum.TextXAlignment.Left
			textLabel.TextYAlignment = Enum.TextYAlignment.Top
			textLabel.Parent = Essence_Instances.SuggesstionsText

			table.insert(suggestionButtons, textLabel)

			if imageId ~= "" then
				local imageLabel = Instance.new("ImageLabel")
				imageLabel.Name = func .. "Image"
				imageLabel.Image = imageId
				imageLabel.Size = UDim2.new(0, 15, 0, 15)
				imageLabel.Position = UDim2.new(1, -35, 0, (i - 1) * 35)
				imageLabel.BackgroundTransparency = 1
				imageLabel.Parent = Essence_Instances.SuggesstionsImages
			end

			textLabel.MouseButton1Down:Connect(function()
				if navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox0.Visible then
					navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox0.Text = func
				elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox1.Visible then
					navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox1.Text = func
				elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox2.Visible then
					navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox2.Text = func
				elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox3.Visible then
					navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox3.Text = func
				elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox4.Visible then
					navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox4.Text = func
				elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox5.Visible then
					navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox5.Text = func
				elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox6.Visible then
					navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox6.Text = func
				elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox7.Visible then
					navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox7.Text = func
				elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox8.Visible then
					navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox8.Text = func
				elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox9.Visible then
					navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox9.Text = func
				elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox10.Visible then
					navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox10.Text = func
				end
				coroutine.wrap(function()
					tween_service:Create(Essence_Instances.SuggesstionFrame, quick, {BackgroundTransparency = 1}):Play()
					tween_service:Create(Essence_Instances.SuggesstionsTextHighlight, quick, {TextTransparency = 1}):Play()
					tween_service:Create(Essence_Instances.Stroke11, quick, {Transparency = 1}):Play()
					Essence_Instances.SuggesstionsText.Visible = false
					Essence_Instances.SuggesstionsImages.Visible = false
					task.wait(1)
					Essence_Instances.SuggesstionFrame.Visible = false
				end)()
			end)

			textLabel.MouseEnter:Connect(function()
				selectedSuggestionIndex = i
				updateSuggestionHighlights()
			end)
		end

		updateSuggestionHighlights()
	else
		coroutine.wrap(function()
			tween_service:Create(Essence_Instances.SuggesstionFrame, quick, {BackgroundTransparency = 1}):Play()
			tween_service:Create(Essence_Instances.SuggesstionsTextHighlight, quick, {TextTransparency = 1}):Play()
			tween_service:Create(Essence_Instances.Stroke11, quick, {Transparency = 1}):Play()
			Essence_Instances.SuggesstionsText.Visible = false
			Essence_Instances.SuggesstionsImages.Visible = false
			task.wait(1)
			Essence_Instances.SuggesstionFrame.Visible = false
		end)()
		selectedSuggestionIndex = 1
		suggestions = {}
		suggestionButtons = {}
	end

	updateInProgress = false
end

--i tried to small these functions it didnt work so i said if it works dont touch it
navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox0:GetPropertyChangedSignal("Text"):Connect(function()
	updateSuggestions()
end)
navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox1:GetPropertyChangedSignal("Text"):Connect(function()
	updateSuggestions()
end)
navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox2:GetPropertyChangedSignal("Text"):Connect(function()
	updateSuggestions()
end)
navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox3:GetPropertyChangedSignal("Text"):Connect(function()
	updateSuggestions()
end)
navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox4:GetPropertyChangedSignal("Text"):Connect(function()
	updateSuggestions()
end)
