--Properties
Essence_Instances.MainFrame.Name = "Essence"
if (isStudio) then
	Essence_Instances.MainFrame.Parent = local_player.PlayerGui
else
	if not coregui then
		Essence_Instances.MainFrame.Parent = coregui.RobloxGui.Modules.Server.ClientChat.DefaultClientChatModules.CommandModules
	else
		Essence_Instances.MainFrame.Parent = game:GetService("CoreGui").RobloxGui.Modules.Server.ClientChat.DefaultClientChatModules.CommandModules
	end
end
Essence_Instances.MainFrame.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
Essence_Instances.MainFrame.DisplayOrder = 1000
Essence_Instances.MainFrame.ResetOnSpawn = false
Essence_Instances.MainFrame.IgnoreGuiInset = true

Essence_Instances.menu_frame.Name = "menu_frame"
Essence_Instances.menu_frame.Parent = Essence_Instances.frame
Essence_Instances.menu_frame.Active = true
Essence_Instances.menu_frame.AnchorPoint = Vector2.new(0.5, 0.5)
Essence_Instances.menu_frame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
Essence_Instances.menu_frame.BackgroundTransparency = 0.500
Essence_Instances.menu_frame.BorderColor3 = Color3.fromRGB(0, 0, 0)
Essence_Instances.menu_frame.BorderSizePixel = 0
Essence_Instances.menu_frame.ClipsDescendants = true
Essence_Instances.menu_frame.Position = UDim2.new(0.5, 0, 0.5, 0)
