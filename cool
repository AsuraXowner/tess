	module_menu(true)
end)

navi.close.MouseButton1Click:Connect(function()
	module_panel(false)
end)

--// NAVIGATION HOVERS

function hover1()
	for i, button in pairs(MainFrame.frame.about_frame.scroll:GetChildren()) do
		if button:IsA("TextButton") then

			button.MouseEnter:Connect(function()
				
				if not Configs.Executor.Animations then
				    button.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
				else
					tween_service:Create(button, info, {BackgroundColor3 = Color3.fromRGB(40, 40, 40)}):Play()
				end
			end)

			button.MouseButton1Down:Connect(function()
				if not Configs.Executor.Animations then
					button.BackgroundColor3 = Color3.fromRGB(17, 17, 17)
				else
					tween_service:Create(button, info, {BackgroundColor3 = Color3.fromRGB(17, 17, 17)}):Play()
				end
			end)

			button.InputEnded:Connect(function()
				if not Configs.Executor.Animations then
					button.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
				else
					tween_service:Create(button, info, {BackgroundColor3 = Color3.fromRGB(0, 0, 0)}):Play()
				end
			end)

		end
	end
end

function hover_navi0()
	for i, button in pairs(navi:GetChildren()) do
		if button:IsA("ImageButton") then

			button.gradient.Rotation = 0

			if Configs.Executor.Animations then
				tween_service:Create(button.gradient, loop, {Rotation = 360}):Play()
			end

			button.MouseEnter:Connect(function()
				if allowed then
					button.gradient.Enabled = true

					if not Configs.Executor.Animations then
						button.icon.scale.Scale = 1.2
					else
						tween_service:Create(button.icon.scale, info, {Scale = 1.2}):Play()
					end
				end
			end)

			button.MouseButton1Down:Connect(function()
				if not Configs.Executor.Animations then
					button.icon.scale.Scale = .8
				else
					tween_service:Create(button.icon.scale, info, {Scale = .8}):Play()
				end
			end)

			button.InputEnded:Connect(function()
				button.gradient.Enabled = false

				if not Configs.Executor.Animations then
					button.icon.scale.Scale = 1
				else
					tween_service:Create(button.icon.scale, info, {Scale = 1}):Play()
				end
			end)
		end
	end
end

local success, result = pcall(function()
	hover1()
end)

if not success then
	module_announcement(string.format("An error occurred while running hover1", tostring(result)),10,"Error")
end
local success, result = pcall(function()
	hover_navi0()
end)

if not success then
	module_announcement(string.format("An error occurred while running hover_navi0", tostring(result)),10,"Error")
end

local function restate()
	scroll.system.build.value.Text = frame.Parent.Parent:GetAttribute("build")
	scroll.system.model.value.Text = "Essence " .. frame.Parent.Parent:GetAttribute("version")
end

--confirmation

frame.confirmation_prompt.prompt.confirm.MouseEnter:Connect(function()
	tween_service:Create(frame.confirmation_prompt.prompt.confirm, info, {BackgroundColor3 = Color3.fromRGB(112, 117, 120)}):Play()
end)

frame.confirmation_prompt.prompt.confirm.MouseButton1Down:Connect(function()
	tween_service:Create(frame.confirmation_prompt.prompt.confirm, info, {BackgroundColor3 = Color3.fromRGB(58, 61, 62)}):Play()
end)

frame.confirmation_prompt.prompt.confirm.InputEnded:Connect(function()
	tween_service:Create(frame.confirmation_prompt.prompt.confirm, info, {BackgroundColor3 = Color3.fromRGB(85, 89, 91)}):Play()
end)

