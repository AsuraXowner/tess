					tween_service:Create(Button2Auto, info, {Position = UDim2.new(0, 2,0.074, 0)}):Play()

					game:GetService("TweenService"):Create(Button1,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{BackgroundColor3=Color3.fromRGB(34, 34, 34)}):Play()
					game:GetService("TweenService"):Create(Button2Auto,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{ImageColor3=Color3.fromRGB(117, 117, 117)}):Play()
					game:GetService("TweenService"):Create(Stroke,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{Color = Color3.fromRGB(111, 111, 111)}):Play()
				end
			end)
			Button2Auto.MouseButton1Click:Connect(function()
				Configs.Scripts.AimbotToggle = true
				if not Configs.Executor.Animations then
					Button1Auto.Position = UDim2.new(0, 2,0.074, 0)
					Button2Auto.Position = UDim2.new(0, 2,0.889, 0)

					Button1.BackgroundColor3=Color3.fromRGB(99, 99, 99)
					Button1Auto.ImageColor3=Color3.fromRGB(255, 255, 255)
					Stroke.Color = Color3.fromRGB(111, 111, 111)
				else
					tween_service:Create(Button1Auto, info, {Position = UDim2.new(0, 2,0.074, 0)}):Play()
					tween_service:Create(Button2Auto, info, {Position = UDim2.new(0, 2,0.889, 0)}):Play()

					game:GetService("TweenService"):Create(Button1,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{BackgroundColor3=Color3.fromRGB(99, 99, 99)}):Play()
					game:GetService("TweenService"):Create(Button1Auto,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{ImageColor3=Color3.fromRGB(255, 255, 255)}):Play()
					game:GetService("TweenService"):Create(Stroke,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{Color = Color3.fromRGB(111, 111, 111)}):Play()
				end

				--

				if not (isStudio) and not isfile("EssenceSettings/Warning.rblx") then
					module_announcement("The Quick Panel may be detected in certain games. Use it carefully to avoid potential bans.",15,"Warning")
					writefile("EssenceSettings/Warning.rblx", "")
				end
			end)
			return
		end
		local SliderFrame = Instance.new("Frame")
		local ButtonText = Instance.new("TextLabel")
		local BarSlider = Instance.new("Frame")
		local UICorner = Instance.new("UICorner")
		local SliderButton = Instance.new("ImageButton")
		local UICorner_2 = Instance.new("UICorner")
		local BarSliderBackground = Instance.new("Frame")
		local UICorner_3 = Instance.new("UICorner")
		local SwitchFrame = Instance.new("Frame")
		local UICorner_4 = Instance.new("UICorner")
		local SlideNumber = Instance.new("TextBox")
		local SlideDownButton = Instance.new("ImageButton")
		local SlideUpButton = Instance.new("ImageButton")
		local UICorner_5 = Instance.new("UICorner")
		local SliderFrameStroke = Instance.new("UIStroke")
		local BarSliderStroke = Instance.new("UIStroke")
		local SwitchFrameStroke = Instance.new("UIStroke")
		local header = Instance.new("TextLabel")

		local humanoid = local_player.Character and local_player.Character:FindFirstChildOfClass("Humanoid")

		local Dragging = false
		local isFocused = false
		local MaxPercent = 100
		local CurrentPercent = nil
		local UpdateButtonValue = 0.01

		--Properties

		SliderFrame.Name = "SliderFrame"
		SliderFrame.Parent = navi6.Parent.scroll.build.QuickPanelBackground
		SliderFrame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		SliderFrame.BackgroundTransparency = 0.900
		SliderFrame.BorderColor3 = Color3.fromRGB(0, 0, 0)
		SliderFrame.BorderSizePixel = 0
		SliderFrame.ClipsDescendants = true
		SliderFrame.Size = UDim2.new(0, 306, 0, 48)

		ButtonText.Name = "ButtonText"
		ButtonText.Parent = SliderFrame
		ButtonText.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		ButtonText.BackgroundTransparency = 1.000
		ButtonText.BorderColor3 = Color3.fromRGB(0, 0, 0)
		ButtonText.BorderSizePixel = 0
		ButtonText.Position = UDim2.new(0.0315484926, 0, 0.0490468331, 0)
		ButtonText.Size = UDim2.new(0, 104, 0, 18)
		ButtonText.Font = Enum.Font.Arial
		if code == "AimbotSmoothing" then
			ButtonText.Text = Text
			else
			ButtonText.Text = "Character"..Text
		end
		ButtonText.TextColor3 = Color3.fromRGB(209, 209, 209)
		ButtonText.TextSize = 12.000
		ButtonText.TextXAlignment = Enum.TextXAlignment.Left

		BarSlider.Name = "BarSlider"
		BarSlider.Parent = SliderFrame
		BarSlider.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
		BarSlider.BorderColor3 = Color3.fromRGB(0, 0, 0)
		BarSlider.BorderSizePixel = 0
		BarSlider.Position = UDim2.new(0.0130000003, 0, 0.634000003, 0)
		BarSlider.Size = UDim2.new(0, 281, 0, 3)

		UICorner.CornerRadius = UDim.new(0, 4)
		UICorner.Parent = BarSlider

		SliderButton.Name = "SliderButton"
		SliderButton.Parent = BarSlider
		SliderButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		SliderButton.BorderColor3 = Color3.fromRGB(0, 0, 0)
		SliderButton.BorderSizePixel = 0
		SliderButton.Position = UDim2.new(1, 0, -1.66700006, 0)
		SliderButton.Size = UDim2.new(0, 12, 0, 12)
		SliderButton.Image = "rbxasset://textures/ui/GuiImagePlaceholder.png"

		UICorner_2.CornerRadius = UDim.new(0, 40)
		UICorner_2.Parent = SliderButton

		BarSliderBackground.Name = "BarSliderBackground"
		BarSliderBackground.Parent = SliderButton
		BarSliderBackground.BackgroundColor3 = Color3.fromRGB(61, 99, 234)
		BarSliderBackground.BorderColor3 = Color3.fromRGB(0, 0, 0)
		BarSliderBackground.BorderSizePixel = 0
		BarSliderBackground.Position = UDim2.new(-27.941, 0, 0.405000001, 0)
		BarSliderBackground.Size = UDim2.new(0, 335, 0, 3)

		UICorner_3.CornerRadius = UDim.new(0, 4)
		UICorner_3.Parent = BarSliderBackground

		SwitchFrame.Name = "SwitchFrame"
		SwitchFrame.Parent = SliderFrame
		SwitchFrame.BackgroundColor3 = Color3.fromRGB(44, 44, 44)
		SwitchFrame.BackgroundTransparency = 0.800
		SwitchFrame.BorderColor3 = Color3.fromRGB(255, 255, 255)
		SwitchFrame.BorderSizePixel = 0
		SwitchFrame.Position = UDim2.new(0.819999993, 0, 0, 3)
		SwitchFrame.Size = UDim2.new(0, 41, 0, 22)

		UICorner_4.CornerRadius = UDim.new(0, 4)
		UICorner_4.Parent = SwitchFrame

		SlideNumber.Name = "SlideNumber"
		SlideNumber.Parent = SwitchFrame
		SlideNumber.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		SlideNumber.BackgroundTransparency = 1.000
		SlideNumber.BorderColor3 = Color3.fromRGB(0, 0, 0)
		SlideNumber.BorderSizePixel = 0
		SlideNumber.Position = UDim2.new(0.378378391, 0, 0.028213501, 0)
		SlideNumber.Size = UDim2.new(0, 20, 0, 18)
		SlideNumber.ClearTextOnFocus = false
		SlideNumber.Font = Enum.Font.Oswald
		SlideNumber.Text = "0"
		SlideNumber.TextColor3 = Color3.fromRGB(255, 255, 255)
		SlideNumber.TextSize = 17.000
		SlideNumber.TextXAlignment = Enum.TextXAlignment.Right

		SlideDownButton.Name = "SlideDownButton"
		SlideDownButton.Parent = SwitchFrame
		SlideDownButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		SlideDownButton.BackgroundTransparency = 1.000
		SlideDownButton.BorderColor3 = Color3.fromRGB(0, 0, 0)
		SlideDownButton.BorderSizePixel = 0
		SlideDownButton.Position = UDim2.new(0.0979999974, 0, 0.521000028, 0)
		SlideDownButton.Size = UDim2.new(0, 8, 0, 9)
		SlideDownButton.Image = "rbxassetid://16884178261"
		SlideDownButton.ImageRectOffset = Vector2.new(266, 0)
		SlideDownButton.ImageRectSize = Vector2.new(36, 36)

		SlideUpButton.Name = "SlideUpButton"
		SlideUpButton.Parent = SwitchFrame
		SlideUpButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		SlideUpButton.BackgroundTransparency = 1.000
		SlideUpButton.BorderColor3 = Color3.fromRGB(0, 0, 0)
		SlideUpButton.BorderSizePixel = 0
		SlideUpButton.Position = UDim2.new(0.0979999974, 0, 0, 0)
		SlideUpButton.Size = UDim2.new(0, 8, 0, 9)
		SlideUpButton.Image = "rbxassetid://16884178261"
		SlideUpButton.ImageRectOffset = Vector2.new(304, 0)
		SlideUpButton.ImageRectSize = Vector2.new(36, 36)

		UICorner_5.CornerRadius = UDim.new(0, 4)
		UICorner_5.Parent = SliderFrame

		SliderFrameStroke.Name = "stroke"
		SliderFrameStroke.Color = Color3.fromRGB(59, 59, 59)
		SliderFrameStroke.Parent = SliderFrame
		SliderFrameStroke.ApplyStrokeMode = "Border"
		SliderFrameStroke.Thickness = 1.4
		SliderFrameStroke.Transparency = 0.2

		SwitchFrameStroke.Name = "stroke"
		SwitchFrameStroke.Color = Color3.fromRGB(81, 81, 81)
		SwitchFrameStroke.Parent = SwitchFrame
		SwitchFrameStroke.ApplyStrokeMode = "Border"
		SwitchFrameStroke.Thickness = 1.6
		SwitchFrameStroke.Transparency = 0.3

		BarSliderStroke.Name = "stroke"
		BarSliderStroke.Color = Color3.fromRGB(26, 19, 31)
		BarSliderStroke.Parent = BarSlider
		BarSliderStroke.ApplyStrokeMode = "Border"
		BarSliderStroke.Thickness = 1

		--scripts

		local speedErrorShown = false
		local jumpErrorShown = false

		if humanoid then
			if code == "Speed" then
				MaxPercent = 100
				if humanoid and humanoid:IsA("Humanoid") then
					CurrentPercent = humanoid.WalkSpeed / MaxPercent
				else
					CurrentPercent = 0
					if not speedErrorShown then
						module_announcement("Warning: Could not connect to Humanoid for Speed. Setting to 0.", 10, "Warning")
						speedErrorShown = true
					end
				end
				SliderFrame.Position = UDim2.new(0.015, 0, 0.076, 0)
				UpdateButtonValue = 0.01
			elseif code == "Jumppower" then
				MaxPercent = 100
				if humanoid and humanoid:IsA("Humanoid") then
					CurrentPercent = humanoid.JumpPower / MaxPercent
				else
					CurrentPercent = 0
					if not jumpErrorShown then
						module_announcement("Warning: Could not connect to Humanoid for Jumppower. Setting to 0.", 10, "Warning")
						jumpErrorShown = true
					end
				end
				SliderFrame.Position = UDim2.new(0.015, 0, 0.169, 0)
				UpdateButtonValue = 0.01
			elseif code == "Fly" then
				MaxPercent = 100
				CurrentPercent = 0 / MaxPercent
				SliderFrame.Position = UDim2.new(0.015, 0, 0.169, 0)
				UpdateButtonValue = 0.01
			elseif code == "AimbotSmoothing" then
				MaxPercent = 360
				CurrentPercent = 0 / MaxPercent
				SliderFrame.Position = UDim2.new(0.015, 0, 0.169, 0)
				UpdateButtonValue = 0.003
			end
		end

		local function UpdateSlider(percent)
			if percent==nil then return end
			percent = math.clamp(percent, 0, 1)
			CurrentPercent = percent
			SliderButton.Position = UDim2.new(CurrentPercent, 0, SliderButton.Position.Y.Scale, 0)

			local finalValue = math.floor(percent * MaxPercent)

			if humanoid then
				if not (isStudio) and not isfile("EssenceSettings/Warning.rblx") then
					module_announcement("The Quick Panel may be detected in certain games. Use it carefully to avoid potential bans.",15,"Warning")
					writefile("EssenceSettings/Warning.rblx", "")
				end
				if code == "Speed" then
					if humanoid and humanoid:IsA("Humanoid") then
						humanoid.WalkSpeed = finalValue
					else
						if not speedErrorShown then
							module_announcement("Warning: Could not connect to Humanoid for Speed. Setting to 0.", 10, "Warning")
							jumpErrorShown = true
						end
					end
				elseif code == "Jumppower" then
					if humanoid and humanoid:IsA("Humanoid") then
						humanoid.JumpPower = finalValue
					else
						if not jumpErrorShown then
							module_announcement("Warning: Could not connect to Humanoid for Jumppower. Setting to 0.", 10, "Warning")
							jumpErrorShown = true
						end
					end
				end
			end

			if not isFocused then
				SlideNumber.Text = tostring(finalValue)
			end
		end

		SliderButton.MouseButton1Down:Connect(function()
			Dragging = true
		end)

		UserInputService.InputEnded:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseButton1 then
				Dragging = false
			end
		end)

		UserInputService.InputChanged:Connect(function(input)
			if Dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
				local MousePos = UserInputService:GetMouseLocation()
				local RelPos = MousePos.X - BarSlider.AbsolutePosition.X
				local newPercent = math.clamp(RelPos / BarSlider.AbsoluteSize.X, 0, 1)
				UpdateSlider(newPercent)
			end
		end)

		SlideUpButton.MouseButton1Click:Connect(function()
			UpdateSlider(CurrentPercent + UpdateButtonValue)
		end)
		SlideDownButton.MouseButton1Click:Connect(function()
			UpdateSlider(CurrentPercent - UpdateButtonValue)
		end)

		SlideNumber.Focused:Connect(function()
			isFocused = true
			SlideNumber:CaptureFocus()
		end)

		SlideNumber.FocusLost:Connect(function(enterPressed)
			isFocused = false
			if enterPressed then
				local userInput = tonumber(SlideNumber.Text)
				if userInput then
					UpdateSlider(userInput / MaxPercent)
				else
					SlideNumber.Text = tostring(math.floor(CurrentPercent * MaxPercent))
				end
			end
		end)

		UpdateSlider(CurrentPercent)

	end
end

function module_menu(state)
	if state and not menu.Visible then
		menu.Visible = true
		menu.menu.Position = UDim2.new(0, -60, .5, 0)
		menu.menu.scale.Scale = .5
		if not Configs.Executor.Animations then

			menu.BackgroundTransparency = .5
			menu.menu.AnchorPoint = Vector2.new(.5, .5)
			menu.menu.Position = UDim2.new(0, 60, .5, 0)
			menu.menu.scale.Scale = 1
		else
			tween_service:Create(menu, info, {BackgroundTransparency = .5}):Play()
			tween_service:Create(menu.menu, info, {AnchorPoint = Vector2.new(.5, .5), Position = UDim2.new(0, 60, .5, 0)}):Play()
			tween_service:Create(menu.menu.scale, info, {Scale = 1}):Play()
		end
	else
		if not Configs.Executor.Animations then

			menu.BackgroundTransparency = 1
			menu.menu.AnchorPoint = Vector2.new(0, .5)
			menu.menu.Position = UDim2.new(0, -60, .5, 0)
			menu.menu.scale.Scale = .5
		else
			tween_service:Create(menu, info, {BackgroundTransparency = 1}):Play()
			tween_service:Create(menu.menu, info, {AnchorPoint = Vector2.new(0, .5), Position = UDim2.new(0, -60, .5, 0)}):Play()
			tween_service:Create(menu.menu.scale, info, {Scale = .5}):Play()
        end
		--

		task.wait(.5)

		--

		menu.Visible = false
	end
end

function module_Go_To(page:pages)
	Configs.Functions.ActiveFunctions += 1
	if page == "Players" then
		main_frame.ClipsDescendants = true

		main_frame.about_frame.Visible = false
		main_frame.Execution_frame.Visible = false
		main_frame.Settings_frame.Visible = false
		main_frame.QuickPanel_frame.Visible = false
		main_frame.dashboard_frame.Visible = false
		main_frame.main_frame.Visible = true

		--
		
		if not Configs.Executor.Animations then
			main_frame.about_frame.Position = UDim2.new(.5, 0, 1.2, 0)
			main_frame.Settings_frame.Position = UDim2.new(.5, 0, 1.2, 0)
			main_frame.Execution_frame.Position = UDim2.new(.5, 0, 1.2, 0)
			main_frame.dashboard_frame.Position = UDim2.new(.5, 0, 1.2, 0)
			main_frame.QuickPanel_frame.Position = UDim2.new(.5, 0, 1.2, 0)
			main_frame.main_frame.Position = UDim2.new(.5, 0, 1, 0)
			main_frame.about_frame.GroupTransparency = 1
			main_frame.Settings_frame.GroupTransparency = 1
			main_frame.Execution_frame.GroupTransparency = 1
			main_frame.dashboard_frame.GroupTransparency = 1
			main_frame.QuickPanel_frame.GroupTransparency = 1
			main_frame.main_frame.GroupTransparency = 0
		else
			tween_service:Create(main_frame.about_frame, slow, {Position = UDim2.new(.5, 0, 1.2, 0), GroupTransparency = 1}):Play()
			tween_service:Create(main_frame.Settings_frame, slow, {Position = UDim2.new(.5, 0, 1.2, 0), GroupTransparency = 1}):Play()
			tween_service:Create(main_frame.Execution_frame, slow, {Position = UDim2.new(.5, 0, 1.2, 0), GroupTransparency = 1}):Play()
			tween_service:Create(main_frame.dashboard_frame, slow, {Position = UDim2.new(.5, 0, 1.2, 0), GroupTransparency = 1}):Play()
			tween_service:Create(main_frame.QuickPanel_frame, slow, {Position = UDim2.new(.5, 0, 1.2, 0), GroupTransparency = 1}):Play()
			tween_service:Create(main_frame.main_frame, slow, {Position = UDim2.new(.5, 0, 1, 0), GroupTransparency = 0}):Play()
		end

		--

		main_frame.ClipsDescendants = false

		if not (isStudio) then
			writefile("EssenceSettings/Lastpage.lua", "Players")
		end
	elseif page == "Dashboard" then

		main_frame.ClipsDescendants = true

		main_frame.about_frame.Visible = false
		main_frame.Execution_frame.Visible = false
		main_frame.Settings_frame.Visible = false
		main_frame.QuickPanel_frame.Visible = false
		main_frame.dashboard_frame.Visible = true
		main_frame.main_frame.Visible = false

		--
		
		if not Configs.Executor.Animations then
			main_frame.about_frame.Position = UDim2.new(.5, 0, 1.2, 0)
			main_frame.Settings_frame.Position = UDim2.new(.5, 0, 1.2, 0)
			main_frame.Execution_frame.Position = UDim2.new(.5, 0, 1.2, 0)
			main_frame.dashboard_frame.Position = UDim2.new(.5, 0, 1, 0)
			main_frame.QuickPanel_frame.Position = UDim2.new(.5, 0, 1.2, 0)
			main_frame.main_frame.Position = UDim2.new(.5, 0, 1.2, 0)
			main_frame.about_frame.GroupTransparency = 1
			main_frame.Settings_frame.GroupTransparency = 1
			main_frame.Execution_frame.GroupTransparency = 1
			main_frame.dashboard_frame.GroupTransparency = 0
			main_frame.QuickPanel_frame.GroupTransparency = 1
			main_frame.main_frame.GroupTransparency = 1
		else
			tween_service:Create(main_frame.about_frame, slow, {Position = UDim2.new(.5, 0, 1.2, 0), GroupTransparency = 1}):Play()
			tween_service:Create(main_frame.Settings_frame, slow, {Position = UDim2.new(.5, 0, 1.2, 0), GroupTransparency = 1}):Play()
			tween_service:Create(main_frame.Execution_frame, slow, {Position = UDim2.new(.5, 0, 1.2, 0), GroupTransparency = 1}):Play()
			tween_service:Create(main_frame.dashboard_frame, slow, {Position = UDim2.new(.5, 0, 1, 0), GroupTransparency = 0}):Play()
			tween_service:Create(main_frame.QuickPanel_frame, slow, {Position = UDim2.new(.5, 0, 1.2, 0), GroupTransparency = 1}):Play()
			tween_service:Create(main_frame.main_frame, slow, {Position = UDim2.new(.5, 0, 1.2, 0), GroupTransparency = 1}):Play()
		end

		--

		main_frame.ClipsDescendants = false

		if not (isStudio) then
			writefile("EssenceSettings/Lastpage.lua", "Dashboard")
		end
	elseif page == "Credits" then
		main_frame.ClipsDescendants = true

		main_frame.about_frame.Visible = true
		main_frame.Execution_frame.Visible = false
		main_frame.Settings_frame.Visible = false
		main_frame.QuickPanel_frame.Visible = false
		main_frame.dashboard_frame.Visible = false
		main_frame.main_frame.Visible = false

		--// ACCESSIBILITY BUTTON RECENT

		options.recent.icon.Image = "rbxassetid://11419720347"
		options.recent.icon.corner.CornerRadius = UDim.new(0, 0)

		options.recent.func:SetAttribute("profile", false)
		options.recent.func:SetAttribute("dashboard", false)
		options.recent.func:SetAttribute("credits", true)
		options.recent.func:SetAttribute("execution", false)
		options.recent.func:SetAttribute("quickpanel", false)
		options.recent.func:SetAttribute("settings", false)

		--
		
		if not Configs.Executor.Animations then
			main_frame.about_frame.Position = UDim2.new(.5, 0, 1, 0)
			main_frame.Settings_frame.Position = UDim2.new(.5, 0, 1.2, 0)
			main_frame.Execution_frame.Position = UDim2.new(.5, 0, 1.2, 0)
			main_frame.dashboard_frame.Position = UDim2.new(.5, 0, 1.2, 0)
			main_frame.QuickPanel_frame.Position = UDim2.new(.5, 0, 1.2, 0)
			main_frame.main_frame.Position = UDim2.new(.5, 0, 1.2, 0)
			main_frame.about_frame.GroupTransparency = 0
			main_frame.Settings_frame.GroupTransparency = 1
			main_frame.Execution_frame.GroupTransparency = 1
			main_frame.dashboard_frame.GroupTransparency = 1
			main_frame.QuickPanel_frame.GroupTransparency = 1
			main_frame.main_frame.GroupTransparency = 1
		else
			tween_service:Create(main_frame.about_frame, slow, {Position = UDim2.new(.5, 0, 1, 0), GroupTransparency = 0}):Play()
			tween_service:Create(main_frame.Settings_frame, slow, {Position = UDim2.new(.5, 0, 1.2, 0), GroupTransparency = 1}):Play()
			tween_service:Create(main_frame.Execution_frame, slow, {Position = UDim2.new(.5, 0, 1.2, 0), GroupTransparency = 1}):Play()
			tween_service:Create(main_frame.dashboard_frame, slow, {Position = UDim2.new(.5, 0, 1.2, 0), GroupTransparency = 1}):Play()
			tween_service:Create(main_frame.QuickPanel_frame, slow, {Position = UDim2.new(.5, 0, 1.2, 0), GroupTransparency = 1}):Play()
			tween_service:Create(main_frame.main_frame, slow, {Position = UDim2.new(.5, 0, 1.2, 0), GroupTransparency = 1}):Play()
		end

		--

		main_frame.ClipsDescendants = false

		if not (isStudio) then
			writefile("EssenceSettings/Lastpage.lua", "Credits")
		end
	elseif page == "Execution" then
		main_frame.ClipsDescendants = true

		main_frame.about_frame.Visible = false
		main_frame.Execution_frame.Visible = true
		main_frame.Settings_frame.Visible = false
		main_frame.QuickPanel_frame.Visible = false
		main_frame.dashboard_frame.Visible = false
		main_frame.main_frame.Visible = false

		--// ACCESSIBILITY BUTTON RECENT

		options.recent.func:SetAttribute("profile", false)
		options.recent.func:SetAttribute("dashboard", false)
		options.recent.func:SetAttribute("credits", false)
		options.recent.func:SetAttribute("execution", true)
		options.recent.func:SetAttribute("quickpanel", false)
		options.recent.func:SetAttribute("settings", false)

		--
		
		if not Configs.Executor.Animations then
			main_frame.about_frame.Position = UDim2.new(.5, 0, 1.2, 0)
			main_frame.Settings_frame.Position = UDim2.new(.5, 0, 1.2, 0)
			main_frame.Execution_frame.Position = UDim2.new(.5, 0, 1, 0)
			main_frame.dashboard_frame.Position = UDim2.new(.5, 0, 1.2, 0)
			main_frame.QuickPanel_frame.Position = UDim2.new(.5, 0, 1.2, 0)
			main_frame.main_frame.Position = UDim2.new(.5, 0, 1.2, 0)
			main_frame.about_frame.GroupTransparency = 1
			main_frame.Settings_frame.GroupTransparency = 1
			main_frame.Execution_frame.GroupTransparency = 0
			main_frame.dashboard_frame.GroupTransparency = 1
			main_frame.QuickPanel_frame.GroupTransparency = 1
			main_frame.main_frame.GroupTransparency = 1
		else
			tween_service:Create(main_frame.about_frame, slow, {Position = UDim2.new(.5, 0, 1.2, 0), GroupTransparency = 1}):Play()
			tween_service:Create(main_frame.Settings_frame, slow, {Position = UDim2.new(.5, 0, 1.2, 0), GroupTransparency = 1}):Play()
			tween_service:Create(main_frame.Execution_frame, slow, {Position = UDim2.new(.5, 0, 1, 0), GroupTransparency = 0}):Play()
			tween_service:Create(main_frame.dashboard_frame, slow, {Position = UDim2.new(.5, 0, 1.2, 0), GroupTransparency = 1}):Play()
			tween_service:Create(main_frame.QuickPanel_frame, slow, {Position = UDim2.new(.5, 0, 1.2, 0), GroupTransparency = 1}):Play()
			tween_service:Create(main_frame.main_frame, slow, {Position = UDim2.new(.5, 0, 1.2, 0), GroupTransparency = 1}):Play()
		end

		--

		main_frame.ClipsDescendants = false

		if not (isStudio) then
			writefile("EssenceSettings/Lastpage.lua", "Execution")
		end
	elseif page == "ScriptHub" then
		main_frame.ClipsDescendants = true

		main_frame.ScriptHub.Visible = true

		--// ACCESSIBILITY BUTTON RECENT
		
		if not Configs.Executor.Animations then
			navi4.scale.Scale = 1
		else
			tween_service:Create(navi4.scale, info, {Scale = 1}):Play()
		end

		--

		main_frame.ClipsDescendants = false

		if not (isStudio) then
			writefile("EssenceSettings/Lastpage.lua", "ScriptHub")
		end
	elseif page == "Settings" then
		main_frame.ClipsDescendants = true

		main_frame.about_frame.Visible = false
		main_frame.Execution_frame.Visible = false
		main_frame.Settings_frame.Visible = true
		main_frame.QuickPanel_frame.Visible = false
		main_frame.dashboard_frame.Visible = false
		main_frame.main_frame.Visible = false

		--// ACCESSIBILITY BUTTON RECENT

		options.recent.func:SetAttribute("profile", false)
		options.recent.func:SetAttribute("dashboard", false)
		options.recent.func:SetAttribute("credits", false)
		options.recent.func:SetAttribute("quickpanel", false)
		options.recent.func:SetAttribute("execution", false)
		options.recent.func:SetAttribute("settings", true)

		--
		
		if not Configs.Executor.Animations then
			main_frame.about_frame.Position = UDim2.new(.5, 0, 1.2, 0)
			main_frame.Settings_frame.Position = UDim2.new(.5, 0, 1, 0)
			main_frame.Execution_frame.Position = UDim2.new(.5, 0, 1.2, 0)
			main_frame.dashboard_frame.Position = UDim2.new(.5, 0, 1.2, 0)
			main_frame.QuickPanel_frame.Position = UDim2.new(.5, 0, 1.2, 0)
			main_frame.main_frame.Position = UDim2.new(.5, 0, 1.2, 0)
			main_frame.about_frame.GroupTransparency = 1
			main_frame.Settings_frame.GroupTransparency = 0
			main_frame.Execution_frame.GroupTransparency = 1
			main_frame.dashboard_frame.GroupTransparency = 1
			main_frame.QuickPanel_frame.GroupTransparency = 1
			main_frame.main_frame.GroupTransparency = 1
		else
			tween_service:Create(main_frame.about_frame, slow, {Position = UDim2.new(.5, 0, 1.2, 0), GroupTransparency = 1}):Play()
			tween_service:Create(main_frame.Settings_frame, slow, {Position = UDim2.new(.5, 0, 1, 0), GroupTransparency = 0}):Play()
			tween_service:Create(main_frame.Execution_frame, slow, {Position = UDim2.new(.5, 0, 1.2, 0), GroupTransparency = 1}):Play()
			tween_service:Create(main_frame.dashboard_frame, slow, {Position = UDim2.new(.5, 0, 1.2, 0), GroupTransparency = 1}):Play()
			tween_service:Create(main_frame.QuickPanel_frame, slow, {Position = UDim2.new(.5, 0, 1.2, 0), GroupTransparency = 1}):Play()
			tween_service:Create(main_frame.main_frame, slow, {Position = UDim2.new(.5, 0, 1.2, 0), GroupTransparency = 1}):Play()
		end

		--

		main_frame.ClipsDescendants = false

		if not (isStudio) then
			writefile("EssenceSettings/Lastpage.lua", "Settings")
		end
	elseif page == "QuickPanel" then
		main_frame.ClipsDescendants = true

		main_frame.about_frame.Visible = false
		main_frame.Execution_frame.Visible = false
		main_frame.Settings_frame.Visible = false
		main_frame.QuickPanel_frame.Visible = true
		main_frame.dashboard_frame.Visible = false
		main_frame.main_frame.Visible = false

		--// ACCESSIBILITY BUTTON RECENT

		options.recent.func:SetAttribute("profile", false)
		options.recent.func:SetAttribute("dashboard", false)
		options.recent.func:SetAttribute("credits", false)
		options.recent.func:SetAttribute("quickpanel", true)
		options.recent.func:SetAttribute("execution", false)
		options.recent.func:SetAttribute("settings", false)

		--
		
		if not Configs.Executor.Animations then
			main_frame.about_frame.Position = UDim2.new(.5, 0, 1.2, 0)
			main_frame.Settings_frame.Position = UDim2.new(.5, 0, 1.2, 0)
			main_frame.Execution_frame.Position = UDim2.new(.5, 0, 1.2, 0)
			main_frame.dashboard_frame.Position = UDim2.new(.5, 0, 1.2, 0)
			main_frame.QuickPanel_frame.Position = UDim2.new(.5, 0, 1, 0)
			main_frame.main_frame.Position = UDim2.new(.5, 0, 1.2, 0)
			main_frame.about_frame.GroupTransparency = 1
			main_frame.Settings_frame.GroupTransparency = 1
			main_frame.Execution_frame.GroupTransparency = 1
			main_frame.dashboard_frame.GroupTransparency = 1
			main_frame.QuickPanel_frame.GroupTransparency = 0
			main_frame.main_frame.GroupTransparency = 1
		else
			tween_service:Create(main_frame.about_frame, slow, {Position = UDim2.new(.5, 0, 1.2, 0), GroupTransparency = 1}):Play()
			tween_service:Create(main_frame.Settings_frame, slow, {Position = UDim2.new(.5, 0, 1.2, 0), GroupTransparency = 1}):Play()
			tween_service:Create(main_frame.Execution_frame, slow, {Position = UDim2.new(.5, 0, 1.2, 0), GroupTransparency = 1}):Play()
			tween_service:Create(main_frame.dashboard_frame, slow, {Position = UDim2.new(.5, 0, 1.2, 0), GroupTransparency = 1}):Play()
			tween_service:Create(main_frame.QuickPanel_frame, slow, {Position = UDim2.new(.5, 0, 1, 0), GroupTransparency = 0}):Play()
			tween_service:Create(main_frame.main_frame, slow, {Position = UDim2.new(.5, 0, 1.2, 0), GroupTransparency = 1}):Play()
		end

		--

		main_frame.ClipsDescendants = false

		if not (isStudio) then
			writefile("EssenceSettings/Lastpage.lua", "QuickPanel")
		end
	end
	Configs.Functions.ActiveFunctions -= 1
end

function accessibility(state)
	if StorageOpened and Storagefullscreen then return end
	Configs.Functions.ActiveFunctions += 1
	if state then
		page.page:JumpTo(page.options)

		--
		
		if not Configs.Executor.Animations then
			button.Size = UDim2.fromOffset(270, 40)

			if not ToggleStandByQuickPanel then
				button.ImageTransparency = 0
			else
				button.BackgroundTransparency = 0
			end
			button.scale.Scale = 1.2

			task.spawn(function()
				page.options.players.icon.ImageTransparency = 1
				page.options.dashboard.icon.ImageTransparency = 1
				page.options.recent.icon.ImageTransparency = 1

				page.options.players.icon.scale.Scale = 0
				page.options.dashboard.icon.scale.Scale = 0
				page.options.recent.icon.scale.Scale = 0

				--
				task.wait()
				--

				page.options.players.icon.ImageTransparency = 0
				page.options.players.icon.scale.Scale = 1

				--
				task.wait(.1)
				--

				page.options.dashboard.icon.ImageTransparency = 0
				page.options.dashboard.icon.scale.Scale = 1

				--
				task.wait(.1)
				--

				page.options.recent.icon.ImageTransparency = 0
				page.options.recent.icon.scale.Scale = 1

			end)
		else
			tween_service:Create(button, info, {Size = UDim2.fromOffset(270, 40)}):Play()

			if not ToggleStandByQuickPanel then
				tween_service:Create(button, quick, {ImageTransparency = 0}):Play()
			else
				tween_service:Create(button, quick, {BackgroundTransparency = 0}):Play()
			end
			tween_service:Create(button.scale, info, {Scale = 1.2}):Play()

			task.spawn(function()
				tween_service:Create(page.options.players.icon, inst, {ImageTransparency = 1}):Play()
				tween_service:Create(page.options.dashboard.icon, inst, {ImageTransparency = 1}):Play()
				tween_service:Create(page.options.recent.icon, inst, {ImageTransparency = 1}):Play()

				tween_service:Create(page.options.players.icon.scale, inst, {Scale = 0}):Play()
				tween_service:Create(page.options.dashboard.icon.scale, inst, {Scale = 0}):Play()
				tween_service:Create(page.options.recent.icon.scale, inst, {Scale = 0}):Play()

				--
				task.wait()
				--

				tween_service:Create(page.options.players.icon, quick, {ImageTransparency = 0}):Play()
				tween_service:Create(page.options.players.icon.scale, quick, {Scale = 1}):Play()

				--
				task.wait(.1)
				--

				tween_service:Create(page.options.dashboard.icon, quick, {ImageTransparency = 0}):Play()
				tween_service:Create(page.options.dashboard.icon.scale, quick, {Scale = 1}):Play()

				--
				task.wait(.1)
				--

				tween_service:Create(page.options.recent.icon, quick, {ImageTransparency = 0}):Play()
				tween_service:Create(page.options.recent.icon.scale, quick, {Scale = 1}):Play()

			end)
		end
	else
		page.page:JumpTo(page.front)

		--
		
		if not Configs.Executor.Animations then
			button.Size = UDim2.fromOffset(190, 40)
		else
			tween_service:Create(button, info, {Size = UDim2.fromOffset(190, 40)}):Play()
		end
	end
	Configs.Functions.ActiveFunctions -= 1
end

function module_Prompt(PromptName,PromptDesc,cancel,confirm,open)
	if open then
		coroutine.wrap(function()
			local Sound = Instance.new("Sound")
			Sound.Parent = MainFrame.frame
			Sound.SoundId="rbxassetid://18886652611"
			Sound:Play()
			task.wait(3)
			MainFrame.frame:FindFirstChildOfClass("Sound"):Destroy()
		end)()
		if not Configs.Executor.Animations then
		    main_frame.confirmation_prompt.ImageTransparency = 0.5
		else
			tween_service:Create(main_frame.confirmation_prompt, info, {ImageTransparency = 0.5}):Play()
		end
		main_frame.confirmation_prompt.prompt.description.Text = PromptDesc
		main_frame.confirmation_prompt.prompt.header.Text = PromptName
		main_frame.confirmation_prompt.prompt.confirm.Text = confirm.." (Y)"
		main_frame.confirmation_prompt.prompt.cancel.Text = cancel.." (U)"

		main_frame.confirmation_prompt.Visible = true


		--// ACCESSIBILITY BUTTON RECENT
		if not Configs.Executor.Animations then
			main_frame.confirmation_prompt.prompt.scale.Scale = 1
		else
			tween_service:Create(main_frame.confirmation_prompt.prompt.scale, info, {Scale = 1}):Play()
		end
	elseif not open then
		if not Configs.Executor.Animations then
			main_frame.confirmation_prompt.prompt.scale.Scale = 0
		else
			tween_service:Create(main_frame.confirmation_prompt.prompt.scale, info, {Scale = 0}):Play()
		end
		task.wait(1)
		if not Configs.Executor.Animations then
			main_frame.confirmation_prompt.ImageTransparency = 1
		else
			tween_service:Create(main_frame.confirmation_prompt, info, {ImageTransparency = 1}):Play()
		end
		main_frame.confirmation_prompt.prompt.description.Text = ""
		main_frame.confirmation_prompt.prompt.header.Text = ""
		main_frame.confirmation_prompt.prompt.confirm.Text = ""
		main_frame.confirmation_prompt.prompt.cancel.Text = ""
		main_frame.confirmation_prompt.Visible = false
	end
end
function RunSaveInstance()
	module_announcement("Saving Place | Scriptsaving disabled",5,"Normal")
	local Params = {
		RepoURL = "https://raw.githubusercontent.com/luau/SynSaveInstance/main/",
		SSI = "saveinstance",
	}
	local synsaveinstance = loadstring(game:HttpGet(Params.RepoURL .. Params.SSI .. ".luau", false), Params.SSI)()
	local Options = {noscripts = true , scriptcache = false , SafeMode = false, timeout = 18, SaveBytecode = false , ShowStatus = true , __DEBUG_MODE = true} -- Documentation here https://luau.github.io/UniversalSynSaveInstance/api/SynSaveInstance
	synsaveinstance(Options)
end
