	module_Go_To("Dashboard")
end)

navi6.menu.MouseButton1Click:Connect(function()
	module_menu(true)
end)

navi6.close.MouseButton1Click:Connect(function()
	module_Go_To("Dashboard")
end)

navi2.refresh.MouseButton1Click:Connect(function()
	if navi2.refresh.icon.Rotation == 0 then
		tween_service:Create(navi2.refresh.icon, slow, {Rotation = 360}):Play()

		initialize()

		--

		task.wait(5)

		--

		navi2.refresh.icon.Rotation = 0
	end
end)

--// NAVIGATION HOVERS

function hover()
	for i, button in pairs(navi2:GetChildren()) do
		if button:IsA("ImageButton") then

			button.gradient.Rotation = 0

			if Configs.Executor.Animations then
				tween_service:Create(button.gradient, loop, {Rotation = 360}):Play()
			end

			button.MouseEnter:Connect(function()
				if allowed then
					button.gradient.Enabled = true
					
					if not Configs.Executor.Animations then
						button.icon.scale.Scale = 1.2
					else
						tween_service:Create(button.icon.scale, info, {Scale = 1.2}):Play()
					end
				end
			end)

			button.MouseButton1Down:Connect(function()
				if not Configs.Executor.Animations then
					button.icon.scale.Scale = .8
				else
					tween_service:Create(button.icon.scale, info, {Scale = .8}):Play()
				end
			end)

			button.InputEnded:Connect(function()
				button.gradient.Enabled = false

				if not Configs.Executor.Animations then
					button.icon.scale.Scale = 1
				else
					tween_service:Create(button.icon.scale, info, {Scale = 1}):Play()
				end
			end)
		end
	end
end

local success, result = pcall(function()
	hover()
end)

if not success then
	module_announcement(string.format("An error occurred while running hover", tostring(result)),10,"Error")
end

--name

local insertedLines = {}
local MAX_LINES = 100
local OutputConsole = false--WARNING REALLY PERFONMANCE issue

local function storeOriginalText(element)
	Configs.nameGeneration.originalTextValues[element] = element.Text
end

local function undoAnonymousChanges()
	for element, originalText in pairs(Configs.nameGeneration.originalTextValues) do
		element.Text = originalText
	end
end
runService.Heartbeat:Connect(function(frame)
	if Configs.Scripts.NoclipToggle and local_player.Character or local_player.CharacterAdded:Wait() then
		for _, part in pairs(local_player.Character:GetChildren()) do
			if part:IsA("BasePart") then
				part.CanCollide = false
			end
		end
	end
	if not (isStudio) and Configs.Functions.CustomLocalName then
		for _, text in ipairs(Configs.nameGeneration.cachedText) do
			local lowerText = string.lower(text.Text)
			if string.find(lowerText, lowerName, 1, true) or string.find(lowerText, lowerDisplayName, 1, true) then
				storeOriginalText(text)

				local newText = string.gsub(string.gsub(lowerText, lowerName, randomUsername), lowerDisplayName, randomUsername)
				text.Text = string.gsub(newText, "^%l", string.upper)
			end
		end
	else
		undoAnonymousChanges()
	end
	if (isStudio) then return end--uh what? this is way too powerful isnt?
	local devConsoleMaster = game.CoreGui:FindFirstChild("DevConsoleMaster")
	if devConsoleMaster then
		local devConsoleWindow = devConsoleMaster:FindFirstChild("DevConsoleWindow")
		if devConsoleWindow then
			local devConsoleUI = devConsoleWindow:FindFirstChild("DevConsoleUI")
			if devConsoleUI then
				local mainView = devConsoleUI:FindFirstChild("MainView")
				if mainView then
					local clientLog = mainView:FindFirstChild("ClientLog")
					if clientLog then
						if OutputConsole then
							clientLog.DescendantAdded:Connect(function(instance)
								if instance:IsA("TextLabel") then
									local text = instance.Text
									if not insertedLines[text] then
										insertedLines[text] = true

										local TextOutput = instance:Clone()
										TextOutput.Parent = Essence_Instances.FrameOutput

										local children = Essence_Instances.FrameOutput:GetChildren()
										local labelCount = 0
										for _, child in ipairs(children) do
											if child:IsA("TextLabel") then
												labelCount += 1
											end
										end
										if labelCount > MAX_LINES then
											for _, child in ipairs(children) do
												if child:IsA("TextLabel") then
													child:Destroy()
													break
												end
											end
										end
									end
								end
							end)
						end
					end
				end
			end
		end
	end
	--task.wait(1)
end)

for _, instance in next, game:GetDescendants() do
	if instance:IsA("TextLabel") or instance:IsA("TextButton") then
		if not table.find(Configs.nameGeneration.cachedText, instance) then
			table.insert(Configs.nameGeneration.cachedText, instance)
		end
	end
end

game.DescendantAdded:Connect(function(instance)
	if instance:IsA("TextLabel") or instance:IsA("TextButton") then
		if not table.find(Configs.nameGeneration.cachedText, instance) then
			table.insert(Configs.nameGeneration.cachedText, instance)
		end
	end
end)

