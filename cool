if (isStudio) then
	Configs.Executor.Mode="Studio"
end

local function DrawingCheck(module)
	if Drawing == nil then
		Configs.Functions.DrawingSupported = false
		return false
	else
		Configs.Functions.DrawingSupported = true
		return true
	end
end

DrawingCheck()

-- Module Scripts

function module_panel(state)
	if state then
		if not frame.Visible then
			if not Configs.Executor.Animations then
				StorageOpened = true

				frame.Visible = true
				frame.BackgroundTransparency = 1
				frame.ImageTransparency = 1

				frame.scale.Scale = 1.2

				frame.dashboard_frame.GroupTransparency = 1
				frame.main_frame.GroupTransparency = 1
				frame.window_controls.GroupTransparency = 1

				if Storagefullscreen then
					button.ImageTransparency = 1
					button.BackgroundTransparency = 1
					button.page.front.open.Icon.TextTransparency = 1
					button.page.options.dashboard.icon.ImageTransparency = 1
					button.page.options.dashboard.icon.ImageTransparency = 1
					button.page.options.dashboard.icon.ImageTransparency = 1
					MainFrame.accessibility_button.Interactable = false
				end

				--load stuff
				if Method=="Game" or Method=="Menu" then
					navi.Parent.scroll.resources.item.value.Text = Method
				end
				if Method=="Player" or Method=="Bloxstrap" then
					navi.Parent.scroll.system.model.value.Text = Method
				end
				if (isStudio) then
					navi.Parent.scroll.resources.item.value.Text = "Unknown"
					navi.Parent.scroll.system.model.value.Text = "Studio"
				end
				frame.BackgroundTransparency = .1
				frame.ImageTransparency = .8

				frame.dashboard_frame.GroupTransparency = 0
				frame.main_frame.GroupTransparency = 0
				frame.about_frame.GroupTransparency = 0
				frame.Execution_frame.GroupTransparency = 0
				frame.Settings_frame.GroupTransparency = 0
				frame.QuickPanel_frame.GroupTransparency = 0

				frame.window_controls.GroupTransparency = 0

				blur.Size = 40

				if Storagefullscreen then
					frame.scale.Scale = 1
				else
					frame.shadow.ImageTransparency = 0
					frame.scale.Scale = Storagescale_on_window
				end
			else
				StorageOpened = true

				frame.Visible = true
				frame.BackgroundTransparency = 1
				frame.ImageTransparency = 1

				frame.scale.Scale = 1.2

				frame.dashboard_frame.GroupTransparency = 1
				frame.main_frame.GroupTransparency = 1
				frame.window_controls.GroupTransparency = 1

				if Storagefullscreen then
					tween_service:Create(button, quick, {ImageTransparency = 1}):Play()
					tween_service:Create(button, quick, {BackgroundTransparency = 1}):Play()
					tween_service:Create(button.page.front.open.Icon, quick, {TextTransparency = 1}):Play()
					tween_service:Create(button.page.options.dashboard.icon, quick, {ImageTransparency = 1}):Play()
					tween_service:Create(button.page.options.dashboard.icon, quick, {ImageTransparency = 1}):Play()
					tween_service:Create(button.page.options.dashboard.icon, quick, {ImageTransparency = 1}):Play()
					MainFrame.accessibility_button.Interactable = false
				end

				--load stuff
				if Method=="Game" or Method=="Menu" then
					navi.Parent.scroll.resources.item.value.Text = Method
				end
				if Method=="Player" or Method=="Bloxstrap" then
					navi.Parent.scroll.system.model.value.Text = Method
				end
				if (isStudio) then
					navi.Parent.scroll.resources.item.value.Text = "Unknown"
					navi.Parent.scroll.system.model.value.Text = "Studio"
				end
				tween_service:Create(frame, info, {BackgroundTransparency = .1, ImageTransparency = .8}):Play()

				tween_service:Create(frame.dashboard_frame, info, {GroupTransparency = 0}):Play()
				tween_service:Create(frame.main_frame, info, {GroupTransparency = 0}):Play()
				tween_service:Create(frame.about_frame, info, {GroupTransparency = 0}):Play()
				tween_service:Create(frame.Execution_frame, info, {GroupTransparency = 0}):Play()
				tween_service:Create(frame.Settings_frame, info, {GroupTransparency = 0}):Play()
				tween_service:Create(frame.QuickPanel_frame, info, {GroupTransparency = 0}):Play()

				tween_service:Create(frame.window_controls, info, {GroupTransparency = 0}):Play()

				tween_service:Create(blur, info, {Size = 40}):Play()

				if Storagefullscreen then
					tween_service:Create(frame.scale, info, {Scale = 1}):Play()
				else
					tween_service:Create(frame.shadow, info, {ImageTransparency = 0}):Play()
					tween_service:Create(frame.scale, info, {Scale = Storagescale_on_window}):Play()
				end
			end
		end
	else
		if not Configs.Executor.Animations then
			StorageOpened = false

			frame.BackgroundTransparency = 1
			frame.ImageTransparency = 1

			frame.dashboard_frame.GroupTransparency = 1
			frame.main_frame.GroupTransparency = 1
			frame.about_frame.GroupTransparency = 1
			frame.Execution_frame.GroupTransparency = 1
			frame.Settings_frame.GroupTransparency = 1
			frame.QuickPanel_frame.GroupTransparency = 1

			frame.shadow.ImageTransparency = 1

			frame.window_controls.GroupTransparency = 1

			blur.Size = 0

			frame.scale.Scale = 1.2

			if not ToggleStandByQuickPanel then
				button.ImageTransparency = 0.8
			else
				button.BackgroundTransparency = 0.8
			end
			button.page.front.open.Icon.TextTransparency = 0
			button.page.options.dashboard.icon.ImageTransparency = 0
			button.page.options.dashboard.icon.ImageTransparency = 0
			button.page.options.dashboard.icon.ImageTransparency = 0
		else
			StorageOpened = false

			tween_service:Create(frame, info, {BackgroundTransparency = 1, ImageTransparency = 1}):Play()

			tween_service:Create(frame.dashboard_frame, info, {GroupTransparency = 1}):Play()
			tween_service:Create(frame.main_frame, info, {GroupTransparency = 1}):Play()
			tween_service:Create(frame.about_frame, info, {GroupTransparency = 1}):Play()
			tween_service:Create(frame.Execution_frame, info, {GroupTransparency = 1}):Play()
			tween_service:Create(frame.Settings_frame, info, {GroupTransparency = 1}):Play()
			tween_service:Create(frame.QuickPanel_frame, info, {GroupTransparency = 1}):Play()

			tween_service:Create(frame.shadow, info, {ImageTransparency = 1}):Play()

			tween_service:Create(frame.window_controls, info, {GroupTransparency = 1}):Play()

			tween_service:Create(blur, info, {Size = 0}):Play()

			tween_service:Create(frame.scale, info, {Scale = 1.2}):Play()

			if not ToggleStandByQuickPanel then
				tween_service:Create(button, quick, {ImageTransparency = 0.8}):Play()
			else
				tween_service:Create(button, quick, {BackgroundTransparency = 0.8}):Play()
			end
			tween_service:Create(button.page.front.open.Icon, quick, {TextTransparency = 0}):Play()
			tween_service:Create(button.page.options.dashboard.icon, quick, {ImageTransparency = 0}):Play()
			tween_service:Create(button.page.options.dashboard.icon, quick, {ImageTransparency = 0}):Play()
			tween_service:Create(button.page.options.dashboard.icon, quick, {ImageTransparency = 0}):Play()
		end
		MainFrame.accessibility_button.Interactable = true

		--

		task.wait(.5)

		--

		frame.Visible = false
	end
end

function module_Files(state,Text)
	local function fullscreen(state, transform, delete_cache)
		coroutine.wrap(function()
			if state then
				Storagefullscreen = true
				blur.Enabled = true

				controls.restore.icon.Image = "rbxassetid://11422140434"

				controls.resize.Visible = false
				controls.resize_disabled.Visible = true
				
				if not Configs.Executor.Animations then
					if not ToggleStandByQuickPanel then
						button.ImageTransparency = 0.8
					else
						button.BackgroundTransparency = 0.8
					end
					button.page.front.open.Icon.TextTransparency = 1
					button.page.options.dashboard.icon.ImageTransparency = 1
					button.page.options.dashboard.icon.ImageTransparency = 1
					button.page.options.dashboard.icon.ImageTransparency = 1
					MainFrame.accessibility_button.Interactable = false
					if Essence_Instances.Folder0:FindFirstChildOfClass("UIListLayout") then
						Essence_Instances.Folder0:FindFirstChildOfClass("UIListLayout"):Destroy()
					end
					if navi6.Parent.scroll.build:FindFirstChildOfClass("TextLabel") then
						navi6.Parent.scroll.build.header.TextTransparency = 0
					end
					main_frame.corner.CornerRadius = UDim.new(0, 0)
					main_frame.menu_frame.corner.CornerRadius = UDim.new(0, 0)
					main_frame.window_controls.corner.CornerRadius = UDim.new(0, 0)

					main_frame.shadow.Visible = false
					--

					if not delete_cache then
						last_pos = main_frame.Position
						last_size = main_frame.Size
					end

					if transform then
						main_frame.Position = UDim2.fromScale(.5, .5)
						main_frame.Size = UDim2.fromScale(1, 1)
					end
				else
				if not ToggleStandByQuickPanel then
					tween_service:Create(button, quick, {ImageTransparency = 0.8}):Play()
				else
					tween_service:Create(button, quick, {BackgroundTransparency = 0.8}):Play()
				end
				tween_service:Create(button.page.front.open.Icon, quick, {TextTransparency = 1}):Play()
				tween_service:Create(button.page.options.dashboard.icon, quick, {ImageTransparency = 1}):Play()
				tween_service:Create(button.page.options.dashboard.icon, quick, {ImageTransparency = 1}):Play()
				tween_service:Create(button.page.options.dashboard.icon, quick, {ImageTransparency = 1}):Play()
				MainFrame.accessibility_button.Interactable = false
				if Essence_Instances.Folder0:FindFirstChildOfClass("UIListLayout") then
					Essence_Instances.Folder0:FindFirstChildOfClass("UIListLayout"):Destroy()
				end
				if navi6.Parent.scroll.build:FindFirstChildOfClass("TextLabel") then
					tween_service:Create(navi6.Parent.scroll.build.header, quick, {TextTransparency = 0}):Play()
				end
				main_frame.corner.CornerRadius = UDim.new(0, 0)
				main_frame.menu_frame.corner.CornerRadius = UDim.new(0, 0)
				main_frame.window_controls.corner.CornerRadius = UDim.new(0, 0)

				main_frame.shadow.Visible = false
				--

				if not delete_cache then
					last_pos = main_frame.Position
					last_size = main_frame.Size
				end

				if transform then
					tween_service:Create(main_frame, info, {Position = UDim2.fromScale(.5, .5)}):Play()
					tween_service:Create(main_frame, smooth, {Size = UDim2.fromScale(1, 1)}):Play()
					end
				end
			else
				Storagefullscreen = false
				blur.Enabled = false

				controls.restore.icon.Image = "rbxassetid://11295287158"

				controls.resize.Visible = true
				controls.resize_disabled.Visible = false

				--

				main_frame.corner.CornerRadius = UDim.new(0, 20)
				main_frame.menu_frame.corner.CornerRadius = UDim.new(0, 20)
				main_frame.window_controls.corner.CornerRadius = UDim.new(0, 20)

				main_frame.shadow.Visible = true
				if not Configs.Executor.Animations then
					if not ToggleStandByQuickPanel then
						button.ImageTransparency = 0.8
					else
						button.BackgroundTransparency = 0.8
					end
					button.page.front.open.Icon.TextTransparency = 0
					button.page.options.dashboard.icon.ImageTransparency = 0
					button.page.options.dashboard.icon.ImageTransparency = 0
					button.page.options.dashboard.icon.ImageTransparency = 0
					MainFrame.accessibility_button.Interactable = true
					if Essence_Instances.list30:FindFirstChildOfClass("UIListLayout") then
						if Essence_Instances.Folder0:FindFirstChildOfClass("UIListLayout") then
						else
							local Template = Essence_Instances.list30:FindFirstChildOfClass("UIListLayout"):Clone()

							Template.Parent = Essence_Instances.Folder0
						end
					end
					if navi6.Parent.scroll.build:FindFirstChildOfClass("TextLabel") then
						navi6.Parent.scroll.build.header.TextTransparency = 1
					end
						
					if transform then
						main_frame.Position = last_pos
						main_frame.Size = last_size
					end
				else
				if not ToggleStandByQuickPanel then
					tween_service:Create(button, quick, {ImageTransparency = 0.8}):Play()
				else
					tween_service:Create(button, quick, {BackgroundTransparency = 0.8}):Play()
				end
				tween_service:Create(button.page.front.open.Icon, quick, {TextTransparency = 0}):Play()
				tween_service:Create(button.page.options.dashboard.icon, quick, {ImageTransparency = 0}):Play()
				tween_service:Create(button.page.options.dashboard.icon, quick, {ImageTransparency = 0}):Play()
				tween_service:Create(button.page.options.dashboard.icon, quick, {ImageTransparency = 0}):Play()
				MainFrame.accessibility_button.Interactable = true
				if Essence_Instances.list30:FindFirstChildOfClass("UIListLayout") then
					if Essence_Instances.Folder0:FindFirstChildOfClass("UIListLayout") then
					else
						local Template = Essence_Instances.list30:FindFirstChildOfClass("UIListLayout"):Clone()

						Template.Parent = Essence_Instances.Folder0
					end
				end

				if navi6.Parent.scroll.build:FindFirstChildOfClass("TextLabel") then
					tween_service:Create(navi6.Parent.scroll.build.header, quick, {TextTransparency = 1}):Play()
				end

				--

				if transform then
					tween_service:Create(main_frame, smooth, {Position = last_pos}):Play()
					tween_service:Create(main_frame, info, {Size = last_size}):Play()
				end
				end
			end
		end)()
	end
	if not (isStudio) and writefile and isfile and makefolder and readfile and listfiles then
		if not state then
			if not isfolder("EssenceSettings") then
				makefolder("EssenceSettings")
			end

			if not isfolder("EssenceSettings/SettingsConfig") then
				makefolder("EssenceSettings/SettingsConfig")
			end
			if not isfolder("EssenceSettings/ExecutionConfig") then
				makefolder("EssenceSettings/ExecutionConfig")
			end

			if not isfolder("EssenceSettings/InstancesClient") then
				makefolder("EssenceSettings/InstancesClient")
			end
			if not isfolder("EssenceSettings/AutoExec") then
				makefolder("EssenceSettings/AutoExec")
			end

			--
			local editorValues = {}--i just wonder sometimes how vape script made saving profiles :skull:
			table.insert(editorValues, navi3.Editor.Text)
			for i = 2, 10 do
				local editor = navi3:FindFirstChild("Editor" .. i)
				if editor then
					table.insert(editorValues, editor.Text)
				else
					table.insert(editorValues, "nil")
				end
			end

			local LayoutValues = {}
			table.insert(LayoutValues, navi1.Parent.scroll.main_tools.features.ExecutionPanelButton.LayoutOrder)
			table.insert(LayoutValues, navi1.Parent.scroll.main_tools.features.ScriptHubButton.LayoutOrder)
			table.insert(LayoutValues, navi1.Parent.scroll.main_tools.features.QuickPanelButton.LayoutOrder)
			table.insert(LayoutValues, navi1.Parent.scroll.additional_features.features.CloudStorageButton.LayoutOrder)
			table.insert(LayoutValues, navi1.Parent.scroll.additional_features.features.CustomizationButton.LayoutOrder)
			table.insert(LayoutValues, navi1.Parent.scroll.additional_features.features.SettingsButton.LayoutOrder)

			local dragValues = {}
			table.insert(dragValues, MainFrame.accessibility_button.Position.X.Offset)
			table.insert(dragValues, MainFrame.accessibility_button.Position.Y.Offset)
			table.insert(dragValues, MainFrame.accessibility_button.Rotation)
			table.insert(dragValues, MainFrame.accessibility_button.button.page.front.open.Icon.Rotation)
			table.insert(dragValues, ToggleStandByQuickPanel)

			local settingsData = {
				customLocalName = tostring(Configs.Functions.CustomLocalName),
				autoInject = tostring(Configs.Executor.ExecutorSettings.AutoInject),
				autoExec = tostring(Configs.Executor.ExecutorSettings.AutoExec),
				autoUpdater = tostring(Configs.Executor.ExecutorSettings.AutoUpdater),
				modeDrag = ModeDrag,
				storageFullscreen = tostring(Storagefullscreen),
				KeybindMenu = tostring(Configs.Executor.KeybindMenu),
				Animations = Configs.Executor.Animations,
				MultiInstance = Configs.Executor.MultiInstance,
				editors = editorValues,
				layouts = LayoutValues,
				dragtopgui = dragValues,
				MainFrame_X = main_frame.Size.X.Offset,
				MainFrame_Y = main_frame.Size.Y.Offset,
                MainFrame_S = main_frame.Parent.scale.Scale,
				MainFrameVisible = tostring(main_frame.Visible),
			}

			local jsonData = HttpService:JSONEncode(settingsData)

			writefile("EssenceSettings/SettingsConfig/SettingsExecutor.lua", jsonData)
		elseif state then
			if isfile("EssenceSettings/SettingsConfig/SettingsExecutor.lua") then--sigmaboy
				local jsonData = readfile("EssenceSettings/SettingsConfig/SettingsExecutor.lua")
				local settingsData = HttpService:JSONDecode(jsonData)

				if settingsData then
					local Check = nil
					Configs.Functions.CustomLocalName = (settingsData.customLocalName == "true")
					Configs.Executor.ExecutorSettings.AutoInject = (settingsData.autoInject == "true")
					Configs.Executor.ExecutorSettings.AutoExec = (settingsData.autoExec == "true")
					Configs.Executor.ExecutorSettings.AutoUpdater = (settingsData.autoUpdater == "true")
					Configs.Executor.ExecutorSettings.MultiInstance = (settingsData.MultiInstance == "true")
					Configs.Executor.ExecutorSettings.Animations = (settingsData.Animations == "true")
					Configs.Executor.ExecutorSettings.KeybindMenu = (settingsData.KeybindMenu)
					ModeDrag = tonumber(settingsData.modeDrag) or 0
                    main_frame.Parent.scale.Scale = tonumber(settingsData.MainFrame_S) or 1
					Check = (settingsData.storageFullscreen)
					if settingsData.MainFrame_X > 549 and settingsData.MainFrame_Y > 349 then
						min_size = Vector2.new(settingsData.MainFrame_X, settingsData.MainFrame_Y)
						last_size = UDim2.fromOffset(settingsData.MainFrame_X, settingsData.MainFrame_Y)
					end

					local editors = settingsData.editors or {}
					local layouts = settingsData.layouts or {}
					local dragui = settingsData.dragtopgui or {}
					if navi3:FindFirstChild("Editor") then
						navi3.Editor.Text = editors[1] or ""
					end
					for i = 2, 10 do
						local ed = navi3:FindFirstChild("Editor" .. i)
						if ed then
							ed.Text = editors[i] or ""
						end
					end

					if settingsData.MainFrameVisible == "true" then
						module_panel(not main_frame.Visible)
					end

					navi1.Parent.scroll.main_tools.features.ExecutionPanelButton.LayoutOrder = layouts[1] or 0
					navi1.Parent.scroll.main_tools.features.ScriptHubButton.LayoutOrder = layouts[2] or 0
					navi1.Parent.scroll.main_tools.features.QuickPanelButton.LayoutOrder = layouts[3] or 0
					navi1.Parent.scroll.additional_features.features.CloudStorageButton.LayoutOrder = layouts[4] or 0
					navi1.Parent.scroll.additional_features.features.CustomizationButton.LayoutOrder = layouts[5] or 0
					navi1.Parent.scroll.additional_features.features.SettingsButton.LayoutOrder = layouts[6] or 0
					MainFrame.accessibility_button.Position = UDim2.new(0, dragui[1], 0, dragui[2])
					MainFrame.accessibility_button.Rotation = dragui[3]
					MainFrame.accessibility_button.button.page.front.open.Icon.Rotation = dragui[4]

					ToggleStandByQuickPanel=dragui[5]
					if not ToggleStandByQuickPanel then
						tween_service:Create(MainFrame.accessibility_button.button, TweenInfo.new(0.3, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {ImageTransparency = 0.8}):Play()
						tween_service:Create(MainFrame.accessibility_button.button, TweenInfo.new(0.3, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {BackgroundTransparency = 1}):Play()
						tween_service:Create(MainFrame.accessibility_button.button.UICorner, TweenInfo.new(0.3, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {CornerRadius = UDim.new(0, 0)}):Play()
					else
						tween_service:Create(MainFrame.accessibility_button.button, TweenInfo.new(0.3, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {ImageTransparency = 1}):Play()
						tween_service:Create(MainFrame.accessibility_button.button, TweenInfo.new(0.3, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {BackgroundTransparency = 0.8}):Play()
						tween_service:Create(MainFrame.accessibility_button.button.UICorner, TweenInfo.new(0.3, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {CornerRadius = UDim.new(0, 8)}):Play()
					end
					if settingsData.storageFullscreen == "false" then
						fullscreen(not Storagefullscreen, true)
					end
				else
					module_announcement("There was a error loading executor settings please contant dev: SettingsExecutor.lua",15,"Error")
				end
			end
		end
	end
end

local function fullscreen(state, transform, delete_cache)
	Configs.Functions.ActiveFunctions += 1
	if state then
		
		Storagefullscreen = true
		blur.Enabled = true

		controls.restore.icon.Image = "rbxassetid://11422140434"

		controls.resize.Visible = false
		controls.resize_disabled.Visible = true

		if not Configs.Executor.Animations then
			if not ToggleStandByQuickPanel then
				button.ImageTransparency = 0.8
			else
				button.BackgroundTransparency = 0.8
			end
			button.page.front.open.Icon.TextTransparency = 1
			button.page.options.dashboard.icon.ImageTransparency = 1
			button.page.options.dashboard.icon.ImageTransparency = 1
			button.page.options.dashboard.icon.ImageTransparency = 1
			MainFrame.accessibility_button.Interactable = false
			if Essence_Instances.Folder0:FindFirstChildOfClass("UIListLayout") then
				Essence_Instances.Folder0:FindFirstChildOfClass("UIListLayout"):Destroy()
			end
			if navi6.Parent.scroll.build:FindFirstChildOfClass("TextLabel") then
				navi6.Parent.scroll.build.header.TextTransparency = 0
			end
			main_frame.corner.CornerRadius = UDim.new(0, 0)
			main_frame.menu_frame.corner.CornerRadius = UDim.new(0, 0)
			main_frame.window_controls.corner.CornerRadius = UDim.new(0, 0)

			main_frame.shadow.Visible = false
			--

			if not delete_cache then
				last_pos = main_frame.Position
				last_size = main_frame.Size
			end

			if transform then
				main_frame.Position = UDim2.fromScale(.5, .5)
				main_frame.Size = UDim2.fromScale(1, 1)
			end
		else
			if not Configs.Executor.Animations then
				if not ToggleStandByQuickPanel then
					button.ImageTransparency = 0.8
				else
					button.BackgroundTransparency = 0.8
				end
				button.page.front.open.Icon.TextTransparency = 1
				button.page.options.dashboard.icon.ImageTransparency = 1
				button.page.options.dashboard.icon.ImageTransparency = 1
				button.page.options.dashboard.icon.ImageTransparency = 1
				MainFrame.accessibility_button.Interactable = true
				MainFrame.accessibility_button.Interactable = false
				if Essence_Instances.Folder0:FindFirstChildOfClass("UIListLayout") then
					Essence_Instances.Folder0:FindFirstChildOfClass("UIListLayout"):Destroy()
				end
				if navi6.Parent.scroll.build:FindFirstChildOfClass("TextLabel") then
					tween_service:Create(navi6.Parent.scroll.build.header, quick, {TextTransparency = 0}):Play()
				end
				main_frame.corner.CornerRadius = UDim.new(0, 0)
				main_frame.menu_frame.corner.CornerRadius = UDim.new(0, 0)
				main_frame.window_controls.corner.CornerRadius = UDim.new(0, 0)

				main_frame.shadow.Visible = false
				
				local success, result = pcall(function()
					module_Files(false)
				end)

				if not success then
					module_announcement(string.format("An error occurred while running module_files", tostring(result)),10,"Error")
				end
				
				--

				if not delete_cache then
					last_pos = main_frame.Position
					last_size = main_frame.Size
				end
				
				if transform then
					main_frame.Position = UDim2.fromScale(.5, .5)
					main_frame.Size = UDim2.fromScale(1, 1)
				end
			else
			if not ToggleStandByQuickPanel then
				tween_service:Create(button, quick, {ImageTransparency = 0.8}):Play()
			else
				tween_service:Create(button, quick, {BackgroundTransparency = 0.8}):Play()
			end
			tween_service:Create(button.page.front.open.Icon, quick, {TextTransparency = 1}):Play()
			tween_service:Create(button.page.options.dashboard.icon, quick, {ImageTransparency = 1}):Play()
			tween_service:Create(button.page.options.dashboard.icon, quick, {ImageTransparency = 1}):Play()
			tween_service:Create(button.page.options.dashboard.icon, quick, {ImageTransparency = 1}):Play()
			MainFrame.accessibility_button.Interactable = false
			if Essence_Instances.Folder0:FindFirstChildOfClass("UIListLayout") then
				Essence_Instances.Folder0:FindFirstChildOfClass("UIListLayout"):Destroy()
			end
			if navi6.Parent.scroll.build:FindFirstChildOfClass("TextLabel") then
				tween_service:Create(navi6.Parent.scroll.build.header, quick, {TextTransparency = 0}):Play()
			end
			main_frame.corner.CornerRadius = UDim.new(0, 0)
			main_frame.menu_frame.corner.CornerRadius = UDim.new(0, 0)
			main_frame.window_controls.corner.CornerRadius = UDim.new(0, 0)

			main_frame.shadow.Visible = false
			local success, result = pcall(function()
				module_Files(false)
			end)

			if not success then
				module_announcement(string.format("An error occurred while running module_files", tostring(result)),10,"Error")
			end
			--

			if not delete_cache then
				last_pos = main_frame.Position
				last_size = main_frame.Size
			end

			if transform then
				tween_service:Create(main_frame, info, {Position = UDim2.fromScale(.5, .5)}):Play()
				tween_service:Create(main_frame, smooth, {Size = UDim2.fromScale(1, 1)}):Play()
			end
			end
		end
	else
		Storagefullscreen = false
		blur.Enabled = false

		controls.restore.icon.Image = "rbxassetid://11295287158"

		controls.resize.Visible = true
		controls.resize_disabled.Visible = false

		--

		main_frame.corner.CornerRadius = UDim.new(0, 20)
		main_frame.menu_frame.corner.CornerRadius = UDim.new(0, 20)
		main_frame.window_controls.corner.CornerRadius = UDim.new(0, 20)

		main_frame.shadow.Visible = true
		if not ToggleStandByQuickPanel then
			tween_service:Create(button, quick, {ImageTransparency = 0.8}):Play()
		else
			tween_service:Create(button, quick, {BackgroundTransparency = 0.8}):Play()
		end
		tween_service:Create(button.page.front.open.Icon, quick, {TextTransparency = 0}):Play()
		tween_service:Create(button.page.options.dashboard.icon, quick, {ImageTransparency = 0}):Play()
		tween_service:Create(button.page.options.dashboard.icon, quick, {ImageTransparency = 0}):Play()
		tween_service:Create(button.page.options.dashboard.icon, quick, {ImageTransparency = 0}):Play()
		MainFrame.accessibility_button.Interactable = true
		if Essence_Instances.list30:FindFirstChildOfClass("UIListLayout") then
			if Essence_Instances.Folder0:FindFirstChildOfClass("UIListLayout") then
			else
				local Template = Essence_Instances.list30:FindFirstChildOfClass("UIListLayout"):Clone()

				Template.Parent = Essence_Instances.Folder0
			end
		end
		local success, result = pcall(function()
			module_Files(false)
		end)

		if not success then
			module_announcement(string.format("An error occurred while running module_files", tostring(result)),10,"Error")
		end

		if navi6.Parent.scroll.build:FindFirstChildOfClass("TextLabel") then
			tween_service:Create(navi6.Parent.scroll.build.header, quick, {TextTransparency = 1}):Play()
		end

		--

		if transform then
			tween_service:Create(main_frame, smooth, {Position = last_pos}):Play()
			tween_service:Create(main_frame, info, {Size = last_size}):Play()
		end
	end
	Configs.Functions.ActiveFunctions -= 1
end

function module_announcement(Text,Duration,state)
	coroutine.wrap(function()
		local notification = Instance.new("ImageButton")
		local padding = Instance.new("UIPadding")
		local corner = Instance.new("UICorner")
		local gradient = Instance.new("UIGradient")
		local description = Instance.new("TextLabel")
		local icon = Instance.new("ImageButton")
		local close = Instance.new("ImageButton")
		local timeline = Instance.new("ImageLabel")
		local bar = Instance.new("ImageLabel")
		local scale = Instance.new("UIScale")
		local scale1 = Instance.new("UIScale")
		local Stroke = Instance.new("UIStroke")

		local ColorBackground = Color3.fromRGB(79, 79, 79)
		local ColorImageBackground = Color3.fromRGB(80, 80, 80)
		local ColorStrokeGradient = Color3.fromRGB(255, 255, 255)

		if state=="Warning" then
			ColorBackground = Color3.fromRGB(112, 55, 9)
			ColorImageBackground = Color3.fromRGB(197, 108, 0)
			ColorStrokeGradient = Color3.fromRGB(112, 55, 9)
		elseif state=="Error" then
			ColorBackground = Color3.fromRGB(112, 58, 66)
			ColorImageBackground = Color3.fromRGB(197, 127, 128)
			ColorStrokeGradient = Color3.fromRGB(255, 47, 50)
		elseif state=="Normal" then
			ColorBackground = Color3.fromRGB(79, 79, 79)
			ColorImageBackground = Color3.fromRGB(80, 80, 80)
			ColorStrokeGradient = Color3.fromRGB(255, 255, 255)
		else
			ColorBackground = Color3.fromRGB(79, 79, 79)
			ColorImageBackground = Color3.fromRGB(80, 80, 80)
			ColorStrokeGradient = Color3.fromRGB(255, 255, 255)
		end


		notification.Name = "notification"
		notification.Parent = Essence_Instances.notificationList
		notification.AnchorPoint = Vector2.new(0.5, 0)
		notification.AutomaticSize = Enum.AutomaticSize.X
		notification.BackgroundColor3 = ColorBackground
		notification.BackgroundTransparency = 1
		notification.BorderColor3 = Color3.fromRGB(0, 0, 0)
		notification.BorderSizePixel = 0
		notification.ClipsDescendants = true
		notification.Position = UDim2.new(0.5, 0, 0, 30)
		notification.Selectable = false
		notification.Size = UDim2.new(0, 0, 0, 40)
		notification.ZIndex = 10
		notification.AutoButtonColor = false
		--notification.Visible = false
		notification.Image = "rbxassetid://16294030997"
		notification.ImageTransparency = 1
		notification.ImageColor3 = ColorImageBackground
		notification.ScaleType = Enum.ScaleType.Crop
		notification.SliceCenter = Rect.new(512, 512, 512, 512)
		notification.SliceScale = 4.000

		padding.Name = "padding"
		padding.Parent = notification
		padding.PaddingBottom = UDim.new(0, 5)
		padding.PaddingLeft = UDim.new(0, 20)
		padding.PaddingRight = UDim.new(0, 20)
		padding.PaddingTop = UDim.new(0, 5)

		corner.CornerRadius = UDim.new(0, 100)
		corner.Name = "corner"
		corner.Parent = notification

		description.Name = "description"
		description.AutomaticSize = Enum.AutomaticSize.X
		description.Parent = notification
		description.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
		description.BackgroundTransparency = 1.000
		description.BorderColor3 = Color3.fromRGB(0, 0, 0)
		description.BorderSizePixel = 0
		description.Position = UDim2.new(0, 26, 0, 0)
		description.Size = UDim2.new(0, 0, 1, 0)
		description.ZIndex = 10
		description.Font = Enum.Font.Arimo
		description.Text = "Notification"
		description.TextColor3 = Color3.fromRGB(255, 255, 255)
		description.TextSize = 15.000
		description.TextTransparency = 1

		icon.Name = "icon"
		icon.Parent = notification
		icon.AnchorPoint = Vector2.new(0.5, 0.5)
		icon.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		icon.BackgroundTransparency = 1.000
		icon.BorderColor3 = Color3.fromRGB(0, 0, 0)
		icon.BorderSizePixel = 0
		icon.Position = UDim2.new(0, 8, 0.5, 0)
		icon.Size = UDim2.new(0, 16, 0, 16)
		icon.ZIndex = 10
		icon.Image = "rbxassetid://11295275950"
		icon.ImageTransparency = 1
		icon.ScaleType = Enum.ScaleType.Fit

		scale.Name = "scale"
		scale.Parent = icon

		Stroke.Name = "stroke"
		Stroke.Color = Color3.fromRGB(255, 255, 255)
		Stroke.Parent = notification
		Stroke.Transparency = 1

		gradient.Rotation = 80
		gradient.Color = ColorSequence.new{ColorSequenceKeypoint.new(0.00, ColorStrokeGradient), ColorSequenceKeypoint.new(1.00, Color3.fromRGB(255, 255, 255))}
		gradient.Transparency = NumberSequence.new{NumberSequenceKeypoint.new(0.00, 0.69), NumberSequenceKeypoint.new(1.00, 0.87)}
		gradient.Name = "gradient"
		gradient.Parent = Stroke

		close.Name = "close"
		close.Parent = notification
		close.AnchorPoint = Vector2.new(0.5, 0.5)
		close.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		close.BackgroundTransparency = 1.000
		close.BorderColor3 = Color3.fromRGB(0, 0, 0)
		close.BorderSizePixel = 0
		close.Position = UDim2.new(0, 8, 0.5, 0)
		close.Size = UDim2.new(0, 16, 0, 16)
		close.ZIndex = 10
		close.Interactable=false
		close.Image = "rbxassetid://11293981586"
		close.ImageTransparency = 1.000
		close.ScaleType = Enum.ScaleType.Fit

		scale1.Name = "scale"
		scale1.Parent = close

		timeline.Name = "timeline"
		timeline.Parent = notification
		timeline.AnchorPoint = Vector2.new(0.5, 1)
		timeline.BackgroundColor3 = Color3.fromRGB(100, 106, 105)
		timeline.BackgroundTransparency = 1.000
		timeline.BorderColor3 = Color3.fromRGB(0, 0, 0)
		timeline.BorderSizePixel = 0
		timeline.ClipsDescendants = true
		timeline.Position = UDim2.new(0.5, 0, 2, 0)
		timeline.Size = UDim2.new(0.100000001, 50, 0, 2)
		timeline.Image = "rbxassetid://16294678871"
		timeline.ImageTransparency = 1
		timeline.ScaleType = Enum.ScaleType.Slice
		timeline.SliceCenter = Rect.new(206, 206, 206, 206)

		bar.Name = "bar"
		bar.Parent = timeline
		bar.AnchorPoint = Vector2.new(0, 0.5)
		bar.BackgroundColor3 = Color3.fromRGB(100, 106, 105)
		bar.BackgroundTransparency = 1.000
		bar.BorderColor3 = Color3.fromRGB(0, 0, 0)
		bar.BorderSizePixel = 0
		bar.ClipsDescendants = true
		bar.Position = UDim2.new(0, 0, 0.5, 0)
		bar.Size = UDim2.new(0, 0, 1, 0)
		bar.Image = "rbxassetid://16294678871"
		bar.ImageTransparency = 1
		bar.ScaleType = Enum.ScaleType.Slice
		bar.SliceCenter = Rect.new(206, 206, 206, 206)

		icon.MouseEnter:Connect(function()
			if icon.ImageTransparency == 1 then return end
			--InDebounce=true
			tween_service:Create(close, quick, {ImageTransparency = 0}):Play()
			close.Interactable=true
			icon.Interactable=false
			tween_service:Create(icon, quick, {ImageTransparency = 1}):Play()
			task.wait(0.7)
			--InDebounce=false
		end)

		notification.InputEnded:Connect(function()
			if close.ImageTransparency == 1 then return end
			--InDebounce=true
			tween_service:Create(close, quick, {ImageTransparency = 1}):Play()
			close.Interactable=false
			icon.Interactable=true
			tween_service:Create(icon, quick, {ImageTransparency = 0}):Play()
			task.wait(0.7)
			--InDebounce=false
		end)

		close.MouseButton1Click:Connect(function()
			tween_service:Create(notification, quick, {BackgroundTransparency = 1}):Play()
			tween_service:Create(notification, quick, {ImageTransparency = 1}):Play()
			tween_service:Create(description, quick, {TextTransparency = 1}):Play()
			tween_service:Create(close, quick, {ImageTransparency = 1}):Play()
			tween_service:Create(icon, quick, {ImageTransparency = 1}):Play()
			tween_service:Create(Stroke, quick, {Transparency = 1}):Play()
			MainFrame:FindFirstChildOfClass("Sound"):Destroy()
			task.wait(0.5)
			notification:Destroy()
		end)
		tween_service:Create(gradient, loop, {Rotation = 360}):Play()

		local Sound = Instance.new("Sound")
		Sound.Parent = MainFrame
		Sound.SoundId="rbxassetid://3398620867"
		Sound:Play()
		description.Text=Text
		task.wait(0.1)
		tween_service:Create(notification, quick, {BackgroundTransparency = 0.05}):Play()
		tween_service:Create(notification, quick, {ImageTransparency = 0}):Play()
		tween_service:Create(close, quick, {ImageTransparency = 1}):Play()
		tween_service:Create(icon, quick, {ImageTransparency = 0}):Play()
		tween_service:Create(Stroke, quick, {Transparency = 0}):Play()
		tween_service:Create(description, quick, {TextTransparency = 0.2}):Play()
		task.wait(Duration)
		tween_service:Create(notification, quick, {BackgroundTransparency = 1}):Play()
		tween_service:Create(notification, quick, {ImageTransparency = 1}):Play()
		tween_service:Create(description, quick, {TextTransparency = 1}):Play()
		tween_service:Create(close, quick, {ImageTransparency = 1}):Play()
		tween_service:Create(icon, quick, {ImageTransparency = 1}):Play()
		tween_service:Create(Stroke, quick, {Transparency = 1}):Play()
		if MainFrame:FindFirstChildOfClass("Sound") then
			MainFrame:FindFirstChildOfClass("Sound"):Destroy()
		end
		task.wait(0.5)
		notification:Destroy()
	end)()
end

function module_UpdateFolders(code,state)
	if (isStudio) then return end
	local settingValue = Configs.Executor.ExecutorSettings[code]

	Configs.Executor.ExecutorSettings[code] = state

	if code=="AutoExec" then
		local success, result = pcall(function()
			module_Files(false)
		end)

		if not success then
			module_announcement(string.format("An error occurred while running module_files", tostring(result)),10,"Error")
		end
	elseif code == "AutoInject" then
		local success, result = pcall(function()
			module_Files(false)
		end)

		if not success then
			module_announcement(string.format("An error occurred while running module_files", tostring(result)),10,"Error")
		end
	elseif code == "ModeDrag" then
		local success, result = pcall(function()
			module_Files(false)
		end)

		if not success then
			module_announcement(string.format("An error occurred while running module_files", tostring(result)),10,"Error")
		end
	elseif code == "Keybind" then
		local success, result = pcall(function()
			module_Files(false)
		end)

		if not success then
			module_announcement(string.format("An error occurred while running module_files", tostring(result)),10,"Error")
		end
	elseif code == "Animations" then
		local success, result = pcall(function()
			module_Files(false)
		end)

		if not success then
			module_announcement(string.format("An error occurred while running module_files", tostring(result)),10,"Error")
		end
	elseif code == "MultiInstance" then
		local success, result = pcall(function()
			module_Files(false)
		end)

		if not success then
			module_announcement(string.format("An error occurred while running module_files", tostring(result)),10,"Error")
		end
	end
end


local Number = 0
function module_CreateButton(Text,Desc,state,code,module)
	if Text and Desc and not module then
		if not local_player.Character then
			local_player.CharacterAdded:Wait()
		end
		coroutine.wrap(function()
			local Template = navi5.Parent.scroll.build.SettingsMenu.list.item:Clone()
			local settingValue = Configs.Executor.ExecutorSettings[code]

			Template.Parent = navi5.Parent.scroll.build.SettingsMenu
			if Number==0 then
				Template.Image = "rbxassetid://16287196357"
				Template.ScaleType = Enum.ScaleType.Slice
				Template.SliceCenter = Rect.new(512, 214, 512, 214)
				Template.SliceScale = 0.12
			elseif Number==1 then
				Template.Image = "rbxassetid://16286719854"
				Template.ScaleType = Enum.ScaleType.Crop
			elseif Number==2 then
				Template.Image = "rbxassetid://16287194510"
				Template.ScaleType = Enum.ScaleType.Slice
				Template.SliceCenter = Rect.new(512, 0, 512, 0)
				Template.SliceScale = 0.12
			elseif Number==3 then
				Template.Image = "rbxassetid://16287196357"
				Template.ScaleType = Enum.ScaleType.Slice
				Template.SliceCenter = Rect.new(512, 214, 512, 214)
				Template.SliceScale = 0.12
			elseif Number==4 then
				Template.Image = "rbxassetid://16287194510"
				Template.ScaleType = Enum.ScaleType.Slice
				Template.SliceCenter = Rect.new(512, 0, 512, 0)
				Template.SliceScale = 0.12
			elseif Number==5 then
				Template.Image = "rbxassetid://16286719854"
				Template.ScaleType = Enum.ScaleType.Crop
			end


			if code=="AutoExec" or code=="AutoInject" then
			else
				if code=="ModeDrag" then
					if ModeDrag==1 then
						if not Configs.Executor.Animations then
							Template.ButtonFrame.Button1.Position = UDim2.new(0, 2,0.889, 0)
							Template.ButtonFrame.Button2.Position = UDim2.new(0, 2,0.074, 0)

							Template.ButtonFrame.BackgroundColor3 = Color3.fromRGB(34, 34, 34)
							Template.ButtonFrame.Button2.ImageColor3 = Color3.fromRGB(117, 117, 117)
							Template.ButtonFrame.UIStroke.Color = Color3.fromRGB(111, 111, 111)
							Template.label.TextTransparency=0.5
							Template.desc.TextTransparency=0.5
						else
							tween_service:Create(Template.ButtonFrame.Button1, info, {Position = UDim2.new(0, 2,0.889, 0)}):Play()
							tween_service:Create(Template.ButtonFrame.Button2, info, {Position = UDim2.new(0, 2,0.074, 0)}):Play()

							game:GetService("TweenService"):Create(Template.ButtonFrame,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{BackgroundColor3=Color3.fromRGB(34, 34, 34)}):Play()
							game:GetService("TweenService"):Create(Template.ButtonFrame.Button2,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{ImageColor3=Color3.fromRGB(117, 117, 117)}):Play()
							game:GetService("TweenService"):Create(Template.ButtonFrame.UIStroke,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{Color = Color3.fromRGB(111, 111, 111)}):Play()
							game:GetService("TweenService"):Create(Template.label,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{TextTransparency=0.5}):Play()
							game:GetService("TweenService"):Create(Template.desc,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{TextTransparency=0.5}):Play()
						end
					elseif code=="Animations" then
						Template.Image = "rbxassetid://16286719854"
						Template.ScaleType = Enum.ScaleType.Crop
					end
				elseif code=="Keybind" then
					Template.ButtonFrame.Button1:Destroy()
					Template.ButtonFrame.Button2:Destroy()
					local ButtonKeybind = Instance.new("TextButton")
					task.wait(3)
					ButtonKeybind.Name = "ButtonKeybind"
					ButtonKeybind.Parent = Template.ButtonFrame
					ButtonKeybind.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
					ButtonKeybind.BackgroundTransparency = 1.000
					ButtonKeybind.BorderColor3 = Color3.fromRGB(0, 0, 0)
					ButtonKeybind.BorderSizePixel = 0
					ButtonKeybind.Position = UDim2.new(0, 2, 0.0740000382, 0)
					ButtonKeybind.Size = UDim2.new(0, 35, 0, 22)
					ButtonKeybind.Font = Enum.Font.Arial
					ButtonKeybind.Text = Configs.Executor.KeybindMenu.Name
					ButtonKeybind.TextColor3 = Color3.fromRGB(255, 255, 255)
					ButtonKeybind.TextSize = 15.000

					Template.ButtonFrame.Position = UDim2.new(0.953, 0,0.748, 0)
					Template.ButtonFrame.Size = UDim2.new(0, 41, 0, 27)


					Template.label.Text = Text

					Template.desc.Text = Desc
					Template.desc.RichText = true

					Number += 1

					Template.ButtonFrame.ButtonKeybind.MouseButton1Click:Connect(function()
						Template.ButtonFrame.ButtonKeybind.Text = "Input."
						if not Configs.Executor.Animations then
							Template.ButtonFrame.BackgroundColor3 = Color3.fromRGB(99, 99, 99)
							Template.ButtonFrame.ButtonKeybind.TextColor3 = Color3.fromRGB(255, 255, 255)
							Template.ButtonFrame.UIStroke.Color = Color3.fromRGB(111, 111, 111)
							Template.label.TextTransparency=0.3
							Template.desc.TextTransparency=0.3
						else
							game:GetService("TweenService"):Create(Template.ButtonFrame,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{BackgroundColor3=Color3.fromRGB(99, 99, 99)}):Play()
							game:GetService("TweenService"):Create(Template.ButtonFrame.ButtonKeybind,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{TextColor3=Color3.fromRGB(255, 255, 255)}):Play()
							game:GetService("TweenService"):Create(Template.ButtonFrame.UIStroke,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{Color = Color3.fromRGB(111, 111, 111)}):Play()
							game:GetService("TweenService"):Create(Template.label,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{TextTransparency=0.3}):Play()
							game:GetService("TweenService"):Create(Template.desc,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{TextTransparency=0.3}):Play()
						end
						local input = UserInputService.InputBegan:Wait()
						local keyCode = input.KeyCode
						if keyCode ~= Enum.KeyCode.Unknown and keyCode ~= Enum.KeyCode.Backspace then
							if UserInputService:GetFocusedTextBox() ~= nil then return end
							Configs.Executor.KeybindMenu = keyCode
							Template.ButtonFrame.ButtonKeybind.Text = Configs.Executor.KeybindMenu.Name
							wait(1)
							module_Files(false)
						else
							Template.ButtonFrame.ButtonKeybind.Text = Configs.Executor.KeybindMenu.Name
							if not Configs.Executor.Animations then
								Template.ButtonFrame.BackgroundColor3 = Color3.fromRGB(34, 34, 34)
								Template.ButtonFrame.ButtonKeybind.TextColor3 = Color3.fromRGB(117, 117, 117)
								Template.ButtonFrame.UIStroke.Color = Color3.fromRGB(111, 111, 111)
								Template.label.TextTransparency=0.5
								Template.desc.TextTransparency=0.5
							else
								game:GetService("TweenService"):Create(Template.ButtonFrame,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{BackgroundColor3=Color3.fromRGB(34, 34, 34)}):Play()
								game:GetService("TweenService"):Create(Template.ButtonFrame.ButtonKeybind,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{TextColor3=Color3.fromRGB(117, 117, 117)}):Play()
								game:GetService("TweenService"):Create(Template.ButtonFrame.UIStroke,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{Color = Color3.fromRGB(111, 111, 111)}):Play()
								game:GetService("TweenService"):Create(Template.label,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{TextTransparency=0.5}):Play()
								game:GetService("TweenService"):Create(Template.desc,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{TextTransparency=0.5}):Play()
							end
						end
						if not Configs.Executor.Animations then
							Template.ButtonFrame.BackgroundColor3 = Color3.fromRGB(34, 34, 34)
							Template.ButtonFrame.ButtonKeybind.TextColor3 = Color3.fromRGB(117, 117, 117)
							Template.ButtonFrame.UIStroke.Color = Color3.fromRGB(111, 111, 111)
							Template.label.TextTransparency=0.5
							Template.desc.TextTransparency=0.5
						else
							game:GetService("TweenService"):Create(Template.ButtonFrame,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{BackgroundColor3=Color3.fromRGB(34, 34, 34)}):Play()
							game:GetService("TweenService"):Create(Template.ButtonFrame.ButtonKeybind,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{TextColor3=Color3.fromRGB(117, 117, 117)}):Play()
							game:GetService("TweenService"):Create(Template.ButtonFrame.UIStroke,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{Color = Color3.fromRGB(111, 111, 111)}):Play()
							game:GetService("TweenService"):Create(Template.label,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{TextTransparency=0.5}):Play()
							game:GetService("TweenService"):Create(Template.desc,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{TextTransparency=0.5}):Play()
						end
					end)
					return
				end
			end

			if not state or not settingValue then
				if code=="ModeDrag" then
				elseif code=="Animations" then
					Template.Image = "rbxassetid://16286719854"
					Template.ScaleType = Enum.ScaleType.Crop
					if Configs.Executor.Animations then
						if not Configs.Executor.Animations then
							Template.ButtonFrame.Button1.Position = UDim2.new(0, 2,0.889, 0)
							Template.ButtonFrame.Button2.Position = UDim2.new(0, 2,0.074, 0)

							Template.ButtonFrame.BackgroundColor3 = Color3.fromRGB(34, 34, 34)
							Template.ButtonFrame.Button2.ImageColor3 = Color3.fromRGB(117, 117, 117)
							Template.ButtonFrame.UIStroke.Color = Color3.fromRGB(111, 111, 111)
							Template.label.TextTransparency=0.5
							Template.desc.TextTransparency=0.5
						else
							tween_service:Create(Template.ButtonFrame.Button1, info, {Position = UDim2.new(0, 2,0.889, 0)}):Play()
							tween_service:Create(Template.ButtonFrame.Button2, info, {Position = UDim2.new(0, 2,0.074, 0)}):Play()

							game:GetService("TweenService"):Create(Template.ButtonFrame,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{BackgroundColor3=Color3.fromRGB(34, 34, 34)}):Play()
							game:GetService("TweenService"):Create(Template.ButtonFrame.Button2,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{ImageColor3=Color3.fromRGB(117, 117, 117)}):Play()
							game:GetService("TweenService"):Create(Template.ButtonFrame.UIStroke,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{Color = Color3.fromRGB(111, 111, 111)}):Play()
							game:GetService("TweenService"):Create(Template.label,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{TextTransparency=0.5}):Play()
							game:GetService("TweenService"):Create(Template.desc,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{TextTransparency=0.5}):Play()
						end
					end
				elseif code=="MultiInstance" then
					if not Configs.Executor.Animations then
						Template.ButtonFrame.Button1.Position = UDim2.new(0, 2,0.889, 0)
						Template.ButtonFrame.Button2.Position = UDim2.new(0, 2,0.074, 0)

						Template.ButtonFrame.BackgroundColor3 = Color3.fromRGB(34, 34, 34)
						Template.ButtonFrame.Button2.ImageColor3 = Color3.fromRGB(117, 117, 117)
						Template.ButtonFrame.UIStroke.Color = Color3.fromRGB(111, 111, 111)
						Template.label.TextTransparency=0.5
						Template.desc.TextTransparency=0.5
					else
						tween_service:Create(Template.ButtonFrame.Button1, info, {Position = UDim2.new(0, 2,0.889, 0)}):Play()
						tween_service:Create(Template.ButtonFrame.Button2, info, {Position = UDim2.new(0, 2,0.074, 0)}):Play()

						game:GetService("TweenService"):Create(Template.ButtonFrame,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{BackgroundColor3=Color3.fromRGB(34, 34, 34)}):Play()
						game:GetService("TweenService"):Create(Template.ButtonFrame.Button2,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{ImageColor3=Color3.fromRGB(117, 117, 117)}):Play()
						game:GetService("TweenService"):Create(Template.ButtonFrame.UIStroke,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{Color = Color3.fromRGB(111, 111, 111)}):Play()
						game:GetService("TweenService"):Create(Template.label,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{TextTransparency=0.5}):Play()
						game:GetService("TweenService"):Create(Template.desc,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{TextTransparency=0.5}):Play()
					end
				else

					if code == "Keybind" then
					else
						if not Configs.Executor.Animations then
							Template.ButtonFrame.Button1.Position = UDim2.new(0, 2,0.889, 0)
							Template.ButtonFrame.Button2.Position = UDim2.new(0, 2,0.074, 0)

							Template.ButtonFrame.BackgroundColor3 = Color3.fromRGB(34, 34, 34)
							Template.ButtonFrame.Button2.ImageColor3 = Color3.fromRGB(117, 117, 117)
							Template.ButtonFrame.UIStroke.Color = Color3.fromRGB(111, 111, 111)
							Template.label.TextTransparency=0.5
							Template.desc.TextTransparency=0.5
						else
							tween_service:Create(Template.ButtonFrame.Button1, info, {Position = UDim2.new(0, 2,0.889, 0)}):Play()
							tween_service:Create(Template.ButtonFrame.Button2, info, {Position = UDim2.new(0, 2,0.074, 0)}):Play()

							game:GetService("TweenService"):Create(Template.ButtonFrame,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{BackgroundColor3=Color3.fromRGB(34, 34, 34)}):Play()
							game:GetService("TweenService"):Create(Template.ButtonFrame.Button2,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{ImageColor3=Color3.fromRGB(117, 117, 117)}):Play()
							game:GetService("TweenService"):Create(Template.ButtonFrame.UIStroke,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{Color = Color3.fromRGB(111, 111, 111)}):Play()
							game:GetService("TweenService"):Create(Template.label,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{TextTransparency=0.5}):Play()
							game:GetService("TweenService"):Create(Template.desc,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{TextTransparency=0.5}):Play()
						end
					end		
				end	
			end

			Template.label.Text = Text

			Template.desc.Text = Desc
			Template.desc.RichText = true

			Number += 1

			if code == "Keybind" then
			else
				Template.ButtonFrame.Button1.MouseButton1Click:Connect(function()
					if code=="AutoExec" or code=="AutoInject" then
						Configs.Executor.ExecutorSettings[code] = false
					else
						if code=="ModeDrag" then
							ModeDrag=1
						elseif code=="Animations" then
							Configs.Executor[code] = true
						elseif code=="MultiInstance" then
						end
					end
					if not Configs.Executor.Animations then
						Template.ButtonFrame.Button1.Position = UDim2.new(0, 2,0.889, 0)
						Template.ButtonFrame.Button2.Position = UDim2.new(0, 2,0.074, 0)

						Template.ButtonFrame.BackgroundColor3 = Color3.fromRGB(34, 34, 34)
						Template.ButtonFrame.Button2.ImageColor3 = Color3.fromRGB(117, 117, 117)
						Template.ButtonFrame.UIStroke.Color = Color3.fromRGB(111, 111, 111)
                        Template.label.TextTransparency=0.5
						Template.desc.TextTransparency=0.5
						else
						tween_service:Create(Template.ButtonFrame.Button1, info, {Position = UDim2.new(0, 2,0.889, 0)}):Play()
						tween_service:Create(Template.ButtonFrame.Button2, info, {Position = UDim2.new(0, 2,0.074, 0)}):Play()

						game:GetService("TweenService"):Create(Template.ButtonFrame,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{BackgroundColor3=Color3.fromRGB(34, 34, 34)}):Play()
						game:GetService("TweenService"):Create(Template.ButtonFrame.Button2,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{ImageColor3=Color3.fromRGB(117, 117, 117)}):Play()
						game:GetService("TweenService"):Create(Template.ButtonFrame.UIStroke,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{Color = Color3.fromRGB(111, 111, 111)}):Play()
						game:GetService("TweenService"):Create(Template.label,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{TextTransparency=0.5}):Play()
						game:GetService("TweenService"):Create(Template.desc,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{TextTransparency=0.5}):Play()
					end
					module_UpdateFolders(code,false)
				end)
				Template.ButtonFrame.Button2.MouseButton1Click:Connect(function()
					if code=="AutoExec" or code=="AutoInject" then
						Configs.Executor.ExecutorSettings[code] = true
					else
						if code=="ModeDrag" then
							ModeDrag=2
						elseif code=="Animations" then
							Configs.Executor[code] = false
						elseif code=="MultiInstance" then

						end
					end
					if not Configs.Executor.Animations then
						Template.ButtonFrame.Button1.Position = UDim2.new(0, 2,0.074, 0)
						Template.ButtonFrame.Button2.Position = UDim2.new(0, 2,0.889, 0)

						Template.label.TextTransparency = 0.3
						Template.desc.TextTransparency = 0.3
						Template.ButtonFrame.BackgroundColor3 = Color3.fromRGB(99, 99, 99)
						Template.ButtonFrame.Button1.ImageColor3 = Color3.fromRGB(255, 255, 255)
						Template.ButtonFrame.UIStroke.Color = Color3.fromRGB(111, 111, 111)
					else
						tween_service:Create(Template.ButtonFrame.Button1, info, {Position = UDim2.new(0, 2,0.074, 0)}):Play()
						tween_service:Create(Template.ButtonFrame.Button2, info, {Position = UDim2.new(0, 2,0.889, 0)}):Play()

						game:GetService("TweenService"):Create(Template.label,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{TextTransparency=0.3}):Play()
						game:GetService("TweenService"):Create(Template.desc,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{TextTransparency=0.3}):Play()
						game:GetService("TweenService"):Create(Template.ButtonFrame,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{BackgroundColor3=Color3.fromRGB(99, 99, 99)}):Play()
						game:GetService("TweenService"):Create(Template.ButtonFrame.Button1,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{ImageColor3=Color3.fromRGB(255, 255, 255)}):Play()
						game:GetService("TweenService"):Create(Template.ButtonFrame.UIStroke,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{Color = Color3.fromRGB(111, 111, 111)}):Play()
					end
					module_UpdateFolders(code,true)
				end)
			end
		end)()
	elseif module and Text then
		if code == "Noclip" then
