	end
end
local success, result = pcall(function()
	hover5()
end)

if not success then
	module_announcement(string.format("An error occurred while running hover5", tostring(result)),10,"Error")
end
local success, result = pcall(function()
	hover_navi4()
end)

if not success then
	module_announcement(string.format("An error occurred while running hover_navi4", tostring(result)),10,"Error")
end




--WINDOW HANDLER
--// RESIZE

function scale_ratio()
	local ratio = viewport.X + viewport.Y

	if ratio > 1500 then
		--frame.Visible = true
	else
		frame.Visible = false
	end
end

local success, result = pcall(function()
	scale_ratio()
end)

if not success then
	module_announcement(string.format("An error occurred while running scale_ratio", tostring(result)),10,"Error")
end

local function start_resize(input)
	resizing = true
	start_mouse_pos = Vector2.new(input.Position.X, input.Position.Y)
	start_frame_size = main_frame.Size
	start_frame_pos = main_frame.Position

	layer.Visible = true
end

local function stop()
	resizing = false
	layer.Visible = false

	--
	

	if not Configs.Executor.Animations then
		controls.resize.icon.scale.Scale = 1

		controls.resize.selection.ImageTransparency = 1
		controls.resize.selection.scale.Scale = .5
	else
		tween_service:Create(controls.resize.icon.scale, info, {Scale = 1}):Play()

		tween_service:Create(controls.resize.selection, info, {ImageTransparency = 1}):Play()
		tween_service:Create(controls.resize.selection.scale, info, {Scale = .5}):Play()
	end

	--

	local success, result = pcall(function()
		module_Files(false)
	end)

	if not success then
		module_announcement(string.format("An error occurred while running module_files", tostring(result)),10,"Error")
	end
end

local function resize(input) 
	if resizing and (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) then
		local currentMousePosition = Vector2.new(input.Position.X, input.Position.Y)
		local mouseDelta = currentMousePosition - start_mouse_pos
		local newSizeX = start_frame_size.X.Offset + (mouseDelta.X * 2)
		local newSizeY = start_frame_size.Y.Offset + (mouseDelta.Y * 2)
		local minX, minY = 550, 350
		local maxX = math.max(max_size.X, minX)
		local maxY = math.max(max_size.Y, minY)

		newSizeX = math.clamp(newSizeX, minX, maxX)
		newSizeY = math.clamp(newSizeY, minY, maxY)


		local newSize = Vector2.new(newSizeX, newSizeY)

		local newFrameWidth = main_frame.AbsoluteSize.X
		local newFrameHeight = main_frame.AbsoluteSize.Y

		--

		tween_service:Create(main_frame, smooth, {Size = UDim2.new(0, newSizeX, 0, newSizeY)}):Play()
		tween_service:Create(main_frame, info, {Position = UDim2.new(start_frame_pos.X.Scale, start_frame_pos.X.Offset - (newFrameWidth - main_frame.AbsoluteSize.X) / 2,
			start_frame_pos.Y.Scale, start_frame_pos.Y.Offset - (newFrameHeight - main_frame.AbsoluteSize.Y) / 2)}):Play()

		--

		fullscreen(false, false)

		requesting_fullscreen = false
		blur.Enabled = false
		Storagefullscreen = false

		last_pos = main_frame.Position
		last_size = main_frame.Size

		controls.restore.icon.Image = "rbxassetid://11295287158"
	end
end


controls.resize.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
		start_resize(input)
	end
end)

input_service.InputEnded:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
		stop()
	end
end)

input_service.InputChanged:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch and resizing then
		resize(input)
	end
end)

--// DRAGGING

function start_dragging(input)
	if dragging or resizing then
		return
	end

	dragging = true
	start_position = input.Position
	start_frame_pos = main_frame.Position

	layer.Visible = true
end

function update(input)
	if dragging and start_position and start_frame_pos then
		if Storagefullscreen then
			fullscreen(false, false)

			tween_service:Create(main_frame, info, {Size = last_size}):Play()
		end

		local viewport = workspace_service.CurrentCamera.ViewportSize
		local delta = input.Position - start_position
		local position = UDim2.new(start_frame_pos.X.Scale, start_frame_pos.X.Offset + delta.X, start_frame_pos.Y.Scale, start_frame_pos.Y.Offset + delta.Y)

		tween_service:Create(main_frame, info, {Position = position}):Play()
	end
end

function end_drag()
	dragging = false
	last_pos = main_frame.Position

	layer.Visible = false
end


local function dragUI(frame)
	local dragging = false
	local dragStart
	local startPos
	local screenSize = camera.ViewportSize

	frame.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			dragging = true
			dragStart = input.Position
			startPos = MainFrame.accessibility_button.Position

			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)

	UserInputService.InputChanged:Connect(function(input)
		if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
			local delta = input.Position - dragStart
			local newX = startPos.X.Offset + delta.X
			local newY = startPos.Y.Offset + delta.Y

			local leftBoundary = screenSize.X * 0.01
			local rightBoundary = screenSize.X * 1
			local topBoundary = screenSize.Y * 0.0
			local bottomBoundary = screenSize.Y * 1

			local targetRotation = 0
			local targetRotation2 = 0

			if newX < leftBoundary then
				targetRotation = -90
				targetRotation2 = 0
				newX = leftBoundary
				newY = math.clamp(newY, 10, screenSize.Y - MainFrame.accessibility_button.AbsoluteSize.Y - 10)
				ToggleStandByQuickPanel=false
			elseif newX > rightBoundary then
				targetRotation = 90
				targetRotation2 = 0
				newX = rightBoundary
				newY = math.clamp(newY, 10, screenSize.Y - MainFrame.accessibility_button.AbsoluteSize.Y - 10)
				ToggleStandByQuickPanel=false
			elseif newY < topBoundary then
				targetRotation = 0
				targetRotation2 = 0
				newY = topBoundary
				newX = math.clamp(newX, 10, screenSize.X - MainFrame.accessibility_button.AbsoluteSize.X - 10)
				ToggleStandByQuickPanel=false
			elseif newY > bottomBoundary then
				targetRotation = 180
				targetRotation2 = 180
				newY = bottomBoundary
				newX = math.clamp(newX, 10, screenSize.X - MainFrame.accessibility_button.AbsoluteSize.X - 10)
				ToggleStandByQuickPanel=false
			else
				ToggleStandByQuickPanel=true
				--newX = math.clamp(newX, 10, screenSize.X - MainFrame.accessibility_button.AbsoluteSize.X - 10)
				--newY = math.clamp(newY, 10, screenSize.Y - MainFrame.accessibility_button.AbsoluteSize.Y - 10)
			end

			MainFrame.accessibility_button.Position = UDim2.new(0, newX, 0, newY)

			if not ToggleStandByQuickPanel then
				tween_service:Create(MainFrame.accessibility_button.button, TweenInfo.new(0.3, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {ImageTransparency = 0.8}):Play()
				tween_service:Create(MainFrame.accessibility_button.button, TweenInfo.new(0.3, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {BackgroundTransparency = 1}):Play()
				tween_service:Create(MainFrame.accessibility_button.button.UICorner, TweenInfo.new(0.3, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {CornerRadius = UDim.new(0, 0)}):Play()
			else
				tween_service:Create(MainFrame.accessibility_button.button, TweenInfo.new(0.3, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {ImageTransparency = 1}):Play()
				tween_service:Create(MainFrame.accessibility_button.button, TweenInfo.new(0.3, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {BackgroundTransparency = 0.8}):Play()
				tween_service:Create(MainFrame.accessibility_button.button.UICorner, TweenInfo.new(0.3, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {CornerRadius = UDim.new(0, 8)}):Play()
			end

			tween_service:Create(MainFrame.accessibility_button.button.page.front.open.Icon, TweenInfo.new(0.3, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {Rotation = targetRotation2}):Play()
			tween_service:Create(MainFrame.accessibility_button, TweenInfo.new(0.3, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {Rotation = targetRotation}):Play()

			local success, result = pcall(function()
				module_Files(false)
			end)

			if not success then
				module_announcement(string.format("An error occurred while running module_files", tostring(result)),10,"Error")
			end
		end
	end)
end

local success, result = pcall(function()
	dragUI(MainFrame.accessibility_button.button)
end)

if not success then
	module_announcement(string.format("An error occurred while running dragUI", tostring(result)),10,"Error")
end

frame.window_controls.button.InputBegan:Connect(function(input)
	if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then
		start_dragging(input)
	end
end)

input_service.InputChanged:Connect(function(input)
	update(input)
end)

input_service.InputEnded:Connect(function(input)
	if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch and dragging) then
		end_drag()
	end
end)

--// BUTTON

local hold = false

frame.window_controls.button.MouseButton1Click:Connect(function()
	if controls.Visible and not hold then
		hold = true
		

		if not Configs.Executor.Animations then
			controls.Position = UDim2.new(1, 100, 1, 0)
			controls.Size = UDim2.fromOffset(50, 50)
		else
			tween_service:Create(controls, info, {Position = UDim2.new(1, 100, 1, 0), Size = UDim2.fromOffset(50, 50)}):Play()
		end

		--
		task.wait(.5)
		--

		controls.Visible = false
		hold = false

	elseif not controls.Visible and not hold then
		hold = true

		controls.Visible = true
		controls.Position = UDim2.new(1, 100, 1, 0)
		controls.Size = UDim2.fromOffset(50, 50)

		--
		
		if not Configs.Executor.Animations then
			controls.Position = UDim2.new(1, 0, 1, 0)
			controls.Size = UDim2.fromOffset(100, 50)
		else
			tween_service:Create(controls, info, {Position = UDim2.new(1, 0, 1, 0), Size = UDim2.fromOffset(100, 50)}):Play()
		end

		--
		task.wait(.5)
		--

		hold = false
	end
end)

controls.restore.MouseButton1Click:Connect(function()
	fullscreen(not Storagefullscreen, true)
end)



controls.resize.MouseEnter:Connect(function()
	if not resizing then
		controls.resize.selection.ImageTransparency = 1
		controls.resize.selection.scale.Scale = .5
		
		if not Configs.Executor.Animations then
			controls.resize.icon.scale.Scale = 1.2

			controls.resize.selection.ImageTransparency = .8
			controls.resize.selection.scale.Scale = 1
		else
			tween_service:Create(controls.resize.icon.scale, info, {Scale = 1.2}):Play()

			tween_service:Create(controls.resize.selection, info, {ImageTransparency = .8}):Play()
			tween_service:Create(controls.resize.selection.scale, info, {Scale = 1}):Play()
		end
	end
end)

controls.resize.MouseButton1Down:Connect(function()
	if not Configs.Executor.Animations then
		controls.resize.icon.scale.Scale = .5

		controls.resize.selection.ImageTransparency = .9
		controls.resize.selection.scale.Scale = .8
	else
		tween_service:Create(controls.resize.icon.scale, info, {Scale = .5}):Play()

		tween_service:Create(controls.resize.selection, info, {ImageTransparency = .9}):Play()
		tween_service:Create(controls.resize.selection.scale, info, {Scale = .8}):Play()
	end
end)

controls.resize.InputEnded:Connect(function()
	if not resizing then
		if not Configs.Executor.Animations then
			controls.resize.icon.scale.Scale = 1

			controls.resize.selection.ImageTransparency = 1
			controls.resize.selection.scale.Scale = .5
		else
			tween_service:Create(controls.resize.icon.scale, info, {Scale = 1}):Play()

			tween_service:Create(controls.resize.selection, info, {ImageTransparency = 1}):Play()
			tween_service:Create(controls.resize.selection.scale, info, {Scale = .5}):Play()
		end
	end
end)



controls.restore.MouseEnter:Connect(function()
	controls.restore.selection.ImageTransparency = 1
	controls.restore.selection.scale.Scale = .5
	
	if not Configs.Executor.Animations then
		controls.restore.icon.scale.Scale = 1.2

		controls.restore.selection.ImageTransparency = .8
		controls.restore.selection.scale.Scale = 1
	else
		tween_service:Create(controls.restore.icon.scale, info, {Scale = 1.2}):Play()

		tween_service:Create(controls.restore.selection, info, {ImageTransparency = .8}):Play()
		tween_service:Create(controls.restore.selection.scale, info, {Scale = 1}):Play()
	end
end)

controls.restore.MouseButton1Down:Connect(function()
	if not Configs.Executor.Animations then
		controls.restore.icon.scale.Scale = .5

		controls.restore.selection.ImageTransparency = .9
		controls.restore.selection.scale.Scale = .8
	else
		tween_service:Create(controls.restore.icon.scale, info, {Scale = .5}):Play()

		tween_service:Create(controls.restore.selection, info, {ImageTransparency = .9}):Play()
		tween_service:Create(controls.restore.selection.scale, info, {Scale = .8}):Play()
	end
end)

controls.restore.InputEnded:Connect(function()
	if not Configs.Executor.Animations then
		controls.restore.icon.scale.Scale = 1

		controls.restore.selection.ImageTransparency = 1
		controls.restore.selection.scale.Scale = .5
	else
		tween_service:Create(controls.restore.icon.scale, info, {Scale = 1}):Play()

		tween_service:Create(controls.restore.selection, info, {ImageTransparency = 1}):Play()
		tween_service:Create(controls.restore.selection.scale, info, {Scale = .5}):Play()
	end
end)

local db = false

input_service.InputBegan:Connect(function(input)
	local key1, key2 = Enum.KeyCode.LeftControl, Configs.Executor.KeybindMenu
	if UserInputService:GetFocusedTextBox() ~= nil then return end
	if (input.KeyCode == key2 and not input_service:IsKeyDown(key1)) then
		if not db  then
			db = true

			module_panel(not main_frame.Visible)


			--
			task.wait(1)
			--

			db = false
		end
	end
end)

--// SCALING
UserInputService.InputChanged:Connect(function(input, gameProcessed)
	if input.UserInputType == Enum.UserInputType.MouseWheel then
		if UserInputService:IsKeyDown(Enum.KeyCode.LeftControl) then
			local delta = input.Position.Z
			local currentScale = main_frame.Parent.scale.Scale or 1
			local newScale = math.clamp(currentScale + delta * 0.1, 0.5, 2)
			tween_service:Create(main_frame.Parent.scale, info, {Scale = newScale}):Play()
		end
	end
end)

UserInputService.InputBegan:Connect(function(input, gameProcessed)
	if input.KeyCode == Configs.Executor.KeybindMenu and UserInputService:IsKeyDown(Enum.KeyCode.LeftControl) then
		if UserInputService:GetFocusedTextBox() then return end
		tween_service:Create(main_frame.Parent.scale, info, {Scale = 1}):Play()
	end
end)

local promptedDisconnected = false
UserInputService.InputBegan:Connect(function(input, gameProccesedEvent)
	if input.KeyCode == Enum.KeyCode.Y then
		if UserInputService:GetFocusedTextBox() ~= nil then return end
		if promptedDisconnected then
			if #player_service:GetPlayers() <= 1 then
				task.wait()
				teleportService:Teleport(game.PlaceId, local_player)
			else
				teleportService:TeleportToPlaceInstance(game.PlaceId, game.jobId, local_player)
			end
		else
			coroutine.wrap(function()
				module_Prompt(false)
			end)()
			if main_frame.confirmation_prompt.prompt.confirm.Text == "Yes (Y)" then
				if main_frame.confirmation_prompt.prompt.header.Text == "Confirmation Required" then
					if main_frame.confirmation_prompt.prompt.description.Text == "You are about to be connected to this client session. Please confirm your intention to proceed." then
						if Configs.Tabs.SelectedInstance and Configs.Tabs.SelectedInstance.details.display_name.Text:find("Not Attached") then
							attachInstance(Configs.Tabs.SelectedInstance)
						end
					elseif main_frame.confirmation_prompt.prompt.description.Text == "You are about to be disconnected from this client session. Please confirm if you wish to proceed." then
						if Configs.Tabs.SelectedInstance then
							detachInstance(Configs.Tabs.SelectedInstance)
						end
					end
				end
			end
		end
	end
end)
UserInputService.InputBegan:Connect(function(input, gameProccesedEvent)
	if input.KeyCode == Enum.KeyCode.U then
		if UserInputService:GetFocusedTextBox() ~= nil then return end
		if promptedDisconnected then
			game:Shutdown()
		else
			module_Prompt(false)
		end
	end
end)
UserInputService.InputBegan:Connect(function(input, gameProccesedEvent)
	if input.KeyCode == Enum.KeyCode.Insert then
		if UserInputService:GetFocusedTextBox() ~= nil then return end
		if Configs.Functions.CustomLocalName then
			Configs.Functions.CustomLocalName=false
			local success, result = pcall(function()
				module_Files(false)
			end)

			if not success then
				module_announcement(string.format("An error occurred while running module_files", tostring(result)),10,"Error")
			end
		elseif not Configs.Functions.CustomLocalName then
			Configs.Functions.CustomLocalName=true
			local success, result = pcall(function()
				module_Files(false)
			end)

			if not success then
				module_announcement(string.format("An error occurred while running module_files", tostring(result)),10,"Error")
			end
		end
	end
end)




















































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































if not (isStudio) then
	while task.wait(1) do
		Essence_Instances.value5.Text = Configs.Functions.ActiveFunctions
		local disconnectedRobloxUI = coregui.RobloxPromptGui.promptOverlay:FindFirstChild("ErrorPrompt")

		if disconnectedRobloxUI and not promptedDisconnected then
			local reasonPrompt = disconnectedRobloxUI.MessageArea.ErrorFrame.ErrorMessage.Text
			promptedDisconnected = true

			local disconnectType
			local foundString

			for _, preDisconnectType in ipairs(Configs.disconnectTypes) do
				for _, typeString in pairs(preDisconnectType[2]) do
					if string.find(reasonPrompt, typeString) then
						disconnectType = preDisconnectType[1]
						foundString = true
						break
					end
				end
			end

			if not foundString then disconnectType = "kick" end
			coregui.RobloxPromptGui.promptOverlay.Visible = false
			module_panel(not main_frame.Visible)
			if disconnectType == "ban" then
				module_Prompt("You've been banned","would you like to leave this game?","Leave",true)
			elseif disconnectType == "kick" then
				module_Prompt("You've been kicked","would you like to rejoin?","Leave","Rejoin",true)
			elseif disconnectType == "network" then
				module_Prompt("You've lost connection","connection, would you like to rejoin?","Leave","Rejoin",true)
			end
			Essence_Instances.confirmation_prompt.Parent = coregui.RobloxPromptGui
			if not (isStudio) then
				writefile("EssenceSettings/Kicked.rblx", "")
			end
		end
	end
end


























































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































