	local recent = page.options.recent

	accessibility(false)

	module_panel(true)

	if recent.func:GetAttribute("credits") then
		module_Go_To("Credits")

	elseif recent.func:GetAttribute("dashboard") then
		module_Go_To("Dashboard")
	elseif recent.func:GetAttribute("profile") then
		local plr = player_service:FindFirstChild(recent.func.player_name.Value)

		if plr then
		else

			--// ACCESSIBILITY BUTTON RECENT

			page.options.recent.icon.Image = "rbxassetid://11419720347"
			page.options.recent.icon.corner.CornerRadius = UDim.new(0, 0)

			page.options.recent.func:SetAttribute("profile", false)
			page.options.recent.func:SetAttribute("dashboard", false)
			page.options.recent.func:SetAttribute("credits", true)
		end

		module_Go_To("Players")
	end
end)

accessibility_button.MouseEnter:Connect(function()
	on_frame = true

	if not input_service.TouchEnabled then
		accessibility(true)
	end
end)

accessibility_button.MouseLeave:Connect(function()
	on_frame = false

	if not input_service.TouchEnabled then
		accessibility(false)
	end
end)

input_service.InputBegan:Connect(function()
	if StorageOpened then 
		if not ToggleStandByQuickPanel then
			tween_service:Create(button, quick, {ImageTransparency = 1}):Play()
		else
			tween_service:Create(button, quick, {BackgroundTransparency = 1}):Play()
		end
		return 
	end
	if not on_frame then

		accessibility(false)

		if not ToggleStandByQuickPanel then
			tween_service:Create(button, quick, {ImageTransparency = .8}):Play()
		else
			tween_service:Create(button, quick, {BackgroundTransparency = .8}):Play()
		end
		tween_service:Create(button.scale, info, {Scale = .7}):Play()

	end
end)

