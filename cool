function hover0()
	for i, button in pairs(menu.menu.buttons:GetChildren()) do
		if button:IsA("ImageButton") then

			button.gradient.Rotation = 0
			if Configs.Executor.Animations then
				tween_service:Create(button.gradient, loop, {Rotation = 360}):Play()
			end


			button.MouseEnter:Connect(function()
				button.gradient.Enabled = true
				
				if not Configs.Executor.Animations then
					button.BackgroundTransparency = .8
					button.icon.scale.Scale = 1.4
				else
					tween_service:Create(button, info, {BackgroundTransparency = .8}):Play()
					tween_service:Create(button.icon.scale, info, {Scale = 1.4}):Play()
				end
			end)

			button.MouseButton1Down:Connect(function()
				if not Configs.Executor.Animations then
					button.BackgroundTransparency = .95
					button.icon.scale.Scale = .8

					menu.menu.scale.Scale = .95
				else
					tween_service:Create(button, info, {BackgroundTransparency = .95}):Play()
					tween_service:Create(button.icon.scale, info, {Scale = .8}):Play()

					tween_service:Create(menu.menu.scale, slow, {Scale = .95}):Play()
				end
			end)

			button.InputEnded:Connect(function()
				button.gradient.Enabled = false
				
				if not Configs.Executor.Animations then
					button.BackgroundTransparency = 1
					button.icon.scale.Scale = 1

					menu.menu.scale.Scale = 1
				else
					tween_service:Create(button, info, {BackgroundTransparency = 1}):Play()
					tween_service:Create(button.icon.scale, info, {Scale = 1}):Play()

					tween_service:Create(menu.menu.scale, slow, {Scale = 1}):Play()
				end
			end)
		end
	end
end

local success, result = pcall(function()
	hover0()
end)

if not success then
	module_announcement(string.format("An error occurred while running hover0", tostring(result)),10,"Error")
end

