	tween_service:Create(frame.confirmation_prompt.prompt.cancel, info, {BackgroundColor3 = Color3.fromRGB(58, 61, 62)}):Play()
end)

frame.confirmation_prompt.prompt.cancel.InputEnded:Connect(function()
	tween_service:Create(frame.confirmation_prompt.prompt.cancel, info, {BackgroundColor3 = Color3.fromRGB(85, 89, 91)}):Play()
end)

--dashboard

navi1.Parent.scroll.main_tools.features.ExecutionPanelButton.MouseButton1Click:Connect(function()
	module_Go_To("Execution")
end)

navi1.Parent.scroll.main_tools.features.ScriptHubButton.MouseButton1Click:Connect(function()
	module_Go_To("ScriptHub")
end)


navi1.Parent.scroll.main_tools.features.QuickPanelButton.MouseButton1Click:Connect(function()
	module_Go_To("QuickPanel")
end)

navi1.Parent.scroll.additional_features.features.SettingsButton.MouseButton1Click:Connect(function()
	module_Go_To("Settings")
end)


main_frame.confirmation_prompt.prompt.cancel.MouseButton1Click:Connect(function()
	tween_service:Create(main_frame.confirmation_prompt.prompt.scale, info, {Scale = 0}):Play()
	main_frame.confirmation_prompt.Visible = false
	if main_frame.confirmation_prompt.prompt.cancel.Text=="Leave (U)" then
		if main_frame.confirmation_prompt.prompt.header.Text=="You've been banned" or main_frame.confirmation_prompt.prompt.header.Text=="You've been kicked" or main_frame.confirmation_prompt.prompt.header.Text=="You've lost connection" then
			game:Shutdown()
		end
	end
	if main_frame.confirmation_prompt.prompt.cancel.Text=="No (U)" then
		if main_frame.confirmation_prompt.prompt.header.Text=="Confirmation Required" and main_frame.confirmation_prompt.prompt.description.Text=="You are about to be connected to this client session. Please confirm your intention to proceed." then
		end
	end
	if main_frame.confirmation_prompt.prompt.cancel.Text=="No (U)" then
		if main_frame.confirmation_prompt.prompt.header.Text=="Confirmation Required" and main_frame.confirmation_prompt.prompt.description.Text=="You are about to be disconnected from this client session. Please confirm if you wish to proceed." then
		end
	end
	module_Prompt(false)
end) 

for _, playerUI in pairs(navi2.Parent.scroll:GetChildren()) do
	if playerUI:IsA("ImageButton") and playerUI:FindFirstChild("quick_actions") then
		if playerUI.Name=="player" then return end
		playerUI.quick_actions.kick.MouseButton1Click:Connect(function()
			Configs.Tabs.SelectedInstance = playerUI
			module_Prompt("Confirmation Required", "You are about to be connected to this client session. Please confirm your intention to proceed.", "No", "Yes", true)
		end)

		playerUI.quick_actions.ban.MouseButton1Click:Connect(function()
			Configs.Tabs.SelectedInstance = playerUI
			module_Prompt("Confirmation Required", "You are about to be disconnected from this client session. Please confirm if you wish to proceed.", "No", "Yes", true)
		end)
	end
end

-- Confirmation prompt
main_frame.confirmation_prompt.prompt.confirm.MouseButton1Click:Connect(function()
	tween_service:Create(main_frame.confirmation_prompt.prompt.scale, info, {Scale = 0}):Play()
	main_frame.confirmation_prompt.Visible = false

	if main_frame.confirmation_prompt.prompt.confirm.Text=="Rejoin (Y)" then
		if main_frame.confirmation_prompt.prompt.header.Text=="You've been banned" or main_frame.confirmation_prompt.prompt.header.Text=="You've been kicked" or main_frame.confirmation_prompt.prompt.header.Text=="You've lost connection" then
			if #player_service:GetPlayers() <= 1 then
				task.wait()
				teleportService:Teleport(game.PlaceId, local_player)
			else
				teleportService:TeleportToPlaceInstance(game.PlaceId, game.jobId, local_player)
			end
		end
	end

	if main_frame.confirmation_prompt.prompt.confirm.Text == "Yes (Y)" then
		if main_frame.confirmation_prompt.prompt.header.Text == "Confirmation Required" then
			if main_frame.confirmation_prompt.prompt.description.Text == "You are about to be connected to this client session. Please confirm your intention to proceed." then
				module_announcement("There was a error attaching to client this caused by executor unsupported",15,"Error")
				if Configs.Tabs.SelectedInstance:FindFirstChild("details") then attachInstance(Configs.Tabs.SelectedInstance) return end
				if Configs.Tabs.SelectedInstance and Configs.Tabs.SelectedInstance.details.display_name.Text:find("Not Attached") then
					attachInstance(Configs.Tabs.SelectedInstance)
				end
			elseif main_frame.confirmation_prompt.prompt.description.Text == "You are about to be disconnected from this client session. Please confirm if you wish to proceed." then
				module_announcement("There was a error deattaching to client this caused by executor unsupported",15,"Error")
				if Configs.Tabs.SelectedInstance then
					detachInstance(Configs.Tabs.SelectedInstance)
				end
			end
		end
	end
	module_Prompt(false)
end)


navi3.Parent.Buttons.ClearButton.MouseButton1Click:Connect(function()
	if navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox0.Visible then
		navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox0.Text = ""
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox1.Visible then
		navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox1.Text = ""
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox2.Visible then
		navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox2.Text = ""
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox3.Visible then
		navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox3.Text = ""
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox4.Visible then
		navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox4.Text = ""	
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox5.Visible then
		navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox5.Text = ""	
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox6.Visible then
		navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox6.Text = ""
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox7.Visible then
		navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox7.Text = ""	
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox8.Visible then
		navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox8.Text = ""
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox9.Visible then
		navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox9.Text = ""	
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox10.Visible then
		navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox10.Text = ""
	end
end)

navi3.Parent.Buttons.ExecuteButton.MouseButton1Click:Connect(function()
	if navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox0.Visible then
		module_execute(navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox0,navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox0.Text)
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox1.Visible then
		module_execute(navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox1,navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox1.Text)	
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox2.Visible then
		module_execute(navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox1,navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox2.Text)	
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox3.Visible then
		module_execute(navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox1,navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox3.Text)	
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox4.Visible then
		module_execute(navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox1,navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox4.Text)	
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox5.Visible then
		module_execute(navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox1,navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox5.Text)	
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox6.Visible then
		module_execute(navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox1,navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox6.Text)	
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox7.Visible then
		module_execute(navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox1,navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox7.Text)	
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox8.Visible then
		module_execute(navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox1,navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox8.Text)	
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox9.Visible then
		module_execute(navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox1,navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox9.Text)	
	elseif navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox10.Visible then
		module_execute(navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox1,navi3.Parent.scroll.build.ScrollingExecution.ExecutionTextBox10.Text)	
	end
end)


navi3.Parent.Buttons.AttachButton.MouseButton1Click:Connect(function()
	if Attaching and not ClientAttached2 then
		module_announcement("Already Attaching",20,"Normal")
	end
	if not ClientAttached2 and Attached then
		module_announcement("Client not Attached",20,"Normal")
	end
	if Attached and ClientAttached2 then
		module_announcement("Already Attached",20,"Normal")
	elseif not Attached and not ClientAttached2 then
		Attaching = true
		module_announcement("Attaching",1,"Normal")
		if navi2.Parent.scroll:FindFirstChildOfClass("ImageButton") then
			navi2.Parent.scroll:FindFirstChildOfClass("ImageButton").details.display_name.Text = local_player.DisplayName .. " (Attaching)"
		end
		task.wait(2)
		module_announcement("Attached",2,"Normal")
		if navi2.Parent.scroll:FindFirstChildOfClass("ImageButton") then
			navi2.Parent.scroll:FindFirstChildOfClass("ImageButton").details.display_name.Text = local_player.DisplayName .. " (Attached)"
			navi2.Parent.scroll[local_player.Name].quick_actions.kick.Visible = false
			navi2.Parent.scroll[local_player.Name].quick_actions.ban.Visible = true
		end
		Attaching = false
		ClientAttached2=true
		Attached=true
	end
end)


navi3.menu.MouseButton1Click:Connect(function()
	module_menu(true)
end)

navi3.close.MouseButton1Click:Connect(function()
	module_Go_To("Dashboard")
end)

navi1.menu.MouseButton1Click:Connect(function()
	module_menu(true)
end)

navi1.close.MouseButton1Click:Connect(function()
	module_panel(false)
end)

function hover2()
	for i, button in pairs(navi1.Parent.scroll:GetDescendants()) do
		if button:IsA("ImageButton") then

			local folder = button:FindFirstChild("quick_access")

			button.MouseEnter:Connect(function()
				if allowed then
					if not Configs.Executor.Animations then
					    button.icon.Size = UDim2.new(0, 35,0, 35)
						if Storagefullscreen then
							button.label.TextSize = 15
						end
						button.stroke.Transparency = 0.65
						button.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
					else
						tween_service:Create(button.icon, info, {Size = UDim2.new(0, 35,0, 35)}):Play()
						if Storagefullscreen then
							tween_service:Create(button.label, quick, {TextSize = 15}):Play()
						end
						tween_service:Create(button.stroke, quick, {Transparency = 0.65}):Play()
						tween_service:Create(button, info, {BackgroundColor3 = Color3.fromRGB(40, 40, 40)}):Play()
					end
				end
			end)

			button.MouseButton1Down:Connect(function()
				tween_service:Create(button, info, {BackgroundColor3 = Color3.fromRGB(17, 17, 17)}):Play()
			end)

			button.InputEnded:Connect(function()
				if not Configs.Executor.Animations then
					button.icon.Size = UDim2.new(0, 30,0, 30)
					if Storagefullscreen then
						button.label.TextSize = 14
					end
					button.stroke.Transparency = 1
					button.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
				else
					tween_service:Create(button.icon, info, {Size = UDim2.new(0, 30,0, 30)}):Play()
					if Storagefullscreen then
						tween_service:Create(button.label, quick, {TextSize = 14}):Play()
					end
					tween_service:Create(button.stroke, quick, {Transparency = 1}):Play()
					tween_service:Create(button, info, {BackgroundColor3 = Color3.fromRGB(0, 0, 0)}):Play()
				end
			end)

			if folder then
				folder.global.MouseEnter:Connect(function()
					if not Configs.Executor.Animations then
						button.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
						folder.global.BackgroundTransparency = .5
						folder.global.BackgroundColor3 = Color3.fromRGB(102, 102, 102)
					else
						tween_service:Create(button, info, {BackgroundColor3 = Color3.fromRGB(0, 0, 0)}):Play()
						tween_service:Create(folder.global, info, {BackgroundTransparency = .5, 
						BackgroundColor3 = Color3.fromRGB(102, 102, 102)}):Play()
					end
				end)

				folder.global.MouseLeave:Connect(function()
					if not Configs.Executor.Animations then
						button.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
						folder.global.BackgroundTransparency = .9
						folder.global.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
					else
						tween_service:Create(button, info, {BackgroundColor3 = Color3.fromRGB(40, 40, 40)}):Play()
						tween_service:Create(folder.global, info, {BackgroundTransparency = .9, 
							BackgroundColor3 = Color3.fromRGB(0, 0, 0)}):Play()
					end
				end)
			end
		end
	end
end

function hover_navi1()
	for i, button in pairs(navi1:GetChildren()) do
		if button:IsA("ImageButton") then

			button.gradient.Rotation = 0

			if Configs.Executor.Animations then
				tween_service:Create(button.gradient, loop, {Rotation = 360}):Play()
			end

			button.MouseEnter:Connect(function()
				if allowed then
					button.gradient.Enabled = true

					if not Configs.Executor.Animations then
						button.icon.scale.Scale = 1.2
					else
						tween_service:Create(button.icon.scale, info, {Scale = 1.2}):Play()
					end
				end
			end)

			button.MouseButton1Down:Connect(function()
				if not Configs.Executor.Animations then
					button.icon.scale.Scale = .8
				else
					tween_service:Create(button.icon.scale, info, {Scale = .8}):Play()
				end
			end)

			button.InputEnded:Connect(function()
				button.gradient.Enabled = false

				if not Configs.Executor.Animations then
					button.icon.scale.Scale = 1
				else
					tween_service:Create(button.icon.scale, info, {Scale = 1}):Play()
				end
			end)
		end
	end
end

local success, result = pcall(function()
	hover2()
end)

if not success then
	module_announcement(string.format("An error occurred while running hover2", tostring(result)),10,"Error")
end
local success, result = pcall(function()
	hover_navi1()
end)

if not success then
	module_announcement(string.format("An error occurred while running hover_navi1", tostring(result)),10,"Error")
end

function hover3()
	for i, button in pairs(navi3.Parent.scroll:GetDescendants()) do
		if button:IsA("ImageButton") then

			button.MouseEnter:Connect(function()

				if not Configs.Executor.Animations then
					button.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
				else
					tween_service:Create(button, info, {BackgroundColor3 = Color3.fromRGB(40, 40, 40)}):Play()
				end
			end)

			button.MouseButton1Down:Connect(function()
				if not Configs.Executor.Animations then
					button.BackgroundColor3 = Color3.fromRGB(17, 17, 17)
				else
					tween_service:Create(button, info, {BackgroundColor3 = Color3.fromRGB(17, 17, 17)}):Play()
				end
			end)

			button.InputEnded:Connect(function()
				if not Configs.Executor.Animations then
					button.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
				else
					tween_service:Create(button, info, {BackgroundColor3 = Color3.fromRGB(0, 0, 0)}):Play()
				end
			end)
		end
	end
end

function hover_navi2()
	for i, button in pairs(navi3:GetChildren()) do
		if button:IsA("ImageButton") then

			if Configs.Executor.Animations then
				tween_service:Create(button.gradient, loop, {Rotation = 360}):Play()
			end

			button.MouseEnter:Connect(function()
				if allowed then
					button.gradient.Enabled = true

					if not Configs.Executor.Animations then
						button.icon.scale.Scale = 1.2
					else
						tween_service:Create(button.icon.scale, info, {Scale = 1.2}):Play()
					end
				end
			end)

			button.MouseButton1Down:Connect(function()
				if not Configs.Executor.Animations then
					button.icon.scale.Scale = .8
				else
					tween_service:Create(button.icon.scale, info, {Scale = .8}):Play()
				end
			end)

			button.InputEnded:Connect(function()
				button.gradient.Enabled = false

				if not Configs.Executor.Animations then
					button.icon.scale.Scale = 1
				else
					tween_service:Create(button.icon.scale, info, {Scale = 1}):Play()
				end
			end)
		end
	end
end

